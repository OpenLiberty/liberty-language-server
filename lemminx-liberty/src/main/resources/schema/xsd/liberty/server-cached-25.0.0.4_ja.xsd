    <xsd:complexType name="com.ibm.ws.jbatch.jms.events-factory">
        <xsd:complexContent>
            <xsd:extension base="com.ibm.ws.jbatch.jms.events">
                <xsd:attribute name="id" type="factoryIdType" use="optional">
