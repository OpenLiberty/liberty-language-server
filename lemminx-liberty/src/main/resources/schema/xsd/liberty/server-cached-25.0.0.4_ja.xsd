                                    <xsd:documentation>HOLD_CURSORS_OVER_COMMIT</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="2">
                                <xsd:annotation>
                                    <xsd:documentation>CLOSE_CURSORS_AT_COMMIT</xsd:documentation>
