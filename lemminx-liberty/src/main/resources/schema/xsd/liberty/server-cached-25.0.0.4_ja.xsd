                    <ext:label>データベース名</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="jndiName" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>JNDI 名。</xsd:documentation>
