        <xsd:sequence>
            <xsd:sequence/>
        </xsd:sequence>
        <xsd:attribute name="name" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>トークン名を指定してください。 オプションは Usernametoken、X509token、Samltoken です。</xsd:documentation>
