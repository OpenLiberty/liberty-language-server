                <xsd:appinfo>
                    <ext:label>コードベース</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="principalType" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>与えられるプリンシパル名と突き合わせるクラス名。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>プリンシパル・クラス</ext:label>
