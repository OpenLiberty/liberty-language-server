                    <ext:label>1 次サーバー照会時間間隔</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="true" name="returnToPrimaryServer" type="booleanType" use="optional">
