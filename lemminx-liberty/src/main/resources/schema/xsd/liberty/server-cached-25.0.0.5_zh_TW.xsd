                    <ext:label>物件類別名稱</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="libraryRef" type="pidType" use="optional">
            <xsd:annotation>
                <xsd:documentation>含有 Factory 實作類別的程式庫。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>共用程式庫參照</ext:label>
                    <ext:reference>library</ext:reference>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.jndi.objectFactory-factory">
        <xsd:complexContent>
            <xsd:extension base="com.ibm.ws.jndi.objectFactory">
                <xsd:attribute name="id" type="factoryIdType" use="optional">
                    <xsd:annotation>
                        <xsd:documentation>唯一的配置 ID。</xsd:documentation>
