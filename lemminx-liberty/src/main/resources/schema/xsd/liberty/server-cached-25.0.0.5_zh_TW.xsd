                <xsd:appinfo>
                    <ext:label>網站</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="client_secret_post" name="tokenEndpointAuthMethod" use="optional">
