    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.security.authentication.filter.webApp">
        <xsd:annotation>
            <xsd:documentation>指定 Web 應用程式名稱，例如 webApp id="sample "name="app1|app2"matchType="notContain"。</xsd:documentation>
            <xsd:appinfo>
                <ext:label>Web 應用程式</ext:label>
            </xsd:appinfo>
        </xsd:annotation>
        <!--Idiom for "empty element context" (whitespace accepted and ignored)-->
        <xsd:sequence>
            <xsd:sequence/>
        </xsd:sequence>
        <xsd:attribute name="name" type="xsd:string" use="required">
            <xsd:annotation>
                <xsd:documentation>指定名稱。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>名稱</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="contains" name="matchType" use="optional">
            <xsd:annotation>
                <xsd:documentation>指定相符類型。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>相符類型</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union memberTypes="variableType">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="equals">
                                <xsd:annotation>
                                    <xsd:documentation>等於</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="contains">
                                <xsd:annotation>
                                    <xsd:documentation>包含</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="notContain">
                                <xsd:annotation>
                                    <xsd:documentation>不包含</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.security.authentication.filter.webApp-factory">
        <xsd:complexContent>
            <xsd:extension base="com.ibm.ws.security.authentication.filter.webApp">
                <xsd:attribute name="id" type="factoryIdType" use="optional">
                    <xsd:annotation>
                        <xsd:documentation>唯一的配置 ID。</xsd:documentation>
                        <xsd:appinfo>
                            <ext:label>ID</ext:label>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.security.authentication.filter.host">
        <xsd:annotation>
            <xsd:documentation>指定主機名稱，例如 host id="sample "name="host.mycomp.com" matchType = "contains"。</xsd:documentation>
            <xsd:appinfo>
                <ext:label>主機</ext:label>
            </xsd:appinfo>
        </xsd:annotation>
        <!--Idiom for "empty element context" (whitespace accepted and ignored)-->
        <xsd:sequence>
            <xsd:sequence/>
        </xsd:sequence>
        <xsd:attribute name="name" type="xsd:string" use="required">
            <xsd:annotation>
                <xsd:documentation>指定名稱。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>名稱</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="contains" name="matchType" use="optional">
            <xsd:annotation>
                <xsd:documentation>指定相符類型。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>相符類型</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union memberTypes="variableType">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="equals">
                                <xsd:annotation>
                                    <xsd:documentation>等於</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="contains">
                                <xsd:annotation>
                                    <xsd:documentation>包含</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="notContain">
                                <xsd:annotation>
                                    <xsd:documentation>不包含</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.security.authentication.filter.host-factory">
        <xsd:complexContent>
            <xsd:extension base="com.ibm.ws.security.authentication.filter.host">
                <xsd:attribute name="id" type="factoryIdType" use="optional">
                    <xsd:annotation>
                        <xsd:documentation>唯一的配置 ID。</xsd:documentation>
                        <xsd:appinfo>
                            <ext:label>ID</ext:label>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.security.authentication.filter">
        <xsd:annotation>
            <xsd:documentation>指定選取規則，以代表要與 HTTP 要求標頭比對的條件。 如果所有條件都符合，則選取 HTTP 要求進行鑑別。 當 matchType 屬性值為 notContain 或 contains 時，過濾器支援管道式的值清單。</xsd:documentation>
            <xsd:appinfo>
                <ext:label>鑑別過濾器</ext:label>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:choice maxOccurs="unbounded" minOccurs="0">
            <xsd:element name="webApp" type="com.ibm.ws.security.authentication.filter.webApp-factory"/>
            <xsd:element name="requestUrl" type="com.ibm.ws.security.authentication.filter.requestUrl-factory"/>
            <xsd:element name="remoteAddress" type="com.ibm.ws.security.authentication.filter.remoteAddress-factory"/>
            <xsd:element name="host" type="com.ibm.ws.security.authentication.filter.host-factory"/>
            <xsd:element name="userAgent" type="com.ibm.ws.security.authentication.filter.userAgent-factory"/>
            <xsd:element name="requestHeader" type="com.ibm.ws.security.authentication.filter.requestHeader-factory"/>
            <xsd:element name="cookie" type="com.ibm.ws.security.authentication.filter.cookie-factory"/>
        </xsd:choice>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.security.authentication.filter-factory">
        <xsd:complexContent>
            <xsd:extension base="com.ibm.ws.security.authentication.filter">
                <xsd:attribute name="id" type="factoryIdType" use="optional">
                    <xsd:annotation>
                        <xsd:documentation>唯一的配置 ID。</xsd:documentation>
                        <xsd:appinfo>
                            <ext:label>ID</ext:label>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.app.management">
        <xsd:annotation>
            <xsd:documentation>用來控制應用程式管理程式行為的內容</xsd:documentation>
            <xsd:appinfo>
                <ext:label>應用程式管理程式</ext:label>
            </xsd:appinfo>
        </xsd:annotation>
        <!--Idiom for "empty element context" (whitespace accepted and ignored)-->
        <xsd:sequence>
            <xsd:sequence/>
        </xsd:sequence>
        <xsd:attribute default="false" name="autoExpand" type="booleanType" use="optional">
            <xsd:annotation>
                <xsd:documentation>啟用自動解壓縮 WAR 檔和 EAR 檔</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>自動展開應用程式</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="useJandex" type="booleanType" use="optional">
            <xsd:annotation>
                <xsd:documentation>如果應用程式中提供 Jandex 索引檔，則會啟用它們。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>使用 Jandex</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="30s" name="startTimeout" type="secondDuration" use="optional">
            <xsd:annotation>
                <xsd:documentation>指定伺服器等待應用程式啟動多久之後，便將它視為慢速。. 指定正整數加上時間單位，時間單位可以是小時 (h)、分鐘 (m) 或秒 (s)。例如，指定 30 秒為 30s。您可以在單一項目中包括多個值。例如，1m30s 相等於 90 秒。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>啟動逾時值</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="30s" name="stopTimeout" type="secondDuration" use="optional">
            <xsd:annotation>
                <xsd:documentation>指定伺服器等待應用程式停止多久之後，便將它視為慢速。. 指定正整數加上時間單位，時間單位可以是小時 (h)、分鐘 (m) 或秒 (s)。例如，指定 30 秒為 30s。您可以在單一項目中包括多個值。例如，1m30s 相等於 90 秒。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>停止逾時</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="${server.config.dir}/apps/expanded/" name="expandLocation" type="dirLocation" use="optional">
            <xsd:annotation>
                <xsd:documentation>expandLocation 屬性讓使用者能夠設定 autoExpanded 應用程式所在的位置。 預設位置為 ${server.config.dir}/apps/expanded/。 多台伺服器無法共用相同的 expandLocation 位置。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>擴充位置</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.app.manager.monitor">
        <xsd:annotation>
            <xsd:documentation>定義伺服器回應應用程式新增項目、更新項目和刪除項目的方式。</xsd:documentation>
            <xsd:appinfo>
                <ext:label>應用程式監視</ext:label>
            </xsd:appinfo>
        </xsd:annotation>
        <!--Idiom for "empty element context" (whitespace accepted and ignored)-->
        <xsd:sequence>
            <xsd:sequence/>
        </xsd:sequence>
        <xsd:attribute default="500ms" name="pollingRate" type="duration" use="optional">
            <xsd:annotation>
                <xsd:documentation>伺服器檢查應用程式新增項目、更新項目和刪除項目的頻率。. 指定正整數加上時間單位，時間單位可以是小時 (h)、分鐘 (m)、秒 (s) 或毫秒 (ms)。例如，指定 500 毫秒為 500ms。您可以在單一項目中包括多個值。例如，1s500ms 相等於 1.5 秒。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>應用程式更新輪詢率</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="dropins" name="dropins" type="dirLocation" use="optional">
            <xsd:annotation>
                <xsd:documentation>應用程式放置區目錄的位置，以伺服器目錄的絕對路徑或相對路徑表示。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>應用程式放置區目錄</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="true" name="dropinsEnabled" type="booleanType" use="optional">
            <xsd:annotation>
                <xsd:documentation>監視放置區目錄中是否有應用程式新增項目、更新項目和刪除項目。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>監視應用程式放置區目錄</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="polled" name="updateTrigger" use="optional">
            <xsd:annotation>
                <xsd:documentation>應用程式更新方法或觸發程式。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>應用程式更新觸發程式</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union memberTypes="variableType">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="polled">
                                <xsd:annotation>
                                    <xsd:documentation>伺服器會在輪詢間隔掃描應用程式變更，並更新任何具有可偵測變更的應用程式。</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="mbean">
                                <xsd:annotation>
                                    <xsd:documentation>只有在被外部程式（如整合開發環境或管理應用程式）所呼叫的 MBean 提示時，伺服器才會更新應用程式。</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="disabled">
                                <xsd:annotation>
                                    <xsd:documentation>停用所有更新監視。伺服器正在執行時，將不會套用應用程式變更。</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.app.manager">
        <xsd:annotation>
            <xsd:documentation>定義應用程式的內容。</xsd:documentation>
            <xsd:appinfo>
                <ext:label>應用程式</ext:label>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:choice maxOccurs="unbounded" minOccurs="0">
            <xsd:element name="startAfter" type="com.ibm.ws.app.manager-factory">
                <xsd:annotation>
                    <xsd:documentation>指定在此應用程式可以開始啟動之前，需要先啟動的應用程式。</xsd:documentation>
