                <xsd:documentation>使用者的密碼，通常在連接至主機時會需要。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>使用者的密碼</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="from" type="xsd:string" use="required">
            <xsd:annotation>
                <xsd:documentation>用來透過「郵件階段作業」實例傳送郵件的電子郵件位址。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>寄件位址</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
