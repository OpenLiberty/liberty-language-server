            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="defaultValue" type="xsd:string">
            <xsd:annotation>
                <xsd:documentation>要指派給變數的 defaultValue（如果沒有可用的值）。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>預設值</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
