                <xsd:appinfo>
                    <ext:label>用戶端 ID</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="defaultME" name="durableSubscriptionHome" type="xsd:string" use="optional">
