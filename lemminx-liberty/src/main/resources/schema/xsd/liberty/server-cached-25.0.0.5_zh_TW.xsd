                                    <xsd:documentation>等於</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="contains">
                                <xsd:annotation>
                                    <xsd:documentation>包含</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="notContain">
                                <xsd:annotation>
                                    <xsd:documentation>不包含</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.security.authentication.filter.cookie-factory">
        <xsd:complexContent>
            <xsd:extension base="com.ibm.ws.security.authentication.filter.cookie">
                <xsd:attribute name="id" type="factoryIdType" use="optional">
