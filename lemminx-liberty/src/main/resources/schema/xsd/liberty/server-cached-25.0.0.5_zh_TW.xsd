                <xsd:documentation>JDBC 驅動程式內容：packetSize。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>封包大小</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
