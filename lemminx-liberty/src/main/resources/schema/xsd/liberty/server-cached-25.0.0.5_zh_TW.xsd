    <xsd:complexType name="com.ibm.ws.wssecurity.caller.config">
        <xsd:annotation>
            <xsd:documentation>供呼叫端處理的必要配置。</xsd:documentation>
            <xsd:appinfo>
                <ext:label>WebSphere 呼叫端</ext:label>
            </xsd:appinfo>
        </xsd:annotation>
        <!--Idiom for "empty element context" (whitespace accepted and ignored)-->
        <xsd:sequence>
            <xsd:sequence/>
        </xsd:sequence>
        <xsd:attribute name="name" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>指定記號名稱。選項有 Usernametoken、X509token、Samltoken。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>呼叫端記號</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="userIdentifier" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>指定 SAML 屬性，以作為主體中的使用者主體名稱。預設值是 NameID 主張。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>主體 ID</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="groupIdentifier" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>指定 SAML 屬性，以作為已鑑別主體所屬的群組名稱。沒有預設值。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>群組 ID</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="userUniqueIdentifier" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>指定 SAML 屬性，以作為套用至主體中之 WSCredential 時的唯一使用者名稱。預設值與 userIdentifier 屬性值相同。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>唯一 ID</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="realmIdentifier" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>指定將作為領域名稱的 SAML 屬性。預設值為 issuer。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>領域 ID</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="true" name="includeTokenInSubject" type="booleanType" use="optional">
            <xsd:annotation>
                <xsd:documentation>指定是否將 SAML 主張包含在主體中。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>將 SAML 主張包含在主體中</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="No" name="mapToUserRegistry" use="optional">
            <xsd:annotation>
                <xsd:documentation>指定如何將身分對映至登錄使用者。選項是 No、User 和 Group。預設值是 No，表示不使用使用者登錄來建立使用者主體。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>將 SAML 身分對映至登錄使用者</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union memberTypes="variableType">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="No">
                                <xsd:annotation>
                                    <xsd:documentation>不將 SAML 身分對映至登錄中的使用者或群組</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="User">
                                <xsd:annotation>
                                    <xsd:documentation>將 SAML 身分對映至登錄中所定義的使用者</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="Group">
                                <xsd:annotation>
                                    <xsd:documentation>將 SAML 身分對映至使用者登錄中所定義的群組</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute name="realmName" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>指定當 mapToUserRegistry 設為 No 或 Group 時的網域範圍名稱。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>領域名稱</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="true" name="allowCustomCacheKey" type="booleanType" use="optional">
            <xsd:annotation>
                <xsd:documentation>容許產生自訂快取金鑰，以存取鑑別快取並取得主體。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>自訂的快取金鑰</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
