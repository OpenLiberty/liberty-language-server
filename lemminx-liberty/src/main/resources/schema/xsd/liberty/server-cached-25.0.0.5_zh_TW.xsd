<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:ext="http://www.ibm.com/xmlns/dde/schema/annotation/ext">
    <xsd:complexType name="com.ibm.ws.app.manager.webappcfg">
        <xsd:annotation>
            <xsd:documentation>定義 Web 應用程式的內容。</xsd:documentation>
            <xsd:appinfo>
                <ext:label>Web 應用程式</ext:label>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:choice maxOccurs="unbounded" minOccurs="0">
            <xsd:element name="startAfter" type="com.ibm.ws.app.manager-factory">
                <xsd:annotation>
                    <xsd:documentation>指定在此應用程式可以開始啟動之前，需要先啟動的應用程式。</xsd:documentation>
                    <xsd:appinfo>
                        <ext:label>之後啟動</ext:label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="classloader" type="com.ibm.ws.classloading.classloader">
                <xsd:annotation>
                    <xsd:documentation>定義應用程式類別載入器的設定。</xsd:documentation>
                    <xsd:appinfo>
                        <ext:label>類別載入器</ext:label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="web-bnd" type="com.ibm.ws.javaee.dd.webbnd.WebBnd"/>
            <xsd:element name="webservices-bnd" type="com.ibm.ws.javaee.ddmodel.wsbnd.WebservicesBnd"/>
            <xsd:element name="web-ext" type="com.ibm.ws.javaee.dd.webext.WebExt"/>
            <xsd:element name="application-ext" type="com.ibm.ws.javaee.dd.appext.ApplicationExt"/>
            <xsd:element name="application-bnd" type="com.ibm.ws.javaee.dd.appbnd.ApplicationBnd"/>
            <xsd:element name="ejb-jar-bnd" type="com.ibm.ws.javaee.dd.ejbbnd.EJBJarBnd"/>
            <xsd:element name="appProperties" type="com.ibm.ws.appconfig.appProperties"/>
            <xsd:element name="ejb-jar-ext" type="com.ibm.ws.javaee.dd.ejbext.EJBJarExt"/>
            <xsd:element name="managed-bean-bnd" type="com.ibm.ws.javaee.dd.managedbean.ManagedBeanBnd"/>
            <xsd:element name="application-client-bnd" type="com.ibm.ws.javaee.dd.clientbnd.ApplicationClientBnd"/>
        </xsd:choice>
        <xsd:attribute name="location" type="location" use="required">
            <xsd:annotation>
                <xsd:documentation>應用程式的位置，以伺服器層次應用程式目錄的絕對路徑或相對路徑表示。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>位置</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="name" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>應用程式的名稱。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>名稱</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="true" name="autoStart" type="booleanType" use="optional">
            <xsd:annotation>
                <xsd:documentation>指出伺服器是否會自動啟動應用程式。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>自動啟動</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="useJandex" type="booleanType" use="optional">
            <xsd:annotation>
                <xsd:documentation>如果應用程式中提供 Jandex 索引檔，則會啟用它們。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>使用 Jandex</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="startAfterRef" type="pidListType" use="optional">
            <xsd:annotation>
                <xsd:documentation>指定在此應用程式可以開始啟動之前，需要先啟動的應用程式。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>開始延遲</ext:label>
                    <ext:reference>application</ext:reference>
                    <ext:reference>webApplication</ext:reference>
                    <ext:reference>enterpriseApplication</ext:reference>
                    <ext:reference>ejbApplication</ext:reference>
                    <ext:reference>resourceAdapter</ext:reference>
                    <ext:reference>springBootApplication</ext:reference>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="classloaderRef" type="pidType" use="optional">
            <xsd:annotation>
                <xsd:documentation>定義應用程式類別載入器的設定。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>類別載入器</ext:label>
                    <ext:reference>classloader</ext:reference>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="contextRoot" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>應用程式的環境定義根目錄。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>應用程式環境定義根目錄</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.app.manager.webappcfg-factory">
        <xsd:complexContent>
            <xsd:extension base="com.ibm.ws.app.manager.webappcfg">
                <xsd:attribute name="id" type="factoryIdType" use="optional">
                    <xsd:annotation>
                        <xsd:documentation>唯一的配置 ID。</xsd:documentation>
                        <xsd:appinfo>
                            <ext:label>ID</ext:label>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.app.manager.earappcfg">
        <xsd:annotation>
            <xsd:documentation>定義企業應用程式的內容。</xsd:documentation>
            <xsd:appinfo>
                <ext:label>企業應用程式</ext:label>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:choice maxOccurs="unbounded" minOccurs="0">
            <xsd:element name="startAfter" type="com.ibm.ws.app.manager-factory">
                <xsd:annotation>
                    <xsd:documentation>指定在此應用程式可以開始啟動之前，需要先啟動的應用程式。</xsd:documentation>
                    <xsd:appinfo>
                        <ext:label>之後啟動</ext:label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="classloader" type="com.ibm.ws.classloading.classloader">
                <xsd:annotation>
                    <xsd:documentation>定義應用程式類別載入器的設定。</xsd:documentation>
                    <xsd:appinfo>
                        <ext:label>類別載入器</ext:label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="web-bnd" type="com.ibm.ws.javaee.dd.webbnd.WebBnd"/>
            <xsd:element name="webservices-bnd" type="com.ibm.ws.javaee.ddmodel.wsbnd.WebservicesBnd"/>
            <xsd:element name="web-ext" type="com.ibm.ws.javaee.dd.webext.WebExt"/>
            <xsd:element name="application-ext" type="com.ibm.ws.javaee.dd.appext.ApplicationExt"/>
            <xsd:element name="application-bnd" type="com.ibm.ws.javaee.dd.appbnd.ApplicationBnd"/>
            <xsd:element name="ejb-jar-bnd" type="com.ibm.ws.javaee.dd.ejbbnd.EJBJarBnd"/>
            <xsd:element name="appProperties" type="com.ibm.ws.appconfig.appProperties"/>
            <xsd:element name="resourceAdapter" type="com.ibm.ws.jca.embeddedResourceAdapter"/>
            <xsd:element name="ejb-jar-ext" type="com.ibm.ws.javaee.dd.ejbext.EJBJarExt"/>
            <xsd:element name="managed-bean-bnd" type="com.ibm.ws.javaee.dd.managedbean.ManagedBeanBnd"/>
            <xsd:element name="application-client-bnd" type="com.ibm.ws.javaee.dd.clientbnd.ApplicationClientBnd"/>
        </xsd:choice>
        <xsd:attribute name="location" type="location" use="required">
            <xsd:annotation>
                <xsd:documentation>應用程式的位置，以伺服器層次應用程式目錄的絕對路徑或相對路徑表示。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>位置</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="name" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>應用程式的名稱。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>名稱</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="true" name="autoStart" type="booleanType" use="optional">
            <xsd:annotation>
                <xsd:documentation>指出伺服器是否會自動啟動應用程式。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>自動啟動</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="useJandex" type="booleanType" use="optional">
            <xsd:annotation>
                <xsd:documentation>如果應用程式中提供 Jandex 索引檔，則會啟用它們。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>使用 Jandex</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="startAfterRef" type="pidListType" use="optional">
            <xsd:annotation>
                <xsd:documentation>指定在此應用程式可以開始啟動之前，需要先啟動的應用程式。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>開始延遲</ext:label>
                    <ext:reference>application</ext:reference>
                    <ext:reference>webApplication</ext:reference>
                    <ext:reference>enterpriseApplication</ext:reference>
                    <ext:reference>ejbApplication</ext:reference>
                    <ext:reference>resourceAdapter</ext:reference>
                    <ext:reference>springBootApplication</ext:reference>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="classloaderRef" type="pidType" use="optional">
            <xsd:annotation>
                <xsd:documentation>定義應用程式類別載入器的設定。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>類別載入器</ext:label>
                    <ext:reference>classloader</ext:reference>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="defaultClientModule" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>企業應用程式的預設用戶端模組。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>預設用戶端模組</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="webModuleClassPathLoader" use="optional">
            <xsd:annotation>
                <xsd:documentation>此屬性定義用於 Web 應用程式類別路徑標頭引用的 JAR 的類別載入器。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Web 模組類路徑載入器</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union memberTypes="variableType">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="war">
                                <xsd:annotation>
                                    <xsd:documentation>對 Web 應用程式類別路徑標頭引用的 JAR 使用 Web 應用程式類別載入器。</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="ear">
                                <xsd:annotation>
                                    <xsd:documentation>對 Web 應用程式類別路徑標頭引用的 JAR 使用父企業應用程式類別載入器。</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute name="annotationScanLibrary" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>%earapp.annotationScanLibrary.desc</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>%earapp.annotationScanLibrary.name</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.app.manager.earappcfg-factory">
        <xsd:complexContent>
            <xsd:extension base="com.ibm.ws.app.manager.earappcfg">
                <xsd:attribute name="id" type="factoryIdType" use="optional">
                    <xsd:annotation>
                        <xsd:documentation>唯一的配置 ID。</xsd:documentation>
                        <xsd:appinfo>
                            <ext:label>ID</ext:label>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.security.authentication.filter.remoteAddress">
        <xsd:annotation>
            <xsd:documentation>指定傳送 HTTP 要求之用戶端應用程式的遠端主機 TCP/IP 位址，例如 remoteAddress id="sample "IP=" 100.100.100.99" matchType="greaterThan "。</xsd:documentation>
            <xsd:appinfo>
                <ext:label>遠端位址</ext:label>
            </xsd:appinfo>
        </xsd:annotation>
        <!--Idiom for "empty element context" (whitespace accepted and ignored)-->
        <xsd:sequence>
            <xsd:sequence/>
        </xsd:sequence>
        <xsd:attribute default="contains" name="matchType" use="optional">
            <xsd:annotation>
                <xsd:documentation>指定相符類型。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>相符類型</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union memberTypes="variableType">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="equals">
                                <xsd:annotation>
                                    <xsd:documentation>等於</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="contains">
                                <xsd:annotation>
                                    <xsd:documentation>包含</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="notContain">
                                <xsd:annotation>
                                    <xsd:documentation>不包含</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="greaterThan">
                                <xsd:annotation>
                                    <xsd:documentation>大於</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="lessThan">
                                <xsd:annotation>
                                    <xsd:documentation>小於</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute name="ip" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>指定遠端主機 TCP/IP 位址。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>IP 位址</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.security.authentication.filter.remoteAddress-factory">
        <xsd:complexContent>
            <xsd:extension base="com.ibm.ws.security.authentication.filter.remoteAddress">
                <xsd:attribute name="id" type="factoryIdType" use="optional">
                    <xsd:annotation>
                        <xsd:documentation>唯一的配置 ID。</xsd:documentation>
                        <xsd:appinfo>
                            <ext:label>ID</ext:label>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.security.authentication.filter.requestUrl">
        <xsd:annotation>
            <xsd:documentation>指定 HTTP 要求 URL，例如 requestUrl id="sample "urlPattern=" /account/checking | /account/saving "matchType =" contains "。</xsd:documentation>
            <xsd:appinfo>
                <ext:label>要求 URL</ext:label>
            </xsd:appinfo>
        </xsd:annotation>
        <!--Idiom for "empty element context" (whitespace accepted and ignored)-->
        <xsd:sequence>
            <xsd:sequence/>
        </xsd:sequence>
        <xsd:attribute name="urlPattern" type="xsd:string" use="required">
            <xsd:annotation>
                <xsd:documentation>指定 URL 型樣。不支援將 * 字元用作萬用字元。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>URL 型樣</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="contains" name="matchType" use="optional">
            <xsd:annotation>
                <xsd:documentation>指定相符類型。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>相符類型</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union memberTypes="variableType">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="equals">
                                <xsd:annotation>
                                    <xsd:documentation>等於</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="contains">
                                <xsd:annotation>
                                    <xsd:documentation>包含</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="notContain">
                                <xsd:annotation>
                                    <xsd:documentation>不包含</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.security.authentication.filter.requestUrl-factory">
        <xsd:complexContent>
            <xsd:extension base="com.ibm.ws.security.authentication.filter.requestUrl">
                <xsd:attribute name="id" type="factoryIdType" use="optional">
                    <xsd:annotation>
                        <xsd:documentation>唯一的配置 ID。</xsd:documentation>
                        <xsd:appinfo>
                            <ext:label>ID</ext:label>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.security.authentication.filter.requestHeader">
        <xsd:annotation>
            <xsd:documentation>指定 HTTP 要求標頭屬性名稱，例如 requestHeader id="sample "name = "email" value= "Kevin@yahoo.com|Steven@gmail.com" matchType = "contains"。</xsd:documentation>
            <xsd:appinfo>
                <ext:label>要求標頭</ext:label>
            </xsd:appinfo>
        </xsd:annotation>
        <!--Idiom for "empty element context" (whitespace accepted and ignored)-->
        <xsd:sequence>
            <xsd:sequence/>
        </xsd:sequence>
        <xsd:attribute name="name" type="xsd:string" use="required">
            <xsd:annotation>
                <xsd:documentation>指定名稱。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>名稱</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="value" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>值屬性會指定要求標頭的值。 如果未指定值，則會使用 name 屬性來進行比對，例如 requestHeader id="sample "name="email" matchType = "contains"。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>值</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="contains" name="matchType" use="optional">
            <xsd:annotation>
                <xsd:documentation>指定相符類型。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>相符類型</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union memberTypes="variableType">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="equals">
                                <xsd:annotation>
                                    <xsd:documentation>等於</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="contains">
                                <xsd:annotation>
                                    <xsd:documentation>包含</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="notContain">
                                <xsd:annotation>
                                    <xsd:documentation>不包含</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.security.authentication.filter.requestHeader-factory">
        <xsd:complexContent>
            <xsd:extension base="com.ibm.ws.security.authentication.filter.requestHeader">
                <xsd:attribute name="id" type="factoryIdType" use="optional">
                    <xsd:annotation>
                        <xsd:documentation>唯一的配置 ID。</xsd:documentation>
                        <xsd:appinfo>
                            <ext:label>ID</ext:label>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.security.authentication.filter.userAgent">
        <xsd:annotation>
            <xsd:documentation>指定使用者代理程式，例如 userAgent id="sample "agent="Mozilla/5.0"matchType =" contains "。</xsd:documentation>
            <xsd:appinfo>
                <ext:label>使用者代理程式</ext:label>
            </xsd:appinfo>
        </xsd:annotation>
        <!--Idiom for "empty element context" (whitespace accepted and ignored)-->
        <xsd:sequence>
            <xsd:sequence/>
        </xsd:sequence>
        <xsd:attribute name="agent" type="xsd:string" use="required">
            <xsd:annotation>
                <xsd:documentation>指定瀏覽器的使用者代理程式，以幫助識別正在使用的瀏覽器。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>代理程式</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="contains" name="matchType" use="optional">
            <xsd:annotation>
                <xsd:documentation>指定相符類型。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>相符類型</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union memberTypes="variableType">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="equals">
                                <xsd:annotation>
                                    <xsd:documentation>等於</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="contains">
                                <xsd:annotation>
                                    <xsd:documentation>包含</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="notContain">
                                <xsd:annotation>
                                    <xsd:documentation>不包含</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.security.authentication.filter.userAgent-factory">
        <xsd:complexContent>
            <xsd:extension base="com.ibm.ws.security.authentication.filter.userAgent">
                <xsd:attribute name="id" type="factoryIdType" use="optional">
                    <xsd:annotation>
                        <xsd:documentation>唯一的配置 ID。</xsd:documentation>
                        <xsd:appinfo>
                            <ext:label>ID</ext:label>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:complexContent>
