            <xsd:documentation>JNDI 預設名稱空間中的單一項目，用來連結 java.net.URL 項目。</xsd:documentation>
            <xsd:appinfo>
                <ext:label>JNDI URL 項目</ext:label>
            </xsd:appinfo>
        </xsd:annotation>
        <!--Idiom for "empty element context" (whitespace accepted and ignored)-->
        <xsd:sequence>
            <xsd:sequence/>
        </xsd:sequence>
        <xsd:attribute name="jndiName" type="xsd:string" use="required">
            <xsd:annotation>
                <xsd:documentation>要用於此項目的 JNDI 名稱。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>JNDI URL 項目名稱</ext:label>
                    <ext:unique>jndiName</ext:unique>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="value" type="xsd:string" use="required">
            <xsd:annotation>
                <xsd:documentation>要與此名稱產生關聯的 JNDI URL 值。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>JNDI URL 項目值</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.jndi.internal.JNDIURLEntry-factory">
        <xsd:complexContent>
            <xsd:extension base="com.ibm.ws.jndi.internal.JNDIURLEntry">
                <xsd:attribute name="id" type="factoryIdType" use="optional">
                    <xsd:annotation>
                        <xsd:documentation>唯一的配置 ID。</xsd:documentation>
                        <xsd:appinfo>
                            <ext:label>ID</ext:label>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.jndi.referenceEntry.properties">
        <xsd:annotation>
            <xsd:documentation>參照項目的內容。</xsd:documentation>
            <xsd:appinfo>
                <ext:label>內容</ext:label>
                <ext:extraProperties/>
            </xsd:appinfo>
        </xsd:annotation>
        <!--Idiom for "empty element context" (whitespace accepted and ignored)-->
        <xsd:sequence>
            <xsd:sequence/>
        </xsd:sequence>
        <xsd:attribute name="internal.properties" type="internalPropertiesType" use="optional">
            <xsd:annotation>
                <xsd:documentation>參照項目的其他內容。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>其他內容</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:anyAttribute processContents="skip"/>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.jndi.referenceEntry.properties-factory">
        <xsd:complexContent>
            <xsd:extension base="com.ibm.ws.jndi.referenceEntry.properties">
                <xsd:attribute name="id" type="factoryIdType" use="optional">
                    <xsd:annotation>
                        <xsd:documentation>唯一的配置 ID。</xsd:documentation>
                        <xsd:appinfo>
                            <ext:label>ID</ext:label>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.jndi.objectFactory">
        <xsd:annotation>
            <xsd:documentation>JNDI 參照項目要使用的 ObjectFactory。</xsd:documentation>
            <xsd:appinfo>
                <ext:label>JNDI 物件 Factory</ext:label>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element maxOccurs="1" minOccurs="0" name="library" type="com.ibm.ws.classloading.sharedlibrary">
                <xsd:annotation>
                    <xsd:documentation>含有 Factory 實作類別的程式庫。</xsd:documentation>
                    <xsd:appinfo>
                        <ext:label>共用程式庫</ext:label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="className" type="xsd:string" use="required">
            <xsd:annotation>
                <xsd:documentation>ObjectFactory 實作類別名稱。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Factory 類別名稱</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="java.lang.Object" name="objectClassName" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>Factory 傳回的物件類型。</xsd:documentation>
