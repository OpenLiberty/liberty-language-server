    <xsd:complexType name="com.ibm.ws.security.java2sec.policyConfig">
        <xsd:annotation>
            <xsd:documentation>Java 2 安全的權限配置。</xsd:documentation>
            <xsd:appinfo>
                <ext:label>Java 2 安全</ext:label>
            </xsd:appinfo>
        </xsd:annotation>
        <!--Idiom for "empty element context" (whitespace accepted and ignored)-->
        <xsd:sequence>
            <xsd:sequence/>
        </xsd:sequence>
        <xsd:attribute name="className" type="xsd:string" use="required">
            <xsd:annotation>
                <xsd:documentation>實作要授與之許可權的類別名稱。例如 java.io.FilePermission。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>權限類別</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="codebase" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>要被授予該權限的程式碼庫。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>程式碼庫</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="principalType" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>要針對給定主體名稱進行比對的類別名稱。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>主體類別</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="principalName" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>要被授予權限的主體。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>主體名稱</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="name" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>要套用許可權的目標。例如，以 java.io.FilePermission 來說，是「所有檔案」。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>目標名稱</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="actions" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>許可權授與容許對目標名稱執行的動作。例如，以 java.io.FilePermission 來說，是「讀取」。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>動作</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="false" name="restriction" type="booleanType" use="optional">
            <xsd:annotation>
                <xsd:documentation>宣告是限制或授與許可權。如果限制設為 "true"，則是拒絕而非授與此許可權。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>限制</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.security.java2sec.policyConfig-factory">
        <xsd:complexContent>
            <xsd:extension base="com.ibm.ws.security.java2sec.policyConfig">
                <xsd:attribute name="id" type="factoryIdType" use="optional">
