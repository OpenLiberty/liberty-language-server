    <xsd:complexType name="com.ibm.ws.jca.jmsTopicConnectionFactory.properties.wasJms.javax.jms.TopicConnectionFactory">
        <xsd:annotation>
            <xsd:documentation>JMS Topic Connection Factory 用來建立通往 JMS 目的地之相關 JMS 提供者的連線，供發佈/訂閱傳訊使用。</xsd:documentation>
            <xsd:appinfo>
                <ext:label>內嵌傳訊</ext:label>
            </xsd:appinfo>
        </xsd:annotation>
