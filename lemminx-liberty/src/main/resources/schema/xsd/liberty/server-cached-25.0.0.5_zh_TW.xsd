                <xsd:documentation>JDBC 驅動程式內容：ifxIFX_USEPUT。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>啟用大量插入</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ifxIFX_USE_STRENC" type="booleanType" use="optional">
            <xsd:annotation>
                <xsd:documentation>JDBC 驅動程式內容：ifxIFX_USE_STRENC。</xsd:documentation>
