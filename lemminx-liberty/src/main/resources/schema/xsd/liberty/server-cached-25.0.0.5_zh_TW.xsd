                <xsd:documentation>JDBC 驅動程式內容：timeFormat。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>時間格式</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union memberTypes="variableType">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="eur">
                                <xsd:annotation>
                                    <xsd:documentation>eur</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="hms">
                                <xsd:annotation>
                                    <xsd:documentation>hms</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="iso">
                                <xsd:annotation>
                                    <xsd:documentation>iso</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="jis">
                                <xsd:annotation>
                                    <xsd:documentation>jis</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="usa">
                                <xsd:annotation>
                                    <xsd:documentation>usa</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute name="timeSeparator" use="optional">
            <xsd:annotation>
                <xsd:documentation>JDBC 驅動程式內容：timeSeparator。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>時間分隔字元</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union memberTypes="variableType">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value=":">
                                <xsd:annotation>
                                    <xsd:documentation>冒號字元 (:)。</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value=".">
                                <xsd:annotation>
                                    <xsd:documentation>句點字元 (.)。</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="\,">
                                <xsd:annotation>
                                    <xsd:documentation>逗點字元 (,)。</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value=" ">
                                <xsd:annotation>
                                    <xsd:documentation>空格字元 ( )。</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute name="timestampFormat" use="optional">
            <xsd:annotation>
                <xsd:documentation>JDBC 驅動程式內容：timestampFormat。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>時間戳記格式</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union memberTypes="variableType">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="iso">
                                <xsd:annotation>
                                    <xsd:documentation>iso</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="ibmsql">
                                <xsd:annotation>
                                    <xsd:documentation>ibmsql</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute name="toolboxTrace" use="optional">
            <xsd:annotation>
                <xsd:documentation>JDBC 驅動程式內容：toolboxTrace。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>工具箱追蹤</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union memberTypes="variableType">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="none">
                                <xsd:annotation>
                                    <xsd:documentation>none</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="datastream">
                                <xsd:annotation>
                                    <xsd:documentation>datastream</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="diagnostic">
                                <xsd:annotation>
                                    <xsd:documentation>diagnostic</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="error">
                                <xsd:annotation>
                                    <xsd:documentation>error</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="information">
                                <xsd:annotation>
                                    <xsd:documentation>information</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="warning">
                                <xsd:annotation>
                                    <xsd:documentation>warning</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="conversion">
                                <xsd:annotation>
                                    <xsd:documentation>conversion</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="proxy">
                                <xsd:annotation>
                                    <xsd:documentation>proxy</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="pcml">
                                <xsd:annotation>
                                    <xsd:documentation>pcml</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="jdbc">
                                <xsd:annotation>
                                    <xsd:documentation>jdbc</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="all">
                                <xsd:annotation>
                                    <xsd:documentation>all</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="thread">
                                <xsd:annotation>
                                    <xsd:documentation>thread</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute name="trace" type="booleanType" use="optional">
            <xsd:annotation>
                <xsd:documentation>JDBC 驅動程式內容：trace。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>追蹤</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="false" name="translateBinary" type="booleanType" use="optional">
            <xsd:annotation>
                <xsd:documentation>JDBC 驅動程式內容：translateBinary。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>轉換二進位值</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="true" name="translateBoolean" type="booleanType" use="optional">
            <xsd:annotation>
                <xsd:documentation>JDBC 驅動程式內容：translateBoolean。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>轉換布林值</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="character" name="translateHex" use="optional">
            <xsd:annotation>
                <xsd:documentation>JDBC 驅動程式內容：translateHex。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>轉換十六進位值</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union memberTypes="variableType">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="binary">
                                <xsd:annotation>
                                    <xsd:documentation>binary</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="character">
                                <xsd:annotation>
                                    <xsd:documentation>character</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute default="false" name="trueAutoCommit" type="booleanType" use="optional">
            <xsd:annotation>
                <xsd:documentation>JDBC 驅動程式內容：trueAutoCommit。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>True 自動確定</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="useBlockUpdate" type="booleanType" use="optional">
            <xsd:annotation>
                <xsd:documentation>JDBC 驅動程式內容：useBlockUpdate。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>使用區塊更新</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="useDrdaMetadataVersion" type="booleanType" use="optional">
            <xsd:annotation>
                <xsd:documentation>JDBC 驅動程式內容：useDrdaMetadataVersion。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>使用 DRDA meta 資料版本</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="user" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>建議使用儲存器管理的鑑別別名，而非配置這個內容。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>使用者</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="variableFieldCompression" use="optional">
            <xsd:annotation>
                <xsd:documentation>JDBC 驅動程式內容：variableFieldCompression。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>變數欄位壓縮</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union memberTypes="variableType">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="true">
                                <xsd:annotation>
                                    <xsd:documentation>true</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="false">
                                <xsd:annotation>
                                    <xsd:documentation>false</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="insert">
                                <xsd:annotation>
                                    <xsd:documentation>insert</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="all">
                                <xsd:annotation>
                                    <xsd:documentation>all</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute default="0" name="xaLooselyCoupledSupport" use="optional">
            <xsd:annotation>
                <xsd:documentation>JDBC 驅動程式內容：xaLooselyCoupledSupport。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>XA 鬆散耦合支援</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union memberTypes="variableType">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:int">
                            <xsd:minInclusive value="0"/>
                            <xsd:maxInclusive value="1"/>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute name="internal.properties" type="internalPropertiesType" use="optional">
            <xsd:annotation>
                <xsd:documentation>資料來源的其他 JDBC 供應商內容</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>其他內容</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:anyAttribute processContents="skip"/>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.jdbc.dataSource.properties.db2.i.toolbox-factory">
        <xsd:complexContent>
            <xsd:extension base="com.ibm.ws.jdbc.dataSource.properties.db2.i.toolbox">
                <xsd:attribute name="id" type="factoryIdType" use="optional">
                    <xsd:annotation>
                        <xsd:documentation>唯一的配置 ID。</xsd:documentation>
                        <xsd:appinfo>
                            <ext:label>ID</ext:label>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.jdbc.dataSource">
        <xsd:annotation>
            <xsd:documentation>定義資料來源配置。</xsd:documentation>
            <xsd:appinfo>
                <ext:label>資料來源</ext:label>
                <ext:groupDecl id="Advanced" label="進階內容">提供進階用法的其他內容。</ext:groupDecl>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:choice maxOccurs="unbounded" minOccurs="0">
            <xsd:element name="jdbcDriver" type="com.ibm.ws.jdbc.jdbcDriver">
                <xsd:annotation>
                    <xsd:documentation>資料來源的 JDBC 驅動程式。如果在伺服器執行期間更新，會造成現有連線毀損。</xsd:documentation>
                    <xsd:appinfo>
                        <ext:label>JDBC 驅動程式</ext:label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="connectionManager" type="com.ibm.ws.jca.connectionManager">
                <xsd:annotation>
                    <xsd:documentation>資料來源的連線管理程式。如果在伺服器執行期間更新，會造成現有連線毀損。</xsd:documentation>
                    <xsd:appinfo>
                        <ext:label>連線管理程式</ext:label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="properties.informix" type="com.ibm.ws.jdbc.dataSource.properties.informix"/>
            <xsd:element name="properties.db2.i.native" type="com.ibm.ws.jdbc.dataSource.properties.db2.i.native"/>
            <xsd:element name="properties.oracle.ucp" type="com.ibm.ws.jdbc.dataSource.properties.oracle.ucp"/>
            <xsd:element name="properties.derby.embedded" type="com.ibm.ws.jdbc.dataSource.properties.derby.embedded"/>
            <xsd:element name="properties" type="com.ibm.ws.jdbc.dataSource.properties"/>
            <xsd:element name="properties.microsoft.sqlserver" type="com.ibm.ws.jdbc.dataSource.properties.microsoft.sqlserver"/>
            <xsd:element name="properties.oracle" type="com.ibm.ws.jdbc.dataSource.properties.oracle"/>
            <xsd:element name="properties.derby.client" type="com.ibm.ws.jdbc.dataSource.properties.derby.client"/>
            <xsd:element name="properties.datadirect.sqlserver" type="com.ibm.ws.jdbc.dataSource.properties.datadirect.sqlserver"/>
            <xsd:element name="properties.informix.jcc" type="com.ibm.ws.jdbc.dataSource.properties.informix.jcc"/>
            <xsd:element name="properties.db2.jcc" type="com.ibm.ws.jdbc.dataSource.properties.db2.jcc"/>
            <xsd:element name="properties.db2.i.toolbox" type="com.ibm.ws.jdbc.dataSource.properties.db2.i.toolbox"/>
            <xsd:element name="properties.sybase" type="com.ibm.ws.jdbc.dataSource.properties.sybase"/>
            <xsd:element name="properties.postgresql" type="com.ibm.ws.jdbc.dataSource.properties.postgresql"/>
            <xsd:element name="containerAuthData" type="com.ibm.ws.security.jca.internal.authdata.config">
                <xsd:annotation>
                    <xsd:documentation>儲存器管理鑑別的預設鑑別資料，當連結沒有為 res-auth=CONTAINER 的資源參照指定鑑別別名時，就會套用它。如果在伺服器執行期間更新，會將變更套用至新的連線要求；使用中的連線不受影響。</xsd:documentation>
                    <xsd:appinfo>
                        <ext:label>預設儲存器管理的鑑別資料</ext:label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="jaasLoginContextEntry" type="com.ibm.ws.security.authentication.internal.jaas.jaasLoginContextEntry">
                <xsd:annotation>
                    <xsd:documentation>鑑別用的 JAAS 登入環境定義項目。如果在伺服器執行期間更新，會將更新套用至新的連線要求；使用中的連線不受影響。</xsd:documentation>
                    <xsd:appinfo>
                        <ext:label>JAAS 登入環境定義項目</ext:label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="identifyException" type="com.ibm.ws.jdbc.dataSource.identifyException-factory">
                <xsd:annotation>
                    <xsd:documentation>在 SQLException 上識別特定的 SQL 錯誤碼或 SQL 狀態。如此一來，伺服器即可根據錯誤狀況，採取適當的動作。例如，關閉過時的連線，而非將其傳回給連線儲存區。</xsd:documentation>
                    <xsd:appinfo>
                        <ext:label>識別異常狀況</ext:label>
                        <ext:group id="Advanced"/>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="onConnect" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>這個 SQL 指令用來對新建的資料庫連線執行一次。此 SQL 陳述式只適用於新建的連線，不適用於從連線儲存區重複使用的現有連線。如果在伺服器執行期間更新，會造成現有連線毀損。</xsd:documentation>
                    <xsd:appinfo>
                        <ext:label>On connect</ext:label>
                        <ext:group id="Advanced"/>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="recoveryAuthData" type="com.ibm.ws.security.jca.internal.authdata.config">
                <xsd:annotation>
                    <xsd:documentation>交易回復的鑑別資料。</xsd:documentation>
                    <xsd:appinfo>
                        <ext:label>回復鑑別資料</ext:label>
                        <ext:group id="Advanced"/>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
        </xsd:choice>
        <xsd:attribute default="false" name="enableConnectionCasting" type="booleanType" use="optional">
            <xsd:annotation>
                <xsd:documentation>指出從資料來源取得的連線，應該可以強制轉型為 JDBC 供應商連線實作所實作的介面類別。如果啟用這個選項，會增加每一項 getConnection 作業的額外負擔。如果不常需要供應商 JDBC 介面，更有效率的作法是維持停用這個選項，並且只在需要時才使用 Connection.unwrap(interface)。如果在伺服器執行期間更新，會將更新套用至新的連線要求；使用中的連線不受影響。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>啟用連線強制轉型</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="jndiName" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>資料來源的 JNDI 名稱。如果在伺服器執行期間更新，會造成現有連線毀損。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>JNDI 名稱</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="jdbcDriverRef" type="pidType" use="optional">
            <xsd:annotation>
                <xsd:documentation>資料來源的 JDBC 驅動程式。如果在伺服器執行期間更新，會造成現有連線毀損。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>JDBC 驅動程式參照</ext:label>
                    <ext:reference>jdbcDriver</ext:reference>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="connectionManagerRef" type="pidType" use="optional">
            <xsd:annotation>
                <xsd:documentation>資料來源的連線管理程式。如果在伺服器執行期間更新，會造成現有連線毀損。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>連線管理程式參照</ext:label>
                    <ext:reference>connectionManager</ext:reference>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="type" use="optional">
            <xsd:annotation>
                <xsd:documentation>資料來源的類型。如果在伺服器執行期間更新，會造成現有連線毀損。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>類型</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union memberTypes="variableType">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="javax.sql.XADataSource">
                                <xsd:annotation>
                                    <xsd:documentation>javax.sql.XADataSource</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="javax.sql.ConnectionPoolDataSource">
                                <xsd:annotation>
                                    <xsd:documentation>javax.sql.ConnectionPoolDataSource</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="javax.sql.DataSource">
                                <xsd:annotation>
                                    <xsd:documentation>javax.sql.DataSource</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="java.sql.Driver">
                                <xsd:annotation>
                                    <xsd:documentation>java.sql.Driver</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute default="MatchOriginalRequest" name="connectionSharing" use="optional">
            <xsd:annotation>
                <xsd:documentation>指明如何比對用於共用的連線。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>連線比對</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union memberTypes="variableType">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="MatchOriginalRequest">
                                <xsd:annotation>
                                    <xsd:documentation>當共用連線時，根據原始連線要求比對。</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="MatchCurrentState">
                                <xsd:annotation>
                                    <xsd:documentation>當共用連線時，根據連線的現行狀態比對。如果在伺服器執行期間更新，會將更新套用至交易中各第一個連線控點。</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute name="containerAuthDataRef" type="pidType" use="optional">
            <xsd:annotation>
                <xsd:documentation>儲存器管理鑑別的預設鑑別資料，當連結沒有為 res-auth=CONTAINER 的資源參照指定鑑別別名時，就會套用它。如果在伺服器執行期間更新，會將變更套用至新的連線要求；使用中的連線不受影響。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>儲存器管理的預設鑑別資料</ext:label>
                    <ext:reference>authData</ext:reference>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="jaasLoginContextEntryRef" type="pidType" use="optional">
            <xsd:annotation>
                <xsd:documentation>鑑別用的 JAAS 登入環境定義項目。如果在伺服器執行期間更新，會將更新套用至新的連線要求；使用中的連線不受影響。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>JAAS 登入環境定義項目參照</ext:label>
                    <ext:reference>jaasLoginContextEntry</ext:reference>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="isolationLevel" use="optional">
            <xsd:annotation>
                <xsd:documentation>預設交易隔離層次。若未指定，並將資料庫識別為 Db2、Derby、Informix、Microsoft SQL Server 或 Sybase，則會使用 TRANSACTION_REPEATABLE_READ。如果沒有為其他資料庫指定此項，則會使用 TRANSACTION_READ_COMMITTED。如果在伺服器執行期間更新，會將更新套用至新的連線要求；使用中的連線不受影響。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>交易隔離層次</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union memberTypes="variableType">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="TRANSACTION_READ_UNCOMMITTED">
                                <xsd:annotation>
                                    <xsd:documentation>可以進行錯誤讀取、不可重複讀取和幻象讀取。</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="TRANSACTION_READ_COMMITTED">
                                <xsd:annotation>
                                    <xsd:documentation>禁止錯誤讀取；可以進行不可重複讀取和幻象讀取。</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="TRANSACTION_REPEATABLE_READ">
                                <xsd:annotation>
                                    <xsd:documentation>禁止錯誤讀取和不可重複讀取；可以進行幻象讀取。</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="TRANSACTION_SERIALIZABLE">
                                <xsd:annotation>
                                    <xsd:documentation>禁止錯誤讀取、不可重複讀取和幻象讀取。</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="TRANSACTION_SNAPSHOT">
                                <xsd:annotation>
                                    <xsd:documentation>Microsoft SQL Server JDBC 驅動程式和 DataDirect Connect for JDBC 驅動程式的 Snapshot 隔離。</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="TRANSACTION_NONE">
                                <xsd:annotation>
                                    <xsd:documentation>指出 JDBC 驅動程式不支援交易。</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute default="10" name="statementCacheSize" use="optional">
            <xsd:annotation>
                <xsd:documentation>每個連線的快取陳述式數目上限。如果在伺服器執行期間更新，在下次使用時，會調整陳述式快取大小。若要設定這個選項，請完成下列必要條件：在您從資料庫或資料庫驅動程式收集的應用程式碼或 SQL 追蹤中，檢查所有唯一的備妥陳述式。確定快取記憶體大小大於陳述式的數目。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>每個連線快取的陳述式數目</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union memberTypes="variableType">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:int">
                            <xsd:minInclusive value="0"/>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute default="true" name="transactional" type="booleanType" use="optional">
            <xsd:annotation>
                <xsd:documentation>啟用參與應用程式伺服器所管理的交易。如果在伺服器執行期間更新，會造成現有連線毀損。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>參與交易</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="true" name="beginTranForResultSetScrollingAPIs" type="booleanType" use="optional">
            <xsd:annotation>
                <xsd:documentation>使用結果集捲動介面時嘗試交易列入。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>列入捲動 API</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="true" name="beginTranForVendorAPIs" type="booleanType" use="optional">
            <xsd:annotation>
                <xsd:documentation>使用供應商介面時嘗試交易列入。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>列入供應商 API</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="commitOrRollbackOnCleanup" use="optional">
            <xsd:annotation>
                <xsd:documentation>當連線關閉或傳回到儲存區時，決定如何清除可能在資料庫工作單元中的連線 (AutoCommit=false)。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>清理時確定或回復</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union memberTypes="variableType">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="commit">
                                <xsd:annotation>
                                    <xsd:documentation>藉由確定來清除連線。</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="rollback">
                                <xsd:annotation>
                                    <xsd:documentation>藉由回復來清除連線。</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute name="queryTimeout" type="secondDuration" use="optional">
            <xsd:annotation>
                <xsd:documentation>SQL 陳述式的預設查詢逾時值。在 JTA 交易中，syncQueryTimeoutWithTransactionTimeout 可置換這個預設值。. 指定正整數加上時間單位，時間單位可以是小時 (h)、分鐘 (m) 或秒 (s)。例如，指定 30 秒為 30s。您可以在單一項目中包括多個值。例如，1m30s 相等於 90 秒。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>查詢逾時值</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="recoveryAuthDataRef" type="pidType" use="optional">
            <xsd:annotation>
                <xsd:documentation>交易回復的鑑別資料。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>回復鑑別資料參照</ext:label>
                    <ext:group id="Advanced"/>
                    <ext:reference>authData</ext:reference>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="false" name="syncQueryTimeoutWithTransactionTimeout" type="booleanType" use="optional">
            <xsd:annotation>
                <xsd:documentation>使用 JTA 交易中的剩餘時間（如果有的話）作為 SQL 陳述式的預設查詢逾時值。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>同步化查詢和交易逾時值</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="supplementalJDBCTrace" type="booleanType" use="optional">
            <xsd:annotation>
                <xsd:documentation>當已在 bootstrap.properties 中啟用 JDBC 驅動程式追蹤時，補充所記載的 JDBC 驅動程式追蹤。JDBC 驅動程式追蹤規格包括：com.ibm.ws.database.logwriter、com.ibm.ws.db2.logwriter、com.ibm.ws.derby.logwriter、com.ibm.ws.informix.logwriter、com.ibm.ws.oracle.logwriter、com.ibm.ws.sqlserver.logwriter、com.ibm.ws.sybase.logwriter。如果在伺服器執行期間更新，會造成現有連線毀損。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>補充 JDBC 追蹤</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="validationTimeout" type="secondDuration" use="optional">
            <xsd:annotation>
                <xsd:documentation>若有指定，會先驗證儲存區中的連線，再從連線儲存區中重複使用。 在連線管理程式驗證連線，以回應 PurgePolicy=ValidateAllConnections 的過時連線時，也會使用驗證逾時值。 驗證逾時的完整數量適用於每一個已驗證的連線，這是透過 Connection.isValid(timeout) JDBC API 作業來完成。 值 0 表示在驗證連線時，不會套用任何逾時值。 驗證逾時值需要符合 JDBC 4.0 規格或以上的 JDBC 驅動程式。. 指定正整數加上時間單位，時間單位可以是小時 (h)、分鐘 (m) 或秒 (s)。例如，指定 30 秒為 30s。您可以在單一項目中包括多個值。例如，1m30s 相等於 90 秒。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>驗證逾時值</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.jdbc.dataSource-factory">
        <xsd:complexContent>
            <xsd:extension base="com.ibm.ws.jdbc.dataSource">
                <xsd:attribute name="id" type="factoryIdType" use="optional">
                    <xsd:annotation>
                        <xsd:documentation>唯一的配置 ID。</xsd:documentation>
                        <xsd:appinfo>
                            <ext:label>ID</ext:label>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.jdbc.dataSource.properties.sybase">
        <xsd:annotation>
            <xsd:documentation>Sybase JDBC 驅動程式的資料來源內容。</xsd:documentation>
            <xsd:appinfo>
                <ext:label>Sybase 內容</ext:label>
                <ext:extraProperties/>
                <ext:groupDecl id="Advanced" label="進階內容">提供進階用法的其他內容。</ext:groupDecl>
            </xsd:appinfo>
        </xsd:annotation>
        <!--Idiom for "empty element context" (whitespace accepted and ignored)-->
        <xsd:sequence>
            <xsd:sequence/>
        </xsd:sequence>
        <xsd:attribute name="databaseName" type="xsd:string" use="required">
            <xsd:annotation>
                <xsd:documentation>JDBC 驅動程式內容：databaseName。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>資料庫名稱</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="localhost" name="serverName" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>資料庫執行所在的伺服器。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>伺服器名稱</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="5000" name="portNumber" type="intType" use="optional">
            <xsd:annotation>
                <xsd:documentation>用於取得資料庫連線的埠。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>埠號</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="SELECT_OPENS_CURSOR=true" name="connectionProperties" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>JDBC 驅動程式內容：connectionProperties。可以使用用於其他資料來源密碼的同一程序，來配置 javax.net.ssl.keyStorePassword 和 javax.net.ssl.trustStorePassword SSL 內容的加密值。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>連線內容</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="loginTimeout" type="secondDuration" use="optional">
            <xsd:annotation>
                <xsd:documentation>JDBC 驅動程式內容：loginTimeout。. 指定正整數加上時間單位，時間單位可以是小時 (h)、分鐘 (m) 或秒 (s)。例如，指定 30 秒為 30s。您可以在單一項目中包括多個值。例如，1m30s 相等於 90 秒。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>登入逾時</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="networkProtocol" use="optional">
            <xsd:annotation>
                <xsd:documentation>JDBC 驅動程式內容：networkProtocol。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>網路通訊協定</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union memberTypes="variableType">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="socket">
                                <xsd:annotation>
                                    <xsd:documentation>socket</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="SSL">
                                <xsd:annotation>
                                    <xsd:documentation>SSL</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute name="password" type="password" use="optional">
            <xsd:annotation>
                <xsd:documentation>建議使用儲存器管理的鑑別別名，而非配置這個內容。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>密碼</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="resourceManagerName" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>JDBC 驅動程式內容：resourceManagerName。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>資源管理程式名稱</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="false" name="SERVER_INITIATED_TRANSACTIONS" use="optional">
            <xsd:annotation>
                <xsd:documentation>JDBC 驅動程式內容：SERVER_INITIATED_TRANSACTIONS。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>伺服器已起始交易</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union memberTypes="variableType">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="true">
                                <xsd:annotation>
                                    <xsd:documentation>true</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="false">
                                <xsd:annotation>
                                    <xsd:documentation>false</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute name="user" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>建議使用儲存器管理的鑑別別名，而非配置這個內容。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>使用者</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="version" type="intType" use="optional">
            <xsd:annotation>
                <xsd:documentation>JDBC 驅動程式內容：version。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>版本</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="internal.properties" type="internalPropertiesType" use="optional">
            <xsd:annotation>
                <xsd:documentation>資料來源的其他 JDBC 供應商內容</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>其他內容</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:anyAttribute processContents="skip"/>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.jdbc.dataSource.properties.sybase-factory">
        <xsd:complexContent>
            <xsd:extension base="com.ibm.ws.jdbc.dataSource.properties.sybase">
                <xsd:attribute name="id" type="factoryIdType" use="optional">
                    <xsd:annotation>
                        <xsd:documentation>唯一的配置 ID。</xsd:documentation>
                        <xsd:appinfo>
                            <ext:label>ID</ext:label>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.jdbc.dataSource.properties.postgresql">
        <xsd:annotation>
            <xsd:documentation>PostgreSQL JDBC 驅動程式的資料來源內容。</xsd:documentation>
            <xsd:appinfo>
                <ext:label>PostgreSQL 內容</ext:label>
                <ext:extraProperties/>
                <ext:groupDecl id="Advanced" label="進階內容">提供進階用法的其他內容。</ext:groupDecl>
            </xsd:appinfo>
        </xsd:annotation>
        <!--Idiom for "empty element context" (whitespace accepted and ignored)-->
        <xsd:sequence>
            <xsd:sequence/>
        </xsd:sequence>
        <xsd:attribute name="databaseName" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>JDBC 驅動程式內容：databaseName。 當 PostgreSQL 驅動程式使用 java.sql.Driver 類型，且配置了 URL 和這個內容時， PostgreSQL 驅動程式會忽略這個內容。 當 PostgreSQL 驅動程式使用 javax.sql.DataSource類型，且已配置 URL 和這個內容時， PostgreSQL 驅動程式會忽略 databaseName 內容。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>資料庫名稱</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="serverName" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>JDBC 驅動程式內容: serverName。 當 PostgreSQL 驅動程式使用 java.sql.Driver 類型，且配置了 URL 和這個內容時， PostgreSQL 驅動程式會忽略這個內容。 當 PostgreSQL 驅動程式使用 javax.sql.DataSource類型，且已配置 URL 和這個內容時， PostgreSQL 驅動程式會忽略 serverName 內容。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>伺服器名稱</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="portNumber" type="intType" use="optional">
            <xsd:annotation>
                <xsd:documentation>JDBC 驅動程式內容: portNumber。 當 PostgreSQL 驅動程式使用 java.sql.Driver 類型，且配置了 URL 和這個內容時， PostgreSQL 驅動程式會忽略這個內容。 當 PostgreSQL 驅動程式使用 javax.sql.DataSource類型，且已配置 URL 和這個內容時， PostgreSQL 驅動程式會忽略 portNumber 內容。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>埠號</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="applicationName" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>JDBC 驅動程式內容：applicationName。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>應用程式名稱</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="cancelSignalTimeout" type="secondDuration" use="optional">
            <xsd:annotation>
                <xsd:documentation>JDBC 驅動程式內容：cancelSignalTimeout。. 指定正整數加上時間單位，時間單位可以是小時 (h)、分鐘 (m) 或秒 (s)。例如，指定 30 秒為 30s。您可以在單一項目中包括多個值。例如，1m30s 相等於 90 秒。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>取消信號逾時</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="connectTimeout" type="secondDuration" use="optional">
            <xsd:annotation>
                <xsd:documentation>JDBC 驅動程式內容：connectTimeout。. 指定正整數加上時間單位，時間單位可以是小時 (h)、分鐘 (m) 或秒 (s)。例如，指定 30 秒為 30s。您可以在單一項目中包括多個值。例如，1m30s 相等於 90 秒。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>連接逾時值</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="currentSchema" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>JDBC 驅動程式內容：currentSchema。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>現行綱目</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="defaultRowFetchSize" use="optional">
            <xsd:annotation>
                <xsd:documentation>JDBC 驅動程式內容：defaultRowFetchSize。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>預設列提取大小</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union memberTypes="variableType">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:int">
                            <xsd:minInclusive value="0"/>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute name="loginTimeout" type="secondDuration" use="optional">
            <xsd:annotation>
                <xsd:documentation>JDBC 驅動程式內容：loginTimeout。. 指定正整數加上時間單位，時間單位可以是小時 (h)、分鐘 (m) 或秒 (s)。例如，指定 30 秒為 30s。您可以在單一項目中包括多個值。例如，1m30s 相等於 90 秒。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>登入逾時</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="password" type="password" use="optional">
            <xsd:annotation>
                <xsd:documentation>建議使用儲存器管理的鑑別別名，而非配置這個內容。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>密碼</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="0" name="preparedStatementCacheQueries" use="optional">
            <xsd:annotation>
                <xsd:documentation>JDBC 驅動程式內容：preparedStatementCacheQueries。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>備妥陳述式快取查詢</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union memberTypes="variableType">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:int">
                            <xsd:minInclusive value="0"/>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute name="readOnly" type="booleanType" use="optional">
            <xsd:annotation>
                <xsd:documentation>JDBC 驅動程式內容：readOnly。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>唯讀</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ssl" type="booleanType" use="optional">
            <xsd:annotation>
                <xsd:documentation>JDBC 驅動程式內容：ssl。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>SSL</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="sslCert" type="location" use="optional">
            <xsd:annotation>
                <xsd:documentation>JDBC 驅動程式內容：sslCert。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>SSL 憑證位置</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="sslfactory" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>JDBC 驅動程式內容：sslfactory。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>SSL Factory 類別名稱</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="sslKey" type="location" use="optional">
            <xsd:annotation>
                <xsd:documentation>JDBC 驅動程式內容：sslKey。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>SSL 金鑰位置</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="sslMode" use="optional">
            <xsd:annotation>
                <xsd:documentation>JDBC 驅動程式內容：sslMode。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>SSL 模式</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union memberTypes="variableType">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="disable">
                                <xsd:annotation>
                                    <xsd:documentation>disable</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="allow">
                                <xsd:annotation>
                                    <xsd:documentation>allow</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="prefer">
                                <xsd:annotation>
                                    <xsd:documentation>prefer</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="require">
                                <xsd:annotation>
                                    <xsd:documentation>require</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="verify-ca">
                                <xsd:annotation>
                                    <xsd:documentation>verify-ca</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="verify-full">
                                <xsd:annotation>
                                    <xsd:documentation>verify-full</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute name="sslPassword" type="password" use="optional">
            <xsd:annotation>
                <xsd:documentation>JDBC 驅動程式內容：sslPassword。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>SSL 密碼</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="sslRootCert" type="location" use="optional">
            <xsd:annotation>
                <xsd:documentation>JDBC 驅動程式內容：sslRootCert。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>SSL 主要憑證位置</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="socketTimeout" type="secondDuration" use="optional">
            <xsd:annotation>
                <xsd:documentation>JDBC 驅動程式內容：socketTimeout。. 指定正整數加上時間單位，時間單位可以是小時 (h)、分鐘 (m) 或秒 (s)。例如，指定 30 秒為 30s。您可以在單一項目中包括多個值。例如，1m30s 相等於 90 秒。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Socket 逾時值</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="targetServerType" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>JDBC 驅動程式內容：targetServerType。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>目標伺服器類型</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="tcpKeepAlive" type="booleanType" use="optional">
            <xsd:annotation>
                <xsd:documentation>JDBC 驅動程式內容：tcpKeepAlive。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>TCP 保持作用中</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="user" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>建議使用儲存器管理的鑑別別名，而非配置這個內容。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>使用者</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="URL" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>用來連接資料庫的 URL。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>URL</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="internal.properties" type="internalPropertiesType" use="optional">
            <xsd:annotation>
                <xsd:documentation>資料來源的其他 JDBC 供應商內容</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>其他內容</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:anyAttribute processContents="skip"/>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.jdbc.dataSource.properties.postgresql-factory">
