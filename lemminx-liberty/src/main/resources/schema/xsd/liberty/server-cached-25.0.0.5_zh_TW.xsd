                <ext:label>JNDI 參照項目</ext:label>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:choice maxOccurs="unbounded" minOccurs="0">
            <xsd:element name="factory" type="com.ibm.ws.jndi.objectFactory">
