            <xsd:annotation>
                <xsd:documentation>找不到所要求的 URI 中的應用程式時要傳回用戶端的訊息。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>找不到應用程式時的訊息</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
