            <xsd:appinfo>
                <ext:label>JNDI 項目</ext:label>
            </xsd:appinfo>
        </xsd:annotation>
        <!--Idiom for "empty element context" (whitespace accepted and ignored)-->
        <xsd:sequence>
            <xsd:sequence/>
        </xsd:sequence>
        <xsd:attribute name="jndiName" type="xsd:string" use="required">
            <xsd:annotation>
                <xsd:documentation>要用於此項目的 JNDI 名稱。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>JNDI 項目名稱</ext:label>
                    <ext:unique>jndiName</ext:unique>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="value" type="xsd:string" use="required">
            <xsd:annotation>
                <xsd:documentation>要與此名稱建立關聯的 JNDI 值。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>JNDI 項目值</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="false" name="decode" type="booleanType" use="optional">
            <xsd:annotation>
                <xsd:documentation>如果查閱時需要將值解碼，則為 True。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>JNDI 解碼值</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.jndi.internal.JNDIEntry-factory">
