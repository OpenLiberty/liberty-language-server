                    <ext:label>使用字串編碼</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="y" name="ifxIFX_XASPEC" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>JDBC 驅動程式內容：ifxIFX_XASPEC。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>緊密耦合 XA 交易</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ifxINFORMIXCONRETRY" type="intType" use="optional">
            <xsd:annotation>
                <xsd:documentation>JDBC 驅動程式內容：ifxINFORMIXCONRETRY。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>連線重試次數上限</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
