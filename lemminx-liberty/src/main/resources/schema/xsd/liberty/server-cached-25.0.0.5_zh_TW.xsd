    <xsd:complexType name="includeType">
        <xsd:annotation>
            <xsd:documentation>指定要包含在伺服器配置中的配置資源。</xsd:documentation>
            <xsd:appinfo>
                <ext:label>併入</ext:label>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:attribute default="false" name="optional" type="xsd:boolean" use="optional">
            <xsd:annotation>
                <xsd:documentation>當找不到所包含的資源時，容許跳過它。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>資源是選用的</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="location" type="location" use="required">
            <xsd:annotation>
                <xsd:documentation>指定資源位置。 值可以是遠端資源的檔案路徑、目錄或 URI。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>位置</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="MERGE" name="onConflict" use="optional">
            <xsd:annotation>
                <xsd:documentation>指定在發現衝突時，用來合併元素的行為。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>衝突時</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union memberTypes="variableType">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="MERGE">
                                <xsd:annotation>
                                    <xsd:documentation>衝突的元素會合併。</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="REPLACE">
                                <xsd:annotation>
                                    <xsd:documentation>當元素衝突時，來自併入檔案的元素會取代衝突的元素。</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="IGNORE">
                                <xsd:annotation>
                                    <xsd:documentation>併入檔案中的衝突元素會被忽略。</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:complexType name="variableDefinitionType">
