                <xsd:documentation>「郵件階段作業」的主機</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>郵件伺服器的主機位址</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="user" type="xsd:string" use="required">
            <xsd:annotation>
                <xsd:documentation>使用者在主機上使用的電子郵件位址。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>使用者帳戶</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="password" type="password" use="required">
