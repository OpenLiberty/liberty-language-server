        <xsd:sequence>
            <xsd:sequence/>
        </xsd:sequence>
        <xsd:attribute default="defaultBus" name="busName" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>連接至 WebSphere Application Server 傳統版中的服務整合匯流排時的匯流排名稱。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>匯流排名稱</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="clientID" name="clientID" type="xsd:string" use="optional">
