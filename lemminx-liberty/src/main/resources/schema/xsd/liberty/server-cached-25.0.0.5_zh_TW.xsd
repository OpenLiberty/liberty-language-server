                    <ext:label>在 SSO 之後呼叫攔截程式</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="libraryRef" type="pidType" use="optional">
            <xsd:annotation>
                <xsd:documentation>共用程式庫配置 ID 的參照。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>共用程式庫</ext:label>
                    <ext:reference>library</ext:reference>
                </xsd:appinfo>
            </xsd:annotation>
