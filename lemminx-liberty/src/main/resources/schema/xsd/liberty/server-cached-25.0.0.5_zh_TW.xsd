    <xsd:complexType name="com.ibm.ws.security.wim.adapter.ldap.attributeConfiguration">
        <xsd:annotation>
            <xsd:documentation>這項配置是定義 LDAP 屬性與使用者登錄綱目欄位名稱的對映。</xsd:documentation>
            <xsd:appinfo>
                <ext:label>LDAP 屬性對映定義</ext:label>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:choice maxOccurs="unbounded" minOccurs="0">
            <xsd:element name="attribute" type="com.ibm.ws.security.wim.adapter.ldap.attribute-factory">
                <xsd:annotation>
                    <xsd:documentation>定義要對映至 LDAP 屬性的使用者登錄綱目欄位名稱。</xsd:documentation>
