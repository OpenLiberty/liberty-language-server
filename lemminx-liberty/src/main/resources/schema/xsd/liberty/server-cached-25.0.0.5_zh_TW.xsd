                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="customFiltersRef" type="pidType" use="optional">
            <xsd:annotation>
                <xsd:documentation>指定自訂 LDAP 過濾器的清單。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>自訂 LDAP 過濾器參照</ext:label>
                    <ext:reference>customLdapFilterProperties</ext:reference>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="domino50FiltersRef" type="pidType" use="optional">
            <xsd:annotation>
                <xsd:documentation>指定 IBM Lotus Domino LDAP 過濾器的清單。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>IBM Lotus Domino LDAP 過濾器參照</ext:label>
                    <ext:reference>domino50LdapFilterProperties</ext:reference>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="edirectoryFiltersRef" type="pidType" use="optional">
