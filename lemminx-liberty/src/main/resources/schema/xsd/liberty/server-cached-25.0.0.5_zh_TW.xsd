                    <ext:label>Socket 逾時值</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ifxIFX_USEPUT" type="booleanType" use="optional">
