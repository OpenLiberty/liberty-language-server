            <xsd:appinfo>
                <ext:label>郵件階段作業物件</ext:label>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="property" type="com.ibm.ws.javamail.mailSession.property-factory"/>
        </xsd:sequence>
        <xsd:attribute name="mailSessionID" type="xsd:string" use="required">
            <xsd:annotation>
                <xsd:documentation>特定「郵件階段作業實例」的 ID</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>郵件階段作業 ID</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="jndiName" type="xsd:string" use="required">
            <xsd:annotation>
                <xsd:documentation>用於 JNDI 查閱之「郵件階段作業」參照的名稱</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>JNDI 查閱名稱</ext:label>
                    <ext:unique>jndiName</ext:unique>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="description" type="xsd:string" use="required">
            <xsd:annotation>
                <xsd:documentation>郵件階段作業的說明</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>mailSession 的說明</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="imap" name="storeProtocol" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>「郵件階段作業」實例使用的「儲存通訊協定」。預設儲存通訊協定是 IMAP</xsd:documentation>
