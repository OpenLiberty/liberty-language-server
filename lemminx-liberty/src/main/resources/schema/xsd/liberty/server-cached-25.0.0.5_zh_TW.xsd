                    <xsd:documentation>參照項目的物件 Factory。</xsd:documentation>
                    <xsd:appinfo>
                        <ext:label>物件 Factory</ext:label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="properties" type="com.ibm.ws.jndi.referenceEntry.properties">
                <xsd:annotation>
                    <xsd:documentation>參照項目的內容。</xsd:documentation>
                    <xsd:appinfo>
                        <ext:label>內容</ext:label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
        </xsd:choice>
        <xsd:attribute name="jndiName" type="xsd:string" use="required">
            <xsd:annotation>
                <xsd:documentation>參照項目的 JNDI 名稱。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>JNDI 名稱</ext:label>
                    <ext:unique>jndiName</ext:unique>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="factoryRef" type="pidType" use="optional">
            <xsd:annotation>
                <xsd:documentation>參照項目的物件 Factory。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>物件 Factory 參照</ext:label>
                    <ext:reference>jndiObjectFactory</ext:reference>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="false" name="decode" type="booleanType" use="optional">
            <xsd:annotation>
                <xsd:documentation>如果查閱時需要將值解碼，則為 True。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>JNDI 解碼值</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
