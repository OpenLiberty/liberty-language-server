            <xsd:annotation>
                <xsd:documentation>埠名稱用來指定 Web 服務埠的名稱。</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>名稱</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="namespace" type="xsd:string" use="optional">
