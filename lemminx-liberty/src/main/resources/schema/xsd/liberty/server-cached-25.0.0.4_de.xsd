    <xsd:complexType name="com.ibm.ws.javaee.ddmodel.wsbnd.Port">
        <xsd:annotation>
            <xsd:documentation>Ein Portelement definiert die Portkonfigurationen für eine Web-Service-Referenz. </xsd:documentation>
