                <xsd:appinfo>
                    <ext:label>Nachrichtenreihenfolge strikt einhalten</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="50000" name="maxMessageDepth" use="optional">
