                <xsd:appinfo>
                    <ext:label>URL des Tokenendpunkts</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="https://api.github.com/user/emails" name="userApi" type="xsd:string" use="optional">
