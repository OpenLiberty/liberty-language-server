                                    <xsd:documentation>Enthält</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="notContain">
                                <xsd:annotation>
                                    <xsd:documentation>Enthält nicht</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
