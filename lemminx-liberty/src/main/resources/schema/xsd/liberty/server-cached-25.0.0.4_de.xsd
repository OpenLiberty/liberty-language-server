                <xsd:appinfo>
                    <ext:label>URL des Berechtigungsendpunkts</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="https://github.com/login/oauth/access_token" name="tokenEndpoint" type="xsd:string" use="optional">
