        <xsd:attribute name="value" type="xsd:string">
            <xsd:annotation>
                <xsd:documentation>Der Wert, der der Variablen zugeordnet werden soll.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Wert</ext:label>
