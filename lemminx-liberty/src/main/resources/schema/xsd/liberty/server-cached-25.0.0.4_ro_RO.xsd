                                    <xsd:documentation>ExpressNonPersistent</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="ReliableNonPersistent">
                                <xsd:annotation>
                                    <xsd:documentation>ReliableNonPersistent</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="ReliablePersistent">
                                <xsd:annotation>
