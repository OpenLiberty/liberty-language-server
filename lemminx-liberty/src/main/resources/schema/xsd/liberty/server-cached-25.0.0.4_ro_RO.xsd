                                    <xsd:documentation>ReadOnly</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="ReadWrite">
                                <xsd:annotation>
                                    <xsd:documentation>ReadWrite</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute name="applicationName" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>Proprietate driver JDBC: applicationName.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Nume aplicaţie</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="authentication" use="optional">
            <xsd:annotation>
                <xsd:documentation>Proprietate driver JDBC: authentication.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Autentificare</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union memberTypes="variableType">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="ActiveDirectoryDefault">
                                <xsd:annotation>
                                    <xsd:documentation>ActiveDirectoryDefault</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="ActiveDirectoryMSI">
                                <xsd:annotation>
                                    <xsd:documentation>ActiveDirectoryMSI</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="ActiveDirectoryIntegrated">
                                <xsd:annotation>
                                    <xsd:documentation>ActiveDirectoryIntegrated</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="ActiveDirectoryPassword">
                                <xsd:annotation>
                                    <xsd:documentation>ActiveDirectoryPassword</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="SqlPassword">
                                <xsd:annotation>
                                    <xsd:documentation>SqlPassword</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="NotSpecified">
                                <xsd:annotation>
                                    <xsd:documentation>NotSpecified</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute name="authenticationScheme" use="optional">
            <xsd:annotation>
                <xsd:documentation>Proprietate driver JDBC: authenticationScheme.</xsd:documentation>
