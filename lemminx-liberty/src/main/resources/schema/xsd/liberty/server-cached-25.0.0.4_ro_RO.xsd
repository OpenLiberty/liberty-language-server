                <ext:groupDecl id="Advanced" label="Proprietăţi avansate">Proprietăţi suplimentare pentru o utilizare mai avansată.</ext:groupDecl>
            </xsd:appinfo>
        </xsd:annotation>
        <!--Idiom for "empty element context" (whitespace accepted and ignored)-->
        <xsd:sequence>
            <xsd:sequence/>
        </xsd:sequence>
        <xsd:attribute name="databaseName" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>Proprietate driver JDBC: databaseName. Această proprietate suprascrie databaseName specificate în URL-ul în cazul în care unul a fost configurat.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Nume bază de date</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="instanceName" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>Proprietate driver JDBC: instanceName. Această proprietate suprascrie instanceName specificat în URL-ul în cazul în care unul a fost configurat.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Nume instanţă</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="serverName" type="xsd:string" use="optional">
