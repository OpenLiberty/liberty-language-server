                    <xsd:documentation>Proprietăţile pentru intrarea de referinţă.</xsd:documentation>
                    <xsd:appinfo>
                        <ext:label>Proprietăţi</ext:label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
        </xsd:choice>
        <xsd:attribute name="jndiName" type="xsd:string" use="required">
            <xsd:annotation>
                <xsd:documentation>Numele JNDI pentru intrarea de referinţă.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Nume JNDI</ext:label>
                    <ext:unique>jndiName</ext:unique>
                </xsd:appinfo>
