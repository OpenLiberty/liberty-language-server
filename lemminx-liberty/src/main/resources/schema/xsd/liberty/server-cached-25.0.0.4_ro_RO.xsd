                <ext:groupDecl id="Advanced" label="Proprietăţi avansate">Proprietăţi suplimentare pentru o utilizare mai avansată.</ext:groupDecl>
            </xsd:appinfo>
        </xsd:annotation>
        <!--Idiom for "empty element context" (whitespace accepted and ignored)-->
        <xsd:sequence>
            <xsd:sequence/>
        </xsd:sequence>
        <xsd:attribute name="createDatabase" use="optional">
            <xsd:annotation>
                <xsd:documentation>Proprietate driver JDBC: createDatabase.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Creare bază de date</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union memberTypes="variableType">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="create">
                                <xsd:annotation>
                                    <xsd:documentation>Când prima conexiune este stabilită, creează automat baza de date dacă ea nu există.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="false">
                                <xsd:annotation>
                                    <xsd:documentation>Nu creează automat baza de date.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute name="databaseName" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>Proprietate driver JDBC: databaseName. Această proprietate este ignorată de către șofer în cazul în care un URL este configurat.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Nume bază de date</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="localhost" name="serverName" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>JDBC proprietate șofer: serverName Această proprietate este ignorată de către șofer în cazul în care un URL este configurat.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Nume server</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="1527" name="portNumber" type="intType" use="optional">
            <xsd:annotation>
                <xsd:documentation>JDBC proprietate șofer: portNumber Această proprietate este ignorată de către șofer în cazul în care un URL este configurat.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Număr port</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="connectionAttributes" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>Proprietate driver JDBC: connectionAttributes.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Atribute de conexiune</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="loginTimeout" type="secondDuration" use="optional">
            <xsd:annotation>
                <xsd:documentation>Proprietate driver JDBC: loginTimeout. Specificaţi un întreg pozitiv urmat de o unitate de timp, care poate fi ore (o), minute (m) sau secunde (s). De exemplu, specificaţi 30 secunde ca 30s. Puteţi include valori multiple într-o singură intrare. De exemplu, 1m30s este echivalent cu 90 secunde.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Timeout de logare</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="password" type="password" use="optional">
            <xsd:annotation>
                <xsd:documentation>Se recomandă să folosiţi un alias de autentificare gestionat de container în loc să configuraţi această proprietate.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Parolă</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="true" name="retrieveMessageText" type="booleanType" use="optional">
