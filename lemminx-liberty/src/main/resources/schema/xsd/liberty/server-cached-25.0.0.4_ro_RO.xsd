    <xsd:complexType name="com.ibm.ws.concurrent.managedThreadFactory">
        <xsd:annotation>
            <xsd:documentation>Fabrică fire de execuţie gestionate</xsd:documentation>
            <xsd:appinfo>
                <ext:label>Fabrică fire de execuţie gestionate</ext:label>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element maxOccurs="1" minOccurs="0" name="contextService" type="com.ibm.ws.context.service">
                <xsd:annotation>
                    <xsd:documentation>Configurează modul în care este propagat contextul la firele de execuţie</xsd:documentation>
                    <xsd:appinfo>
                        <ext:label>Propagare context fir de execuţie</ext:label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
        <xsd:attribute default="DefaultContextService" name="contextServiceRef" type="pidType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Configurează modul în care este propagat contextul la firele de execuţie</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Referinţă propagare context</ext:label>
                    <ext:reference>contextService</ext:reference>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="false" name="createDaemonThreads" type="booleanType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Configurează dacă firele de execuţie create de fabrica de fire de execuţie gestionate ar trebui sau nu să fie fire de execuţie demon.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Creare fire de execuţie demon</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="defaultPriority" use="optional">
            <xsd:annotation>
                <xsd:documentation>Prioritatea implicită pentru firele de execuţie create de fabrica de fire de execuţie gestionate. Dacă nu se specifică, se foloseşte prioritatea firului de execuţie de creaţie. Prioritatea nu poate depăşi prioritatea maximă pentru fabrica de fire de execuţie gestionate, caz în care se foloseşte în schimba prioritatea maximă.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Prioritate implicită fir de execuţie</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union memberTypes="variableType">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:int">
                            <xsd:minInclusive value="1"/>
                            <xsd:maxInclusive value="10"/>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute name="jndiName" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>Nume JNDI</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Nume JNDI</ext:label>
                    <ext:unique>jndiName</ext:unique>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="maxPriority" use="optional">
            <xsd:annotation>
                <xsd:documentation>Prioritatea maximă pentru firele de execuţie create de fabrica de fire de execuţie gestionate.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Prioritate maximă fir de execuţie</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union memberTypes="variableType">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:int">
                            <xsd:minInclusive value="1"/>
                            <xsd:maxInclusive value="10"/>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.concurrent.managedThreadFactory-factory">
