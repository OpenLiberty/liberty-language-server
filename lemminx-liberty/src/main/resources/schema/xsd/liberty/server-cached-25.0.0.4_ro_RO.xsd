                <xsd:documentation>Proprietate driver JDBC: codare.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Codare</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="failoverPartner" type="xsd:string" use="optional">
