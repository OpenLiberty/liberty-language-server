    <xsd:complexType name="com.ibm.ws.persistence.databaseStore">
        <xsd:annotation>
            <xsd:documentation>Specifică o bază de date relaţională ca depozit persistent pentru caracteristicile de server.</xsd:documentation>
            <xsd:appinfo>
                <ext:label>Depozit bază de date</ext:label>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:choice maxOccurs="unbounded" minOccurs="0">
            <xsd:element name="dataSource" type="com.ibm.ws.jdbc.dataSource">
                <xsd:annotation>
                    <xsd:documentation>Sursa de date ce se conectează şa depozitul persistent.</xsd:documentation>
                    <xsd:appinfo>
                        <ext:label>Sursă de date</ext:label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="authData" type="com.ibm.ws.security.jca.internal.authdata.config">
                <xsd:annotation>
                    <xsd:documentation>Datele de autentificare pentru planificarea taskurilor, interogări şi execuţie.</xsd:documentation>
                    <xsd:appinfo>
                        <ext:label>Date autentificare</ext:label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
        </xsd:choice>
        <xsd:attribute default="true" name="createTables" type="booleanType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Când este setată la true, sunt create tabele de bază de date.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Creare tabele de bază de date</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="DefaultDataSource" name="dataSourceRef" type="pidType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Sursa de date ce se conectează şa depozitul persistent.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Referinţă sursă de date</ext:label>
                    <ext:reference>dataSource</ext:reference>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="AUTO" name="keyGenerationStrategy" use="optional">
            <xsd:annotation>
                <xsd:documentation>Strategia preferată pentru generarea cheilor primare unice. Dacă strategia selectată nu este suportată pentru baza de date, poate fi folosită o strategie diferită.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Strategia de generare chei</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union memberTypes="variableType">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="AUTO">
                                <xsd:annotation>
                                    <xsd:documentation>Selectaţi automat strategia pentru generarea cheilor primare unice. </xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="IDENTITY">
                                <xsd:annotation>
                                    <xsd:documentation>Utilizaţi o coloană de identitate a bazei de date pentru a genera chei primare unice.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="SEQUENCE">
                                <xsd:annotation>
                                    <xsd:documentation>Utilizaţi o secvenţă de bază de date pentru a genera chei primare unice.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="TABLE">
                                <xsd:annotation>
                                    <xsd:documentation>Utilizaţi o tabelă de bază de date pentru a genera chei primare unice.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute default="WLP" name="tablePrefix" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>Prefix nume pentru tabele, secvenţe şi alte artefacte de bază de date.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Prefix nume tabel</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="authDataRef" type="pidType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Datele de autentificare pentru planificarea taskurilor, interogări şi execuţie.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Referinţă date autentificare</ext:label>
                    <ext:reference>authData</ext:reference>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="schema" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>Numele de schemă cu acces de citire şi scriere la tabelele de bază de date.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Nume schemă</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.persistence.databaseStore-factory">
        <xsd:complexContent>
            <xsd:extension base="com.ibm.ws.persistence.databaseStore">
                <xsd:attribute name="id" type="factoryIdType" use="optional">
