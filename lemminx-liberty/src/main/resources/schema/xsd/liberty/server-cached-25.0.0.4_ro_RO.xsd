<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:ext="http://www.ibm.com/xmlns/dde/schema/annotation/ext">
    <xsd:complexType name="com.ibm.ws.app.manager.webappcfg">
        <xsd:annotation>
            <xsd:documentation>Defineşte proprietăţile unei aplicaţii web.</xsd:documentation>
            <xsd:appinfo>
                <ext:label>Aplicaţie web</ext:label>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:choice maxOccurs="unbounded" minOccurs="0">
            <xsd:element name="startAfter" type="com.ibm.ws.app.manager-factory">
                <xsd:annotation>
                    <xsd:documentation>Specifică aplicaţiile care necesită să fie pornite înainte ca această aplicaţie să poată porni. </xsd:documentation>
                    <xsd:appinfo>
                        <ext:label>Pornire după</ext:label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="classloader" type="com.ibm.ws.classloading.classloader">
                <xsd:annotation>
                    <xsd:documentation>Defineşte setările pentru un încărcător de clase de aplicaţie.</xsd:documentation>
                    <xsd:appinfo>
                        <ext:label>Încărcător de clasă</ext:label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="web-bnd" type="com.ibm.ws.javaee.dd.webbnd.WebBnd"/>
            <xsd:element name="web-ext" type="com.ibm.ws.javaee.dd.webext.WebExt"/>
            <xsd:element name="managed-bean-bnd" type="com.ibm.ws.javaee.dd.managedbean.ManagedBeanBnd"/>
            <xsd:element name="application-client-bnd" type="com.ibm.ws.javaee.dd.clientbnd.ApplicationClientBnd"/>
            <xsd:element name="ejb-jar-ext" type="com.ibm.ws.javaee.dd.ejbext.EJBJarExt"/>
            <xsd:element name="application-ext" type="com.ibm.ws.javaee.dd.appext.ApplicationExt"/>
            <xsd:element name="webservices-bnd" type="com.ibm.ws.javaee.ddmodel.wsbnd.WebservicesBnd"/>
            <xsd:element name="appProperties" type="com.ibm.ws.appconfig.appProperties"/>
            <xsd:element name="ejb-jar-bnd" type="com.ibm.ws.javaee.dd.ejbbnd.EJBJarBnd"/>
            <xsd:element name="application-bnd" type="com.ibm.ws.javaee.dd.appbnd.ApplicationBnd"/>
        </xsd:choice>
        <xsd:attribute name="location" type="location" use="required">
            <xsd:annotation>
                <xsd:documentation>Locaţia unei aplicaţii exprimată ca o cale absolută sau ca o cale relativă la un director apps la nivel de server.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Locaţie</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="name" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>Numele unei aplicaţii.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Nume</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="true" name="autoStart" type="booleanType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Indică dacă serverul porneşte sau nu automat aplicaţia.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Pornirea automată</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="useJandex" type="booleanType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Permite utilizarea fişierelor de indexuri Jandex dacă acestea sunt furnizate în aplicaţie</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Utilizare Jandex</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="startAfterRef" type="pidListType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Specifică aplicaţiile care necesită să fie pornite înainte ca această aplicaţie să poată porni. </xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Pornire după</ext:label>
                    <ext:reference>application</ext:reference>
                    <ext:reference>webApplication</ext:reference>
                    <ext:reference>enterpriseApplication</ext:reference>
                    <ext:reference>ejbApplication</ext:reference>
                    <ext:reference>resourceAdapter</ext:reference>
                    <ext:reference>springBootApplication</ext:reference>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="classloaderRef" type="pidType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Defineşte setările pentru un încărcător de clase de aplicaţie.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Încărcător de clasă</ext:label>
                    <ext:reference>classloader</ext:reference>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="contextRoot" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>Rădăcina context a unei aplicaţii.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Rădăcină context aplicaţie</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.app.manager.webappcfg-factory">
        <xsd:complexContent>
            <xsd:extension base="com.ibm.ws.app.manager.webappcfg">
                <xsd:attribute name="id" type="factoryIdType" use="optional">
                    <xsd:annotation>
                        <xsd:documentation>Un ID de configuraţie unic.</xsd:documentation>
                        <xsd:appinfo>
                            <ext:label>ID</ext:label>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.app.manager.earappcfg">
        <xsd:annotation>
            <xsd:documentation>Defineşte proprietăţile unei aplicaţii de întreprindere.</xsd:documentation>
            <xsd:appinfo>
                <ext:label>Enterprise Application</ext:label>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:choice maxOccurs="unbounded" minOccurs="0">
            <xsd:element name="startAfter" type="com.ibm.ws.app.manager-factory">
                <xsd:annotation>
                    <xsd:documentation>Specifică aplicaţiile care necesită să fie pornite înainte ca această aplicaţie să poată porni. </xsd:documentation>
                    <xsd:appinfo>
                        <ext:label>Pornire după</ext:label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="classloader" type="com.ibm.ws.classloading.classloader">
                <xsd:annotation>
                    <xsd:documentation>Defineşte setările pentru un încărcător de clase de aplicaţie.</xsd:documentation>
                    <xsd:appinfo>
                        <ext:label>Încărcător de clasă</ext:label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="resourceAdapter" type="com.ibm.ws.jca.embeddedResourceAdapter"/>
            <xsd:element name="web-bnd" type="com.ibm.ws.javaee.dd.webbnd.WebBnd"/>
            <xsd:element name="web-ext" type="com.ibm.ws.javaee.dd.webext.WebExt"/>
            <xsd:element name="managed-bean-bnd" type="com.ibm.ws.javaee.dd.managedbean.ManagedBeanBnd"/>
            <xsd:element name="application-client-bnd" type="com.ibm.ws.javaee.dd.clientbnd.ApplicationClientBnd"/>
            <xsd:element name="ejb-jar-ext" type="com.ibm.ws.javaee.dd.ejbext.EJBJarExt"/>
            <xsd:element name="application-ext" type="com.ibm.ws.javaee.dd.appext.ApplicationExt"/>
            <xsd:element name="webservices-bnd" type="com.ibm.ws.javaee.ddmodel.wsbnd.WebservicesBnd"/>
            <xsd:element name="appProperties" type="com.ibm.ws.appconfig.appProperties"/>
            <xsd:element name="ejb-jar-bnd" type="com.ibm.ws.javaee.dd.ejbbnd.EJBJarBnd"/>
            <xsd:element name="application-bnd" type="com.ibm.ws.javaee.dd.appbnd.ApplicationBnd"/>
        </xsd:choice>
        <xsd:attribute name="location" type="location" use="required">
            <xsd:annotation>
                <xsd:documentation>Locaţia unei aplicaţii exprimată ca o cale absolută sau ca o cale relativă la un director apps la nivel de server.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Locaţie</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="name" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>Numele unei aplicaţii.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Nume</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="true" name="autoStart" type="booleanType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Indică dacă serverul porneşte sau nu automat aplicaţia.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Pornirea automată</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="useJandex" type="booleanType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Permite utilizarea fişierelor de indexuri Jandex dacă acestea sunt furnizate în aplicaţie</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Utilizare Jandex</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="startAfterRef" type="pidListType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Specifică aplicaţiile care necesită să fie pornite înainte ca această aplicaţie să poată porni. </xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Pornire după</ext:label>
                    <ext:reference>application</ext:reference>
                    <ext:reference>webApplication</ext:reference>
                    <ext:reference>enterpriseApplication</ext:reference>
                    <ext:reference>ejbApplication</ext:reference>
                    <ext:reference>resourceAdapter</ext:reference>
                    <ext:reference>springBootApplication</ext:reference>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="classloaderRef" type="pidType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Defineşte setările pentru un încărcător de clase de aplicaţie.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Încărcător de clasă</ext:label>
                    <ext:reference>classloader</ext:reference>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="defaultClientModule" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>Modulul de client implicit al unei aplicaţii de întreprindere ,</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Modul client implicit</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="webModuleClassPathLoader" use="optional">
            <xsd:annotation>
                <xsd:documentation>Acest atribut definește încărcătorul de clasă care este utilizat pentru JAR-urile la care se face referire de antetele aplicației web Class-Path.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Încărcător de cale de clasă pentru modulul web</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union memberTypes="variableType">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="war">
                                <xsd:annotation>
                                    <xsd:documentation>Utilizați încărcătorul de clasă a aplicației web pentru JAR-urile la care se referă un antet Class-Path de aplicație web.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="ear">
                                <xsd:annotation>
                                    <xsd:documentation>Utilizați încărcătorul clasei de aplicații pentru întreprinderile părinte pentru JAR-urile la care se face referire de antetul Class-Path de aplicație web.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.app.manager.earappcfg-factory">
        <xsd:complexContent>
            <xsd:extension base="com.ibm.ws.app.manager.earappcfg">
                <xsd:attribute name="id" type="factoryIdType" use="optional">
                    <xsd:annotation>
                        <xsd:documentation>Un ID de configuraţie unic.</xsd:documentation>
                        <xsd:appinfo>
                            <ext:label>ID</ext:label>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.security.authentication.filter.remoteAddress">
        <xsd:annotation>
            <xsd:documentation>Specifică adresa TCP/IP de gazdă de la distanță a aplicației client care a trimis cererea HTTP, de exemplu, remoteAddress id="sample" ip="100.100.100.99" matchType="greaterThan".</xsd:documentation>
            <xsd:appinfo>
                <ext:label>Adresă la distanţă</ext:label>
            </xsd:appinfo>
        </xsd:annotation>
        <!--Idiom for "empty element context" (whitespace accepted and ignored)-->
        <xsd:sequence>
            <xsd:sequence/>
        </xsd:sequence>
        <xsd:attribute default="contains" name="matchType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Specifică tipul de potrivire.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Tip potrivire</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union memberTypes="variableType">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="equals">
                                <xsd:annotation>
                                    <xsd:documentation>Egal</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="contains">
                                <xsd:annotation>
                                    <xsd:documentation>Conţine</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="notContain">
                                <xsd:annotation>
                                    <xsd:documentation>Nu conţine</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="greaterThan">
                                <xsd:annotation>
                                    <xsd:documentation>Mai mare decât</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="lessThan">
                                <xsd:annotation>
                                    <xsd:documentation>Mai mic decât</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute name="ip" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>Specifică adresa TCP/IP a gazdei la distanţă. </xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Adresă IP</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.security.authentication.filter.remoteAddress-factory">
        <xsd:complexContent>
            <xsd:extension base="com.ibm.ws.security.authentication.filter.remoteAddress">
                <xsd:attribute name="id" type="factoryIdType" use="optional">
                    <xsd:annotation>
                        <xsd:documentation>Un ID de configuraţie unic.</xsd:documentation>
                        <xsd:appinfo>
                            <ext:label>ID</ext:label>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.security.authentication.filter.requestUrl">
        <xsd:annotation>
            <xsd:documentation>Specifică URL-ul de cerere HTTP, de exemplu, requestUrl id="sample" urlPattern="/account/checking|/account/saving" matchType="contains".</xsd:documentation>
            <xsd:appinfo>
                <ext:label>URL cerere</ext:label>
            </xsd:appinfo>
        </xsd:annotation>
        <!--Idiom for "empty element context" (whitespace accepted and ignored)-->
        <xsd:sequence>
            <xsd:sequence/>
        </xsd:sequence>
        <xsd:attribute name="urlPattern" type="xsd:string" use="required">
            <xsd:annotation>
                <xsd:documentation>Specifică tiparul URL-ului. Caracterul * nu este suportat pentru folosirea ca metacaracter.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Tipar URL</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="contains" name="matchType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Specifică tipul de potrivire.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Tip potrivire</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union memberTypes="variableType">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="equals">
                                <xsd:annotation>
                                    <xsd:documentation>Egal</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="contains">
                                <xsd:annotation>
                                    <xsd:documentation>Conţine</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="notContain">
                                <xsd:annotation>
                                    <xsd:documentation>Nu conţine</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.security.authentication.filter.requestUrl-factory">
        <xsd:complexContent>
            <xsd:extension base="com.ibm.ws.security.authentication.filter.requestUrl">
                <xsd:attribute name="id" type="factoryIdType" use="optional">
                    <xsd:annotation>
                        <xsd:documentation>Un ID de configuraţie unic.</xsd:documentation>
                        <xsd:appinfo>
                            <ext:label>ID</ext:label>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.security.authentication.filter.requestHeader">
        <xsd:annotation>
            <xsd:documentation>Specifică numele atributului pentru antetul de cerere HTTP, de exemplu, requestHeader id="sample" name="email" value="Kevin@yahoo.com|Steven@gmail.com" matchType="contains".</xsd:documentation>
            <xsd:appinfo>
                <ext:label>Antet cerere</ext:label>
            </xsd:appinfo>
        </xsd:annotation>
        <!--Idiom for "empty element context" (whitespace accepted and ignored)-->
        <xsd:sequence>
            <xsd:sequence/>
        </xsd:sequence>
        <xsd:attribute name="name" type="xsd:string" use="required">
            <xsd:annotation>
                <xsd:documentation>Specifică numele.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Nume</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="value" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>Atributul de valoare specifică valoarea antetului cererii. Dacă valoarea nu este specificată, este utilizat atributul de nume pentru potrivire, de exemplu, requestHeader id="sample" name="email" matchType="contains".</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Valoare</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="contains" name="matchType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Specifică tipul de potrivire.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Tip potrivire</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union memberTypes="variableType">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="equals">
                                <xsd:annotation>
                                    <xsd:documentation>Egal</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="contains">
                                <xsd:annotation>
                                    <xsd:documentation>Conţine</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="notContain">
                                <xsd:annotation>
                                    <xsd:documentation>Nu conţine</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.security.authentication.filter.requestHeader-factory">
        <xsd:complexContent>
            <xsd:extension base="com.ibm.ws.security.authentication.filter.requestHeader">
                <xsd:attribute name="id" type="factoryIdType" use="optional">
                    <xsd:annotation>
