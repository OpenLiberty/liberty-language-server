                    <ext:label>Alias al depozitului de chei</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="org.apache.ws.security.crypto.merlin.keystore.password" type="password" use="optional">
