    <xsd:complexType name="com.ibm.ws.couchdb.couchdb">
        <xsd:annotation>
            <xsd:documentation>Configuraţie pentru un Conector CouchDB.</xsd:documentation>
            <xsd:appinfo>
                <ext:label>CouchDB</ext:label>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element maxOccurs="1" minOccurs="0" name="library" type="com.ibm.ws.classloading.sharedlibrary">
                <xsd:annotation>
                    <xsd:documentation>Specifică o bibliotecă ce conţine biblioteca CouchDB Client şi dependenţele sale.</xsd:documentation>
                    <xsd:appinfo>
                        <ext:label>Bibliotecă CouchDB Client Java Driver</ext:label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="libraryRef" type="pidType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Specifică o bibliotecă ce conţine biblioteca CouchDB Client şi dependenţele sale.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Referinţă bibliotecă CouchDB client java driver</ext:label>
                    <ext:reference>library</ext:reference>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="username" type="xsd:string" use="required">
            <xsd:annotation>
                <xsd:documentation>ID-ul de utilizator CouchDB utilizat pentru înregistrare la intrare şi accesarea bazelor dumneavoastră de date.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>ID utilizator CouchDB</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="password" type="password" use="required">
            <xsd:annotation>
                <xsd:documentation>Parolă ce corespunde ID-ului dumneavoastră de utilizator Couchdb.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Parolă</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="jndiName" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>Nume JNDI pentru o instanţă CouchDB.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Nume Jndi</ext:label>
                    <ext:unique>jndiName</ext:unique>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="url" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>URL pentru serverul CouchDB, ce include gazda şi portul.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>URL</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="host" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>Nume gazdă pentru serverul CouchDB.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Gazdă</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="5984" name="port" type="intType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Număr port pentru serverul CouchDB.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Port</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="20" name="maxConnections" type="intType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Numărul maxim de conexiuni concurente către hazdă.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Conexiuni maxime</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="30s" name="connectionTimeout" type="duration" use="optional">
            <xsd:annotation>
                <xsd:documentation>Timeout-ul pentru stabilirea unei conexiuni. Specificaţi un întreg pozitiv urmat de o unitate de timp, care poate fi ore (o), minute (m), secunde (s) sau milisecunde (ms). De exemplu, specificaţi 500 milisecunde ca 500ms. Puteţi include valori multiple într-o singură intrare. De exemplu, 1s500ms este echivalent cu 1.5 secunde.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Timeout de conexiune</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="1m" name="socketTimeout" type="duration" use="optional">
            <xsd:annotation>
                <xsd:documentation>Durata de aşteptate pentru un răspuns. Specificaţi un întreg pozitiv urmat de o unitate de timp, care poate fi ore (o), minute (m), secunde (s) sau milisecunde (ms). De exemplu, specificaţi 500 milisecunde ca 500ms. Puteţi include valori multiple într-o singură intrare. De exemplu, 1s500ms este echivalent cu 1.5 secunde.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Timeout socket</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="false" name="enableSSL" type="booleanType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Activează SSL la conectarea la CouchDB.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Activare SSL</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="false" name="relaxedSSLSettings" type="booleanType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Activează setările SSL ce permit managerului de încredere să accepte orice gazdă şi certificat.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Setare ssl relaxat</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="true" name="caching" type="booleanType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Permite încărcarea documentelor din cache-ul clientului dacă există şi revizia nu s-a modificat de la ultima accesare.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Memorare în cache</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="1000" name="maxCacheEntries" use="optional">
            <xsd:annotation>
                <xsd:documentation>Numărul maxim de intrări în cache pentru clientul http.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Număr maxim de intrări în cache</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union memberTypes="variableType">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:int">
                            <xsd:minInclusive value="0"/>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute default="8192" name="maxObjectSizeBytes" use="optional">
            <xsd:annotation>
                <xsd:documentation>Setează dimensiunea maximă pentru un document stocat.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Dimensiunea maximă a obiectului în octeţi</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union memberTypes="variableType">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:int">
                            <xsd:minInclusive value="0"/>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute default="true" name="useExpectContinue" type="booleanType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Utilizaţi antetul Aşteptare continuă în cerere atunci când trimiteţi cereri către CouchDB.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Utilizaţi aşteptare continuă</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="true" name="cleanupIdleConnections" type="booleanType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Închidere automată a conexiunilor atunci când sunt considerate nefolosite.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Curăţare conexiuni nefolosite</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.couchdb.couchdb-factory">
        <xsd:complexContent>
            <xsd:extension base="com.ibm.ws.couchdb.couchdb">
                <xsd:attribute name="id" type="factoryIdType" use="optional">
