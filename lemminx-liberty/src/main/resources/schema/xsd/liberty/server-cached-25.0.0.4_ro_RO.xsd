                <xsd:documentation>Permiteţi ca resursa inclusă să fie sărită dacă nu este găsită.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Resursa este opţională</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="location" type="location" use="required">
            <xsd:annotation>
                <xsd:documentation>Specifică locaţia resurselor. Valoarea poate fi o cale de fișier, un director, sau un URI pentru o resursă la distanță.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Locaţie</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="MERGE" name="onConflict" use="optional">
            <xsd:annotation>
                <xsd:documentation>Specifică comportamentul care este utilizat pentru a combina elemente când sunt găsite conflicte.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>În conflict</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union memberTypes="variableType">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="MERGE">
                                <xsd:annotation>
                                    <xsd:documentation>Elementele în conflict vor fi combinate împreună.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="REPLACE">
                                <xsd:annotation>
                                    <xsd:documentation>Când elementele intră în conflict, elementul din fişierul inclus va înlocui elementul conflictual.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="IGNORE">
                                <xsd:annotation>
                                    <xsd:documentation>Elementele în conflict din fişierul inclus vor fi ignorate.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
