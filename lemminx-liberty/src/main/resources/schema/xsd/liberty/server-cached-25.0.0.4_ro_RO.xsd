    <xsd:complexType name="com.ibm.ws.webcontainer.security.feature.authorization.specialsubject">
        <xsd:annotation>
            <xsd:documentation>Subiect special care are rolul de securitate.</xsd:documentation>
            <xsd:appinfo>
                <ext:label>Subiect special</ext:label>
            </xsd:appinfo>
        </xsd:annotation>
        <!--Idiom for "empty element context" (whitespace accepted and ignored)-->
        <xsd:sequence>
            <xsd:sequence/>
        </xsd:sequence>
        <xsd:attribute name="type" use="required">
            <xsd:annotation>
                <xsd:documentation>Unul dintre tipurile subiect special următoare: ALL_AUTHENTICATED_USERS, EVERYONE.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Tip subiect special</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union memberTypes="variableType">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="ALL_AUTHENTICATED_USERS">
                                <xsd:annotation>
                                    <xsd:documentation>Toţi utilizatorii autentificaţi</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="EVERYONE">
                                <xsd:annotation>
                                    <xsd:documentation>Toţi utilizatorii pentru toate cererile, chiar dacă cererea nu a fost autentificată</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.webcontainer.security.feature.authorization.specialsubject-factory">
