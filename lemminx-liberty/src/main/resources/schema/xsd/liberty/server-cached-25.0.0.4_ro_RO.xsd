            <xsd:annotation>
                <xsd:documentation>Nume gazdă server LDAP, ce poate fi ori o adresă IP ori un nume de serviciu al numelui de domeniu (DNS).</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Gazdă LDAP de preluare la defect</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="port" type="intType" use="required">
            <xsd:annotation>
                <xsd:documentation>Port server LDAP de preluare la defect.</xsd:documentation>
