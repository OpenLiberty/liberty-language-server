        <xsd:attribute name="ws-security.password" type="password" use="optional">
            <xsd:annotation>
                <xsd:documentation>Informaţiile parolei utilizatorului necesare pentru a crea Token nume utilizator.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Parolă</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ws-security.callback-handler" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>Clasa de implementare a handler-ului de apel invers al parolei.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Handler apel invers parolă</ext:label>
