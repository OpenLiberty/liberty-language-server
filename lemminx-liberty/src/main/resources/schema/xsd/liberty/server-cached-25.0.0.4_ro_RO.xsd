                <ext:label>Proprietăţi driver JDBC generic</ext:label>
                <ext:extraProperties/>
            </xsd:appinfo>
        </xsd:annotation>
        <!--Idiom for "empty element context" (whitespace accepted and ignored)-->
        <xsd:sequence>
            <xsd:sequence/>
        </xsd:sequence>
        <xsd:attribute name="databaseName" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>Proprietate driver JDBC: databaseName.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Nume bază de date</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="serverName" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>Serverul unde rulează baza de date.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Nume server</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="portNumber" type="intType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Port pe care să obţineţi conexiunile bazei de date.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Număr port</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="URL" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>URL pentru conectarea la baza de date.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>URL</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="user" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>Se recomandă să folosiţi un alias de autentificare gestionat de container în loc să configuraţi această proprietate.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Utilizator</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="password" type="password" use="optional">
            <xsd:annotation>
                <xsd:documentation>Se recomandă să folosiţi un alias de autentificare gestionat de container în loc să configuraţi această proprietate.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Parolă</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="internal.properties" type="internalPropertiesType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Proprietăţi suplimentare de vînzător pentru JDBC pentrusursa de date.</xsd:documentation>
