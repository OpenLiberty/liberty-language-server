                    <xsd:appinfo>
                        <ext:label>Proprietăţile de cache ale atributului</ext:label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="searchResultsCache" type="com.ibm.ws.security.wim.adapter.ldap.searchResultsCache">
                <xsd:annotation>
                    <xsd:documentation>Configuraţia pentru cache al rezultatelor de căutare.</xsd:documentation>
                    <xsd:appinfo>
                        <ext:label>Proprietăţile cache ale rezultatelor de căutare</ext:label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
        </xsd:choice>
