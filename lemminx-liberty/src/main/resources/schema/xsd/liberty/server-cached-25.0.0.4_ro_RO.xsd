    <xsd:complexType name="com.ibm.ws.jdbc.dataSource.properties.microsoft.sqlserver">
        <xsd:annotation>
            <xsd:documentation>Proprietăţi sursă de date pentru Microsoft SQL Server JDBC Driver.</xsd:documentation>
            <xsd:appinfo>
                <ext:label>Proprietăţi driver JDBC Microsoft SQL Server</ext:label>
