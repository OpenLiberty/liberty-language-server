                <xsd:appinfo>
                    <ext:label>Nume server</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="portNumber" type="intType" use="optional">
            <xsd:annotation>
                <xsd:documentation>JDBC proprietate șofer: portNumber Această proprietate suprascrie portNumber în URL-ul în cazul în care unul a fost configurat.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Număr port</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="accessToken" type="password" use="optional">
            <xsd:annotation>
                <xsd:documentation>Proprietate driver JDBC: accessToken.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Jeton de acces</ext:label>
