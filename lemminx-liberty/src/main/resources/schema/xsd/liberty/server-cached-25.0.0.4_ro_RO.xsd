                <xsd:attribute name="id" type="factoryIdType" use="optional">
                    <xsd:annotation>
                        <xsd:documentation>Un ID de configuraţie unic.</xsd:documentation>
                        <xsd:appinfo>
                            <ext:label>ID</ext:label>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.classloading.sharedlibrary.file">
        <xsd:annotation>
            <xsd:documentation>Adăugați un fișier JAR sau ZIP pentru a fi inclus în calea clasei pentru această bibliotecă</xsd:documentation>
            <xsd:appinfo>
                <ext:label>Fişier</ext:label>
            </xsd:appinfo>
        </xsd:annotation>
        <!--Idiom for "empty element context" (whitespace accepted and ignored)-->
        <xsd:sequence>
            <xsd:sequence/>
        </xsd:sequence>
        <xsd:attribute name="name" type="fileLocation" use="required">
            <xsd:annotation>
                <xsd:documentation>Calea către un fișier poate fi o cale complet calificată sau o cale relativă. Căile relative sunt relative la valoarea proprietății $ {server.config.dir}.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Nume</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.classloading.sharedlibrary.file-factory">
        <xsd:complexContent>
            <xsd:extension base="com.ibm.ws.classloading.sharedlibrary.file">
                <xsd:attribute name="id" type="factoryIdType" use="optional">
                    <xsd:annotation>
                        <xsd:documentation>Un ID de configuraţie unic.</xsd:documentation>
