        <xsd:attribute default="https://www.linkednin.com" name="website" type="tokenType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Adresa site-ului web.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Site web</ext:label>
                </xsd:appinfo>
