    <xsd:complexType name="com.ibm.ws.wssecurity.enc.config">
        <xsd:annotation>
            <xsd:documentation>Informaţii de configurare precum tipul depozitului de chei, parola depozitului de chei</xsd:documentation>
            <xsd:appinfo>
                <ext:label>Criptare WS-Security</ext:label>
                <ext:extraProperties/>
            </xsd:appinfo>
        </xsd:annotation>
        <!--Idiom for "empty element context" (whitespace accepted and ignored)-->
        <xsd:sequence>
            <xsd:sequence/>
        </xsd:sequence>
        <xsd:attribute name="org.apache.ws.security.crypto.merlin.keystore.type" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>JKS, JCEKS sau PKCS11</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Tip depozit de chei</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="org.apache.ws.security.crypto.merlin.keystore.alias" type="xsd:string" use="optional">
