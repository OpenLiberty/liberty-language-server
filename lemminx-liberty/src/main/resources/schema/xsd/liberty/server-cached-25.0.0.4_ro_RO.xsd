    <xsd:complexType name="com.ibm.ws.jdbc.dataSource.properties.db2.jcc">
        <xsd:annotation>
            <xsd:documentation>Proprietăţi sursă de date pentru IBM Data Server Driver pentru JDBC şi SQLJ pentru DB2.</xsd:documentation>
            <xsd:appinfo>
                <ext:label>Proprietăţi DB2 JCC</ext:label>
                <ext:extraProperties/>
                <ext:groupDecl id="Advanced" label="Proprietăţi avansate">Proprietăţi suplimentare pentru o utilizare mai avansată.</ext:groupDecl>
                <ext:groupDecl id="AutomaticClientReroute" label="Proprietăţi de rerutare automată clienţi">Proprietăţile pentru a activa şi configura caracteristica Rerutare automată clienţi pentru conexiunea driver-ului DB2 JDBC.</ext:groupDecl>
            </xsd:appinfo>
        </xsd:annotation>
        <!--Idiom for "empty element context" (whitespace accepted and ignored)-->
        <xsd:sequence>
            <xsd:sequence/>
        </xsd:sequence>
        <xsd:attribute default="4" name="driverType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Proprietate driver JDBC: driverType.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Tip driver</ext:label>
