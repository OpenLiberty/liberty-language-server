                                <xsd:annotation>
                                    <xsd:documentation>Tip driver 2 JDBC.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="4">
                                <xsd:annotation>
                                    <xsd:documentation>Tip driver 4 JDBC.</xsd:documentation>
