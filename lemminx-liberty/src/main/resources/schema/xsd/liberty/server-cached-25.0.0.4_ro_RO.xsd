    <xsd:complexType name="com.ibm.ws.wssecurity.caller.config">
        <xsd:annotation>
            <xsd:documentation>Configuraţia necesară pentru procesarea apelantului.</xsd:documentation>
            <xsd:appinfo>
                <ext:label>Apelant WebSphere</ext:label>
            </xsd:appinfo>
        </xsd:annotation>
        <!--Idiom for "empty element context" (whitespace accepted and ignored)-->
        <xsd:sequence>
            <xsd:sequence/>
        </xsd:sequence>
        <xsd:attribute name="name" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>Specificaţi numele de jeton. Opţiunile sunt Usernametoken, X509token, Samltoken.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Token de apelant</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="userIdentifier" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>Specifică un atribut SAML care este utilizat ca nume principal de utilizator în subiect. Valoarea implicită este aserţiunea NameID.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Identificator principal</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="groupIdentifier" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>Specifică un atribut SAML care este utilizat ca nume al grupului al cărui membru este principalul autentificat. Nu există nicio valoare implicită.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Identificator de grup</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="userUniqueIdentifier" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>Specifică un atribut SAML care este utilizat ca nume utilizator unic după cum se aplică la WSCredential în subiect. Valoarea implicită este aceeaşi cu valoarea atributului userIdentifier.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Identificator unic</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="realmIdentifier" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>Specifică un atribut SAML care este folosit ca numele regiunii. Valoarea implicită este emitent.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Identificator de regiune</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="true" name="includeTokenInSubject" type="booleanType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Specifică dacă se include în subiect o aserţiune SAML.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Includere revendicare SAML în subiect</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="No" name="mapToUserRegistry" use="optional">
            <xsd:annotation>
                <xsd:documentation>Specifică cum se mapează o identitate la un registru de utilizator. Opţiunile sunt Nu, Utilizator şi Grup. Valoarea implicită este Nu şi registrul de utilizator nu este utilizat pentru a crea subiect de utilizator.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Maparea unei identităţi SAML la un registru de utilizator</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union memberTypes="variableType">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="No">
                                <xsd:annotation>
                                    <xsd:documentation>Nu mapaţi o identitate SAML la un utilizator sau grup din registru</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="User">
                                <xsd:annotation>
                                    <xsd:documentation>Maparea unei identităţi SAML la un hartă defined definit în registru</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="Group">
                                <xsd:annotation>
                                    <xsd:documentation>Maparea unei identităţi SAML la un grup definit în registrul de utilizator</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute name="realmName" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>Specifică un nume de regiune când mapToUserRegistry este setată la Nu sau la Grup.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Nume regiune</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="true" name="allowCustomCacheKey" type="booleanType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Permiterea generării unei chei cache personalizate pentru a accesa cache-ul de autentificare şi a obţine subiectul.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Cheie cache personalizată</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.wssecurity.caller.config-factory">
        <xsd:complexContent>
            <xsd:extension base="com.ibm.ws.wssecurity.caller.config">
