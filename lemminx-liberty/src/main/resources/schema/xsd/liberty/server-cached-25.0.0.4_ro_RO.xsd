        <xsd:attribute name="columnEncryptionSetting" use="optional">
            <xsd:annotation>
                <xsd:documentation>Proprietate driver JDBC: columnEncryptionSetting.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Criptare coloană</ext:label>
