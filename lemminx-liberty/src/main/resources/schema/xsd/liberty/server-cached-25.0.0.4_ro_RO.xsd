            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="traceLevel" type="intType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Combinaţie la nivel de bit a valorilor constante următoare: TRACE_NONE=0, TRACE_CONNECTION_CALLS=1, TRACE_STATEMENT_CALLS=2, TRACE_RESULT_SET_CALLS=4, TRACE_DRIVER_CONFIGURATION=16, TRACE_CONNECTS=32, TRACE_DRDA_FLOWS=64, TRACE_RESULT_SET_META_DATA=128, TRACE_PARAMETER_META_DATA=256, TRACE_DIAGNOSTICS=512, TRACE_XA_CALLS=2048, TRACE_ALL=-1.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Nivel urmă</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="user" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>Se recomandă să folosiţi un alias de autentificare gestionat de container în loc să configuraţi această proprietate.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Utilizator</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
