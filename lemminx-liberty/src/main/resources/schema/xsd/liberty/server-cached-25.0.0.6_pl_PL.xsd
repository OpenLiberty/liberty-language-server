                <xsd:documentation>Domyślna wartość, która ma zostać przypisana do zmiennej, jeśli wartość nie jest dostępna.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Wartość domyślna</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
