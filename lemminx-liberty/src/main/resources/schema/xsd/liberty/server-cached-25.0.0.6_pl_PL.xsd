                                    <xsd:documentation>CLEAR_TEXT_PASSWORD_SECURITY</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="4">
                                <xsd:annotation>
                                    <xsd:documentation>USER_ONLY_SECURITY</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="7">
