    <xsd:complexType name="com.ibm.ws.security.registry.basic.config.user">
        <xsd:annotation>
            <xsd:documentation>Użytkownik w podstawowym rejestrze użytkowników.</xsd:documentation>
            <xsd:appinfo>
                <ext:label>Użytkownik</ext:label>
