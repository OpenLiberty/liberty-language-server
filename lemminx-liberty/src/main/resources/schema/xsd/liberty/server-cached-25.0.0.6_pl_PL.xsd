            <xsd:element name="interface" type="com.ibm.ws.javaee.dd.ejbbnd.Interface-factory">
                <xsd:annotation>
                    <xsd:documentation>Określa interfejs sesji.</xsd:documentation>
                    <xsd:appinfo>
                        <ext:label>Interfejs</ext:label>
                    </xsd:appinfo>
                </xsd:annotation>
