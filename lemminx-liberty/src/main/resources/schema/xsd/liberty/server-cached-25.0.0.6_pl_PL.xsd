                <xsd:appinfo>
                    <ext:label>Protokół składnicy dla obiektu javax.mail.Session</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="smtp" name="transportProtocol" type="xsd:string" use="optional">
