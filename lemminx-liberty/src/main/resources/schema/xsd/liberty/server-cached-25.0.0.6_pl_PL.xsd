                                    <xsd:documentation>Sterownik JDBC typu 2.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="4">
                                <xsd:annotation>
                                    <xsd:documentation>Sterownik JDBC typu 4.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute name="databaseName" type="xsd:string" use="optional">
            <xsd:annotation>
