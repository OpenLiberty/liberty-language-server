                    <ext:label>Mapa de ID do Membro do Grupo</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.security.registry.ldap.internal.filters.actived-factory">
