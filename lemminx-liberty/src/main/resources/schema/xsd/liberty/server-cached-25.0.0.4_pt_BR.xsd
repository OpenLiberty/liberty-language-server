    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.security.authentication.internal.jaas.jaasLoginContextEntry-factory">
        <xsd:complexContent>
            <xsd:extension base="com.ibm.ws.security.authentication.internal.jaas.jaasLoginContextEntry">
