                                <xsd:annotation>
                                    <xsd:documentation>Igual a</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="contains">
                                <xsd:annotation>
                                    <xsd:documentation>Contém</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
