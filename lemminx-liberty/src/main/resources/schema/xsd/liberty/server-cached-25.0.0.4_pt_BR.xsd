                <xsd:appinfo>
                    <ext:label>Nível de Rastreio</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="user" type="xsd:string" use="optional">
