            <xsd:appinfo>
                <ext:label>Entrada de Contexto de Login do JAAS</ext:label>
            </xsd:appinfo>
        </xsd:annotation>
        <!--Idiom for "empty element context" (whitespace accepted and ignored)-->
        <xsd:sequence>
