                        </xsd:annotation> 
                    </xsd:enumeration> 
                    <xsd:enumeration value="Group"> 
                        <xsd:annotation> 
                            <xsd:documentation>Mapper une identité SAML à un groupe défini dans le registre d'utilisateurs</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                </xsd:restriction> 
            </xsd:simpleType> 
        </xsd:attribute> 
