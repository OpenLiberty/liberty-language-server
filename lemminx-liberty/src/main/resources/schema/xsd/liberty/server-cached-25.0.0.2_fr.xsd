                    <ext:label>Nom d'hôte</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="port" use="optional" type="intType"> 
            <xsd:annotation> 
                <xsd:documentation>Port.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Port</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:anyAttribute processContents="skip"> 
        </xsd:anyAttribute> 
