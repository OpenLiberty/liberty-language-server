        <xsd:complexContent> 
            <xsd:extension base="com.ibm.ws.security.audit.event"> 
                <xsd:attribute name="id" type="factoryIdType" use="optional"> 
                    <xsd:annotation> 
                        <xsd:documentation>ID de configuration unique.</xsd:documentation> 
                        <xsd:appinfo> 
