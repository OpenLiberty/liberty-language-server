        <xsd:attribute name="profileName" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>JDBC 드라이버 특성: profileName입니다.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>프로파일 이름</ext:label>
                    <ext:group id="Advanced"/>
