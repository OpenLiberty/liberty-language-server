            <xsd:element name="folder" type="com.ibm.ws.classloading.sharedlibrary.folder-factory">
                <xsd:annotation>
                    <xsd:documentation>참조된 폴더의 ID</xsd:documentation>
                    <xsd:appinfo>
                        <ext:label>폴더</ext:label>
                    </xsd:appinfo>
