            <xsd:element name="path" type="com.ibm.ws.classloading.sharedlibrary.path-factory">
                <xsd:annotation>
                    <xsd:documentation>참조된 경로의 ID</xsd:documentation>
                    <xsd:appinfo>
                        <ext:label>경로</ext:label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
        </xsd:choice>
        <xsd:attribute name="name" type="xsd:string" use="optional">
