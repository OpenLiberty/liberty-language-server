                <xsd:documentation>데이터 소스의 추가 JDBC 벤더 특성입니다.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>추가 특성</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
