        <xsd:attribute name="sslTrustStoreType" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>JDBC 드라이버 특성: sslTrustStoreType입니다.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>SSL 신뢰 저장소 유형</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
