        <xsd:attribute name="progressiveStreaming" use="optional">
            <xsd:annotation>
                <xsd:documentation>JDBC 드라이버 특성: progressiveStreaming입니다. 값은 1(예) 또는 2(아니오)입니다.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>점진적 스트리밍</ext:label>
