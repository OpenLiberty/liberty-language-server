                <xsd:documentation>JDBC 드라이버 특성: traceFileAppend입니다.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>추적 파일 추가</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
