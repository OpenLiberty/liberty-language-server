            <xsd:annotation>
                <xsd:documentation>참조 항목에 대한 추가 특성입니다.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>추가 특성</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
