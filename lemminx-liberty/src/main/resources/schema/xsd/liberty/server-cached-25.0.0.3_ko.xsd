        <xsd:attribute name="libraryRef" type="pidType" use="optional">
            <xsd:annotation>
                <xsd:documentation>JDBC 드라이버 JAR 및 원시 파일을 식별합니다.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>공유 라이브러리 참조</ext:label>
                    <ext:reference>library</ext:reference>
                </xsd:appinfo>
