                <xsd:documentation>데이터베이스의 이름입니다.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>데이터베이스 이름</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="jndiName" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>JNDI 이름입니다.</xsd:documentation>
