                <xsd:appinfo>
                    <ext:label>사용자 정보 URL</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="r_emailaddress r_liteprofile" name="scope" type="xsd:string" use="optional">
