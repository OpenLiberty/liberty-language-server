                            <xsd:enumeration value="2">
                                <xsd:annotation>
                                    <xsd:documentation>2</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="3">
                                <xsd:annotation>
                                    <xsd:documentation>3</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
