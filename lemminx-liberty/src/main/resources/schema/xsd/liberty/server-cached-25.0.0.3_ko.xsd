        <xsd:attribute name="description" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>관리자를 위한 공유 라이브러리의 설명</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>설명</ext:label>
                </xsd:appinfo>
