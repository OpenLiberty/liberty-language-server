        <xsd:attribute name="traceFile" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>Свойство драйвера JDBC: traceFile.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Файл трассировки</ext:label>
