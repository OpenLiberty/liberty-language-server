                                <xsd:annotation>
                                    <xsd:documentation>Disabled</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="Enabled">
                                <xsd:annotation>
                                    <xsd:documentation>Enabled</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                        </xsd:restriction>
                    </xsd:simpleType>
