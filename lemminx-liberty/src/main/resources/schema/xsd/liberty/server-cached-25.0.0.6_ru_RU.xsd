        <xsd:attribute name="keyStoreAuthentication" use="optional">
            <xsd:annotation>
                <xsd:documentation>Свойство драйвера JDBC: keyStoreAuthentication.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Идентификация хранилища ключей</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union memberTypes="variableType">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="JavaKeyStorePassword">
                                <xsd:annotation>
                                    <xsd:documentation>JavaKeyStorePassword</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
