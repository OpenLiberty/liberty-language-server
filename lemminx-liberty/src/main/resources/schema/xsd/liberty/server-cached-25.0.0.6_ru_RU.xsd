<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:ext="http://www.ibm.com/xmlns/dde/schema/annotation/ext">
    <xsd:complexType name="com.ibm.ws.app.manager.webappcfg">
        <xsd:annotation>
            <xsd:documentation>Свойства веб-приложения.</xsd:documentation>
            <xsd:appinfo>
                <ext:label>Веб-приложение</ext:label>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:choice maxOccurs="unbounded" minOccurs="0">
            <xsd:element name="startAfter" type="com.ibm.ws.app.manager-factory">
                <xsd:annotation>
                    <xsd:documentation>Указывает приложения, которые должны быть запущены перед запуском данного приложения. </xsd:documentation>
                    <xsd:appinfo>
                        <ext:label>Запускать после</ext:label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="classloader" type="com.ibm.ws.classloading.classloader">
                <xsd:annotation>
                    <xsd:documentation>Задает параметры загрузчика классов приложения.</xsd:documentation>
                    <xsd:appinfo>
                        <ext:label>Загрузчик классов</ext:label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="webservices-bnd" type="com.ibm.ws.javaee.ddmodel.wsbnd.WebservicesBnd"/>
            <xsd:element name="application-client-bnd" type="com.ibm.ws.javaee.dd.clientbnd.ApplicationClientBnd"/>
            <xsd:element name="ejb-jar-bnd" type="com.ibm.ws.javaee.dd.ejbbnd.EJBJarBnd"/>
            <xsd:element name="web-bnd" type="com.ibm.ws.javaee.dd.webbnd.WebBnd"/>
            <xsd:element name="web-ext" type="com.ibm.ws.javaee.dd.webext.WebExt"/>
            <xsd:element name="appProperties" type="com.ibm.ws.appconfig.appProperties"/>
            <xsd:element name="ejb-jar-ext" type="com.ibm.ws.javaee.dd.ejbext.EJBJarExt"/>
            <xsd:element name="managed-bean-bnd" type="com.ibm.ws.javaee.dd.managedbean.ManagedBeanBnd"/>
            <xsd:element name="application-bnd" type="com.ibm.ws.javaee.dd.appbnd.ApplicationBnd"/>
            <xsd:element name="application-ext" type="com.ibm.ws.javaee.dd.appext.ApplicationExt"/>
        </xsd:choice>
        <xsd:attribute name="location" type="location" use="required">
            <xsd:annotation>
                <xsd:documentation>Расположение приложения, полный путь или путь относительно каталога приложений сервера.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Расположение</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="name" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>Имя приложения.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Имя</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="true" name="autoStart" type="booleanType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Указывает, должен сервер запускать приложение автоматически или нет.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Запускать автоматически</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="useJandex" type="booleanType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Включает использование индексных файлов Jandex при их наличии в приложении</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Использовать Jandex</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="startAfterRef" type="pidListType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Указывает приложения, которые должны быть запущены перед запуском данного приложения. </xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Запускать после</ext:label>
                    <ext:reference>application</ext:reference>
                    <ext:reference>webApplication</ext:reference>
                    <ext:reference>enterpriseApplication</ext:reference>
                    <ext:reference>ejbApplication</ext:reference>
                    <ext:reference>resourceAdapter</ext:reference>
                    <ext:reference>springBootApplication</ext:reference>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="classloaderRef" type="pidType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Задает параметры загрузчика классов приложения.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Загрузчик классов</ext:label>
                    <ext:reference>classloader</ext:reference>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="contextRoot" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>Корневой каталог приложения.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Корневой каталог приложения</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.app.manager.webappcfg-factory">
        <xsd:complexContent>
            <xsd:extension base="com.ibm.ws.app.manager.webappcfg">
                <xsd:attribute name="id" type="factoryIdType" use="optional">
                    <xsd:annotation>
                        <xsd:documentation>Уникальный ИД конфигурации.</xsd:documentation>
                        <xsd:appinfo>
                            <ext:label>ИД</ext:label>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.app.manager.earappcfg">
        <xsd:annotation>
            <xsd:documentation>Свойства приложения Java EE.</xsd:documentation>
            <xsd:appinfo>
                <ext:label>Приложение Java EE</ext:label>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:choice maxOccurs="unbounded" minOccurs="0">
            <xsd:element name="startAfter" type="com.ibm.ws.app.manager-factory">
                <xsd:annotation>
                    <xsd:documentation>Указывает приложения, которые должны быть запущены перед запуском данного приложения. </xsd:documentation>
                    <xsd:appinfo>
                        <ext:label>Запускать после</ext:label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="classloader" type="com.ibm.ws.classloading.classloader">
                <xsd:annotation>
                    <xsd:documentation>Задает параметры загрузчика классов приложения.</xsd:documentation>
                    <xsd:appinfo>
                        <ext:label>Загрузчик классов</ext:label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="webservices-bnd" type="com.ibm.ws.javaee.ddmodel.wsbnd.WebservicesBnd"/>
            <xsd:element name="application-client-bnd" type="com.ibm.ws.javaee.dd.clientbnd.ApplicationClientBnd"/>
            <xsd:element name="ejb-jar-bnd" type="com.ibm.ws.javaee.dd.ejbbnd.EJBJarBnd"/>
            <xsd:element name="resourceAdapter" type="com.ibm.ws.jca.embeddedResourceAdapter"/>
            <xsd:element name="web-bnd" type="com.ibm.ws.javaee.dd.webbnd.WebBnd"/>
            <xsd:element name="web-ext" type="com.ibm.ws.javaee.dd.webext.WebExt"/>
            <xsd:element name="appProperties" type="com.ibm.ws.appconfig.appProperties"/>
            <xsd:element name="ejb-jar-ext" type="com.ibm.ws.javaee.dd.ejbext.EJBJarExt"/>
            <xsd:element name="managed-bean-bnd" type="com.ibm.ws.javaee.dd.managedbean.ManagedBeanBnd"/>
            <xsd:element name="application-bnd" type="com.ibm.ws.javaee.dd.appbnd.ApplicationBnd"/>
            <xsd:element name="application-ext" type="com.ibm.ws.javaee.dd.appext.ApplicationExt"/>
        </xsd:choice>
        <xsd:attribute name="location" type="location" use="required">
            <xsd:annotation>
                <xsd:documentation>Расположение приложения, полный путь или путь относительно каталога приложений сервера.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Расположение</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="name" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>Имя приложения.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Имя</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="true" name="autoStart" type="booleanType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Указывает, должен сервер запускать приложение автоматически или нет.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Запускать автоматически</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="useJandex" type="booleanType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Включает использование индексных файлов Jandex при их наличии в приложении</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Использовать Jandex</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="startAfterRef" type="pidListType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Указывает приложения, которые должны быть запущены перед запуском данного приложения. </xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Запускать после</ext:label>
                    <ext:reference>application</ext:reference>
                    <ext:reference>webApplication</ext:reference>
                    <ext:reference>enterpriseApplication</ext:reference>
                    <ext:reference>ejbApplication</ext:reference>
                    <ext:reference>resourceAdapter</ext:reference>
                    <ext:reference>springBootApplication</ext:reference>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="classloaderRef" type="pidType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Задает параметры загрузчика классов приложения.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Загрузчик классов</ext:label>
                    <ext:reference>classloader</ext:reference>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="defaultClientModule" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>Модуль клиента по умолчанию для приложения Java EE.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Модуль клиента по умолчанию</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="webModuleClassPathLoader" use="optional">
            <xsd:annotation>
                <xsd:documentation>Этот атрибут определяет загрузчик классов, используемый для JAR-файлов, на которые ссылаются заголовки Class-Path веб-приложения.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Загрузчик путей классов веб-модуля</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union memberTypes="variableType">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="war">
                                <xsd:annotation>
                                    <xsd:documentation>Используйте загрузчик классов веб-приложения для JAR-файлов, на которые ссылается заголовок Class-Path веб-приложения.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="ear">
                                <xsd:annotation>
                                    <xsd:documentation>Используйте загрузчик классов родительского корпоративного приложения для JAR-файлов, на которые ссылается заголовок Class-Path веб-приложения.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute name="annotationScanLibrary" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>Настройте, какие библиотеки будут сканироваться на предмет аннотаций веб-приложений, в виде списка, разделенного запятыми. Возможные варианты: earLibraries, manifestClassPath, или все.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Библиотеки сканирования аннотаций</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.app.manager.earappcfg-factory">
        <xsd:complexContent>
            <xsd:extension base="com.ibm.ws.app.manager.earappcfg">
                <xsd:attribute name="id" type="factoryIdType" use="optional">
                    <xsd:annotation>
                        <xsd:documentation>Уникальный ИД конфигурации.</xsd:documentation>
                        <xsd:appinfo>
                            <ext:label>ИД</ext:label>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.security.authentication.filter.remoteAddress">
        <xsd:annotation>
            <xsd:documentation>Задает адрес TCP/IP удаленного хоста приложения-клиента, который отправил запрос HTTP, например remoteAddress id="sample "ip=" 100.100.100.99" matchType="greaterThan ".</xsd:documentation>
            <xsd:appinfo>
                <ext:label>Удаленный адрес</ext:label>
            </xsd:appinfo>
        </xsd:annotation>
        <!--Idiom for "empty element context" (whitespace accepted and ignored)-->
        <xsd:sequence>
            <xsd:sequence/>
        </xsd:sequence>
        <xsd:attribute default="contains" name="matchType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Задает тип сравнения.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Тип сравнения</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union memberTypes="variableType">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="equals">
                                <xsd:annotation>
                                    <xsd:documentation>Равно</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="contains">
                                <xsd:annotation>
                                    <xsd:documentation>Содержит</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="notContain">
                                <xsd:annotation>
                                    <xsd:documentation>Не содержит</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="greaterThan">
                                <xsd:annotation>
                                    <xsd:documentation>Больше, чем</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="lessThan">
                                <xsd:annotation>
                                    <xsd:documentation>Меньше, чем</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute name="ip" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>Задает адрес TCP/IP удаленного хоста. </xsd:documentation>
                <xsd:appinfo>
                    <ext:label>IP-адрес</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.security.authentication.filter.remoteAddress-factory">
        <xsd:complexContent>
            <xsd:extension base="com.ibm.ws.security.authentication.filter.remoteAddress">
                <xsd:attribute name="id" type="factoryIdType" use="optional">
                    <xsd:annotation>
                        <xsd:documentation>Уникальный ИД конфигурации.</xsd:documentation>
                        <xsd:appinfo>
                            <ext:label>ИД</ext:label>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.security.authentication.filter.requestUrl">
        <xsd:annotation>
            <xsd:documentation>Задает URL запроса HTTP, например requestUrl id="sample "urlPattern=" /account/checking| /account/saviett "matchType =" contains ".</xsd:documentation>
            <xsd:appinfo>
                <ext:label>URL запроса</ext:label>
            </xsd:appinfo>
        </xsd:annotation>
        <!--Idiom for "empty element context" (whitespace accepted and ignored)-->
        <xsd:sequence>
            <xsd:sequence/>
        </xsd:sequence>
        <xsd:attribute name="urlPattern" type="xsd:string" use="required">
            <xsd:annotation>
                <xsd:documentation>Задает шаблон URL. Использование символа * в качестве символа подстановки не поддерживается.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Шаблон URL</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="contains" name="matchType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Задает тип сравнения.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Тип сравнения</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union memberTypes="variableType">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="equals">
                                <xsd:annotation>
                                    <xsd:documentation>Равно</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="contains">
                                <xsd:annotation>
                                    <xsd:documentation>Содержит</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="notContain">
                                <xsd:annotation>
                                    <xsd:documentation>Не содержит</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.security.authentication.filter.requestUrl-factory">
        <xsd:complexContent>
            <xsd:extension base="com.ibm.ws.security.authentication.filter.requestUrl">
                <xsd:attribute name="id" type="factoryIdType" use="optional">
                    <xsd:annotation>
                        <xsd:documentation>Уникальный ИД конфигурации.</xsd:documentation>
                        <xsd:appinfo>
                            <ext:label>ИД</ext:label>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.security.authentication.filter.requestHeader">
        <xsd:annotation>
            <xsd:documentation>Задает имя атрибута заголовка запроса HTTP, например requestHeader id="sample "name = "email" value= "Kevin@yahoo.com|Steven@gmail.com" matchType = "contains".</xsd:documentation>
            <xsd:appinfo>
                <ext:label>Заголовок запроса</ext:label>
            </xsd:appinfo>
        </xsd:annotation>
        <!--Idiom for "empty element context" (whitespace accepted and ignored)-->
        <xsd:sequence>
            <xsd:sequence/>
        </xsd:sequence>
        <xsd:attribute name="name" type="xsd:string" use="required">
            <xsd:annotation>
                <xsd:documentation>Задает имя.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Имя</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="value" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>Атрибут значения задает значение заголовка запроса. Если значение не задано, то для сопоставления используется атрибут name, например requestHeader id="sample "name="email" matchType = "contains".</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Значение</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="contains" name="matchType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Задает тип сравнения.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Тип сравнения</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union memberTypes="variableType">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="equals">
                                <xsd:annotation>
                                    <xsd:documentation>Равно</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="contains">
                                <xsd:annotation>
                                    <xsd:documentation>Содержит</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="notContain">
                                <xsd:annotation>
                                    <xsd:documentation>Не содержит</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.security.authentication.filter.requestHeader-factory">
        <xsd:complexContent>
            <xsd:extension base="com.ibm.ws.security.authentication.filter.requestHeader">
                <xsd:attribute name="id" type="factoryIdType" use="optional">
                    <xsd:annotation>
                        <xsd:documentation>Уникальный ИД конфигурации.</xsd:documentation>
                        <xsd:appinfo>
                            <ext:label>ИД</ext:label>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.security.authentication.filter.userAgent">
        <xsd:annotation>
            <xsd:documentation>Задает пользовательский агент, например, userAgent id="sample "agent="Mozilla/5.0"matchType =" contains ".</xsd:documentation>
            <xsd:appinfo>
                <ext:label>Браузер</ext:label>
            </xsd:appinfo>
        </xsd:annotation>
        <!--Idiom for "empty element context" (whitespace accepted and ignored)-->
        <xsd:sequence>
            <xsd:sequence/>
        </xsd:sequence>
        <xsd:attribute name="agent" type="xsd:string" use="required">
            <xsd:annotation>
                <xsd:documentation>Задает агент пользователя браузера, позволяющий определить, какой браузер используется.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Браузер</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="contains" name="matchType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Задает тип сравнения.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Тип сравнения</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union memberTypes="variableType">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="equals">
                                <xsd:annotation>
                                    <xsd:documentation>Равно</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="contains">
                                <xsd:annotation>
                                    <xsd:documentation>Содержит</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="notContain">
                                <xsd:annotation>
