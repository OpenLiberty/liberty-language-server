        <xsd:attribute default="*:cn" name="groupIdMap" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>Фильтр LDAP, применяемый для преобразования имени группы в запись LDAP.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Карта ИД групп</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="nsRole:nsRole" name="groupMemberIdMap" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>Фильтр LDAP для связи имен пользователей и групп.</xsd:documentation>
