        <xsd:attribute name="encrypt" type="booleanType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Свойство драйвера JDBC: encrypt.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Шифрование</ext:label>
                    <ext:group id="Advanced"/>
