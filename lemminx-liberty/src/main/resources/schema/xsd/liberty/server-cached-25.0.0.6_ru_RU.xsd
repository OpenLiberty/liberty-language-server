                    <ext:label>Имя роли</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.javaee.ddmodel.common.SecurityRole-factory">
        <xsd:complexContent>
