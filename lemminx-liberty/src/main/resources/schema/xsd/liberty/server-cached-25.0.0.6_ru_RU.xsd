    <xsd:complexType name="com.ibm.ws.security.s4u2proxy.KerberosExtService">
        <xsd:annotation>
            <xsd:documentation>Управляет работой функции ограниченного делегирования. </xsd:documentation>
            <xsd:appinfo>
                <ext:label>Ограниченное делегирование</ext:label>
            </xsd:appinfo>
        </xsd:annotation>
        <!--Idiom for "empty element context" (whitespace accepted and ignored)-->
        <xsd:sequence>
            <xsd:sequence/>
        </xsd:sequence>
        <xsd:attribute default="false" name="s4U2selfEnabled" type="booleanType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Значение true или false указывает, включена ли функция s4U2self.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Включить s4U2self</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.javaee.metadata.context">
        <xsd:annotation>
            <xsd:documentation>Пространство имен компонента приложения, который передает контекстную задачу, доступную для данной задачи.</xsd:documentation>
            <xsd:appinfo>
                <ext:label>Контекст компонента приложения Java EE</ext:label>
            </xsd:appinfo>
        </xsd:annotation>
        <!--Idiom for "empty element context" (whitespace accepted and ignored)-->
        <xsd:sequence>
            <xsd:sequence/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.javaee.metadata.context-factory">
        <xsd:complexContent>
            <xsd:extension base="com.ibm.ws.javaee.metadata.context">
                <xsd:attribute name="id" type="factoryIdType" use="optional">
                    <xsd:annotation>
                        <xsd:documentation>Уникальный ИД конфигурации.</xsd:documentation>
                        <xsd:appinfo>
                            <ext:label>ИД</ext:label>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.couchdb.couchdb">
        <xsd:annotation>
            <xsd:documentation>Конфигурация CouchDB Connector.</xsd:documentation>
            <xsd:appinfo>
                <ext:label>CouchDB</ext:label>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element maxOccurs="1" minOccurs="0" name="library" type="com.ibm.ws.classloading.sharedlibrary">
                <xsd:annotation>
                    <xsd:documentation>Задает библиотеку, содержащую библиотеку клиента CouchDB и ее зависимости.</xsd:documentation>
                    <xsd:appinfo>
                        <ext:label>Библиотека драйвера Java CouchDB</ext:label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="libraryRef" type="pidType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Задает библиотеку, содержащую библиотеку клиента CouchDB и ее зависимости.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Ссылка на библиотеку драйвера Java клиента CouchDB</ext:label>
                    <ext:reference>library</ext:reference>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="username" type="xsd:string" use="required">
            <xsd:annotation>
                <xsd:documentation>ИД пользователя CouchDB для входа в базы данных и доступа к ним.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>ИД пользователя CouchDB</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="password" type="password" use="required">
            <xsd:annotation>
                <xsd:documentation>Пароль, соответствующий ИД пользователя Couchdb.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Пароль</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="jndiName" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>Имя JNDI экземпляра CouchDB.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Имя JNDI</ext:label>
                    <ext:unique>jndiName</ext:unique>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="url" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>URL-адрес сервера CouchDB, включающий хост и порт.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>URL</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="host" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>Имя хоста сервера CouchDB.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Хост</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="5984" name="port" type="intType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Номер порта сервера CouchDB.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Порт</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="20" name="maxConnections" type="intType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Максимальное число одновременных соединений с хостом.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Максимальное число соединений</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="30s" name="connectionTimeout" type="duration" use="optional">
            <xsd:annotation>
                <xsd:documentation>Тайм-аут установки соединения. Укажите положительное целое число, за которым следует единица времени: часы (h), минуты (m), секунды (s) или миллисекунды (ms). Например, укажите 500 миллисекунд как 500ms. Одна запись может включать несколько значений. Так, 1s500ms равно 1,5 сек.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Тайм-аут соединения</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="1m" name="socketTimeout" type="duration" use="optional">
            <xsd:annotation>
                <xsd:documentation>Время ожидания ответа. Укажите положительное целое число, за которым следует единица времени: часы (h), минуты (m), секунды (s) или миллисекунды (ms). Например, укажите 500 миллисекунд как 500ms. Одна запись может включать несколько значений. Так, 1s500ms равно 1,5 сек.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Тайм-аут сокета</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="false" name="enableSSL" type="booleanType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Включает SSL во время соединения с CouchDB.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Включить SSL</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="false" name="relaxedSSLSettings" type="booleanType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Включает нестрогие параметры SSL, которые позволяют администратору доверенных сертификатов принимать любые хосты и сертификаты.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Нестрогие параметры SSL</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="true" name="caching" type="booleanType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Позволяет загружать документы из кэша клиента http, если таковой имеется и не изменялся с момента последнего обращения.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Кэширование</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="1000" name="maxCacheEntries" use="optional">
            <xsd:annotation>
                <xsd:documentation>Максимальное число записей в кэше для клиента http.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Максимальное число записей в кэше</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union memberTypes="variableType">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:int">
                            <xsd:minInclusive value="0"/>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute default="8192" name="maxObjectSizeBytes" use="optional">
            <xsd:annotation>
                <xsd:documentation>Задает максимальный размер хранимого документа.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Максимальный размер объекта в байтах</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union memberTypes="variableType">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:int">
                            <xsd:minInclusive value="0"/>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute default="true" name="useExpectContinue" type="booleanType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Использовать заголовок Expect Continue в заголовках запросов к CouchDB.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Использовать expect continue</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="true" name="cleanupIdleConnections" type="booleanType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Автоматически закрывает подключения, когда они простаивают.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Очистить простаивающие подключения</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.couchdb.couchdb-factory">
        <xsd:complexContent>
            <xsd:extension base="com.ibm.ws.couchdb.couchdb">
                <xsd:attribute name="id" type="factoryIdType" use="optional">
                    <xsd:annotation>
                        <xsd:documentation>Уникальный ИД конфигурации.</xsd:documentation>
                        <xsd:appinfo>
                            <ext:label>ИД</ext:label>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.security.csiv2.transportLayer">
        <xsd:annotation>
            <xsd:documentation>Настройте способ выдачи кредита доверия клиенту.</xsd:documentation>
            <xsd:appinfo>
                <ext:label>Уровень Транспорт</ext:label>
            </xsd:appinfo>
        </xsd:annotation>
        <!--Idiom for "empty element context" (whitespace accepted and ignored)-->
        <xsd:sequence>
            <xsd:sequence/>
        </xsd:sequence>
        <xsd:attribute default="true" name="sslEnabled" type="booleanType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Значения true или false указывают, включен ли SSL для запросов CSIv2. Значение по умолчанию: true (это рекомендованное значение). Когда этому атрибуту присвоено значение false, конфиденциальная информация, такая как пароли и ключи, передается на сервер по незащищенным каналам при использовании IIOP.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>SSL включен</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="sslRef" type="pidType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Укажите конфигурацию SSL, необходимую для установления защищенного соединения.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Ссылка на SSL</ext:label>
                    <ext:reference>ssl</ext:reference>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.security.csiv2.transportLayer-factory">
        <xsd:complexContent>
            <xsd:extension base="com.ibm.ws.security.csiv2.transportLayer">
                <xsd:attribute name="id" type="factoryIdType" use="optional">
                    <xsd:annotation>
                        <xsd:documentation>Уникальный ИД конфигурации.</xsd:documentation>
                        <xsd:appinfo>
                            <ext:label>ИД</ext:label>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.transport.iiop.security.IiopsOptions">
        <xsd:annotation>
            <xsd:documentation>Укажите параметры порта и SSL для сокета IIOPS, используя хост вмещающего элемента iiopEndpoint.</xsd:documentation>
            <xsd:appinfo>
                <ext:label>Параметры защищенного порта IIOP</ext:label>
            </xsd:appinfo>
        </xsd:annotation>
        <!--Idiom for "empty element context" (whitespace accepted and ignored)-->
        <xsd:sequence>
            <xsd:sequence/>
        </xsd:sequence>
        <xsd:attribute name="iiopsPort" type="intType" use="required">
            <xsd:annotation>
                <xsd:documentation>Укажите порт для настройки с опциями SSL.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Защищенный порт</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="1d" name="sessionTimeout" type="secondDuration" use="optional">
            <xsd:annotation>
                <xsd:documentation>Время, в течение которого следует ждать завершения операции чтения или записи через сокет. Это значение переопределяется тайм-аутом, заданным на уровне протокола. Укажите положительное целое число, за которым следует единица времени: часы (h), минуты (m) или секунды (s). Например, укажите 30 секунд как 30s. Одна запись может включать несколько значений. Так, 1m30s равно 90 сек.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Тайм-аут сеанса</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="false" name="suppressHandshakeErrors" type="booleanType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Отключить протоколирование ошибок процедуры согласования SSL. Ошибки процедуры согласования SSL могут возникать во время нормальной работы, но эти сообщения могут быть полезны в случае неполадок SSL. Если этот параметр выключен, то ошибки согласования не заносятся в протоколы сообщений и консоли, а в протоколе трассировки ошибки согласования регистрируются, когда включена трассировка канала SSL.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Не выводить ошибки процедуры согласования SSL</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="sslRef" type="pidType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Набор конфигураций SSL по умолчанию. Значение по умолчанию: defaultSSLConfig.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Набор SSL по умолчанию</ext:label>
                    <ext:reference>ssl</ext:reference>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="86400s" name="sslSessionTimeout" type="duration" use="optional">
            <xsd:annotation>
                <xsd:documentation>Предельное значение тайм-аута для сеанса SSL в канале SSL. Укажите положительное целое число, за которым следует единица времени: часы (h), минуты (m), секунды (s) или миллисекунды (ms). Например, укажите 500 миллисекунд как 500ms. Одна запись может включать несколько значений. Так, 1s500ms равно 1,5 сек.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Тайм-аут сеанса SSL</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="100" name="sslSessionCacheSize" type="intType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Максимальное количество объектов сеанса, которые могут быть сохранены в кэше.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Размер кэша сеанса SSL</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.transport.iiop.security.IiopsOptions-factory">
        <xsd:complexContent>
            <xsd:extension base="com.ibm.ws.transport.iiop.security.IiopsOptions">
                <xsd:attribute name="id" type="factoryIdType" use="optional">
                    <xsd:annotation>
                        <xsd:documentation>Уникальный ИД конфигурации.</xsd:documentation>
                        <xsd:appinfo>
                            <ext:label>ИД</ext:label>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.security.csiv2.serverPolicyLayers">
        <xsd:annotation>
            <xsd:documentation>Укажите уровни CSIv2, такие как транспорт, идентификация и атрибут.</xsd:documentation>
            <xsd:appinfo>
                <ext:label>Уровни</ext:label>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:choice maxOccurs="unbounded" minOccurs="0">
            <xsd:element name="attributeLayer" type="com.ibm.ws.security.csiv2.attributeLayer">
                <xsd:annotation>
                    <xsd:documentation>Определите опции уровня атрибут, которые сервер будет требовать для входящих запросов CSIv2.</xsd:documentation>
                    <xsd:appinfo>
                        <ext:label>Уровень Атрибут</ext:label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="authenticationLayer" type="com.ibm.ws.security.csiv2.authenticationLayer">
                <xsd:annotation>
                    <xsd:documentation>Определите механизмы идентификации и опции ассоциации, которые сервер будет требовать для входящих запросов CSIv2.</xsd:documentation>
                    <xsd:appinfo>
                        <ext:label>Уровень Идентификация</ext:label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="transportLayer" type="com.ibm.ws.security.csiv2.transportLayer">
                <xsd:annotation>
                    <xsd:documentation>Настройте способ выдачи кредита доверия клиенту.</xsd:documentation>
                    <xsd:appinfo>
                        <ext:label>Уровень Транспорт</ext:label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
        </xsd:choice>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.security.csiv2.serverPolicyLayers-factory">
        <xsd:complexContent>
            <xsd:extension base="com.ibm.ws.security.csiv2.serverPolicyLayers">
                <xsd:attribute name="id" type="factoryIdType" use="optional">
                    <xsd:annotation>
                        <xsd:documentation>Уникальный ИД конфигурации.</xsd:documentation>
                        <xsd:appinfo>
                            <ext:label>ИД</ext:label>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.security.csiv2.attributeLayer">
        <xsd:annotation>
            <xsd:documentation>Определите опции уровня атрибут, которые сервер будет требовать для входящих запросов CSIv2.</xsd:documentation>
            <xsd:appinfo>
                <ext:label>Уровень Атрибут</ext:label>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element default="ITTAnonymous" maxOccurs="4" minOccurs="0" name="identityAssertionTypes">
                <xsd:annotation>
                    <xsd:documentation>Укажите типы ключей идентификации, которые поддерживаются для подтверждения идентификации.</xsd:documentation>
                    <xsd:appinfo>
                        <ext:label>Типы ключей подтверждения идентификации</ext:label>
                    </xsd:appinfo>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union memberTypes="variableType">
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value="ITTAnonymous">
                                    <xsd:annotation>
                                        <xsd:documentation>Подтверждать ключом идентификации ITTAnonymous</xsd:documentation>
                                    </xsd:annotation>
                                </xsd:enumeration>
                                <xsd:enumeration value="ITTPrincipalName">
                                    <xsd:annotation>
                                        <xsd:documentation>Подтверждать ключом идентификации ITTPrincipalName</xsd:documentation>
                                    </xsd:annotation>
                                </xsd:enumeration>
                                <xsd:enumeration value="ITTX509CertChain">
                                    <xsd:annotation>
                                        <xsd:documentation>Подтверждать ключом идентификации ITTX509CertChain</xsd:documentation>
                                    </xsd:annotation>
                                </xsd:enumeration>
                                <xsd:enumeration value="ITTDistinguishedName">
                                    <xsd:annotation>
                                        <xsd:documentation>Подтверждать ключом идентификации ITTDistinguishedName</xsd:documentation>
                                    </xsd:annotation>
                                </xsd:enumeration>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:element>
        </xsd:sequence>
        <xsd:attribute default="false" name="identityAssertionEnabled" type="booleanType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Включите или выключите подтверждение идентификации, указав true или false соответственно. Значение по умолчанию - false.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Подтверждение идентификации включено</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="trustedIdentities" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>Укажите список идентификаторов серверов, разделенных символом вертикальной черты (|), которые считаются надежными для подтверждения идентификации на этом сервере. Допустимо также значение “*”, соответствующее неявной надежности (доверять всем).</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Надежные идентификаторы</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.security.csiv2.attributeLayer-factory">
        <xsd:complexContent>
            <xsd:extension base="com.ibm.ws.security.csiv2.attributeLayer">
                <xsd:attribute name="id" type="factoryIdType" use="optional">
                    <xsd:annotation>
                        <xsd:documentation>Уникальный ИД конфигурации.</xsd:documentation>
