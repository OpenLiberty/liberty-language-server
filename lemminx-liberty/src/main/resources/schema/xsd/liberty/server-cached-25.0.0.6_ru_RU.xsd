                            <xsd:enumeration value="all">
                                <xsd:annotation>
                                    <xsd:documentation>Указанный атрибут элемента может содержать непосредственные, вложенные и динамические элементы.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
