                            <xsd:enumeration value="code">
                                <xsd:annotation>
                                    <xsd:documentation>Код авторизации</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
    </xsd:complexType>
