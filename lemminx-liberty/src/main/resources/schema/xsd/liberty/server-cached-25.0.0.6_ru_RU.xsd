                <xsd:union memberTypes="variableType">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:int">
                            <xsd:minInclusive value="4096"/>
                            <xsd:maxInclusive value="10485760"/>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute name="queryTimeoutInterruptProcessingMode" use="optional">
            <xsd:annotation>
                <xsd:documentation>Свойство драйвера JDBC: queryTimeoutInterruptProcessingMode.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Режим прерывания обработки тайм-аута запроса</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union memberTypes="variableType">
                    <xsd:simpleType>
                        <xsd:restriction base="intType">
                            <xsd:enumeration value="1">
                                <xsd:annotation>
                                    <xsd:documentation>INTERRUPT_PROCESSING_MODE_STATEMENT_CANCEL</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="2">
                                <xsd:annotation>
                                    <xsd:documentation>INTERRUPT_PROCESSING_MODE_CLOSE_SOCKET</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
