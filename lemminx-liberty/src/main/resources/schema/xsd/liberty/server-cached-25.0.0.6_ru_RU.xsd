                                    <xsd:documentation>ActiveDirectoryIntegrated</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="ActiveDirectoryPassword">
                                <xsd:annotation>
                                    <xsd:documentation>ActiveDirectoryPassword</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="SqlPassword">
