                                    <xsd:documentation>Равно</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="contains">
                                <xsd:annotation>
                                    <xsd:documentation>Содержит</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="notContain">
                                <xsd:annotation>
                                    <xsd:documentation>Не содержит</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.security.authentication.filter.cookie-factory">
        <xsd:complexContent>
            <xsd:extension base="com.ibm.ws.security.authentication.filter.cookie">
                <xsd:attribute name="id" type="factoryIdType" use="optional">
                    <xsd:annotation>
                        <xsd:documentation>Уникальный ИД конфигурации.</xsd:documentation>
                        <xsd:appinfo>
                            <ext:label>ИД</ext:label>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.security.authentication.filter.webApp">
        <xsd:annotation>
            <xsd:documentation>Задает имя веб-приложения, например, webApp id="sample "name="app1|app2"matchType="notContain".</xsd:documentation>
            <xsd:appinfo>
                <ext:label>Веб-приложение</ext:label>
            </xsd:appinfo>
        </xsd:annotation>
        <!--Idiom for "empty element context" (whitespace accepted and ignored)-->
        <xsd:sequence>
            <xsd:sequence/>
        </xsd:sequence>
        <xsd:attribute name="name" type="xsd:string" use="required">
            <xsd:annotation>
                <xsd:documentation>Задает имя.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Имя</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="contains" name="matchType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Задает тип сравнения.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Тип сравнения</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union memberTypes="variableType">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="equals">
                                <xsd:annotation>
                                    <xsd:documentation>Равно</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="contains">
                                <xsd:annotation>
                                    <xsd:documentation>Содержит</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="notContain">
                                <xsd:annotation>
                                    <xsd:documentation>Не содержит</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.security.authentication.filter.webApp-factory">
        <xsd:complexContent>
            <xsd:extension base="com.ibm.ws.security.authentication.filter.webApp">
                <xsd:attribute name="id" type="factoryIdType" use="optional">
                    <xsd:annotation>
                        <xsd:documentation>Уникальный ИД конфигурации.</xsd:documentation>
                        <xsd:appinfo>
                            <ext:label>ИД</ext:label>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.security.authentication.filter.host">
        <xsd:annotation>
            <xsd:documentation>Задает имя хоста, например, host id="sample "name="host.mycomp.com" matchType = "contains".</xsd:documentation>
            <xsd:appinfo>
                <ext:label>Хост</ext:label>
            </xsd:appinfo>
        </xsd:annotation>
        <!--Idiom for "empty element context" (whitespace accepted and ignored)-->
        <xsd:sequence>
            <xsd:sequence/>
        </xsd:sequence>
        <xsd:attribute name="name" type="xsd:string" use="required">
            <xsd:annotation>
                <xsd:documentation>Задает имя.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Имя</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="contains" name="matchType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Задает тип сравнения.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Тип сравнения</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union memberTypes="variableType">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="equals">
                                <xsd:annotation>
                                    <xsd:documentation>Равно</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="contains">
                                <xsd:annotation>
                                    <xsd:documentation>Содержит</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="notContain">
                                <xsd:annotation>
                                    <xsd:documentation>Не содержит</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.security.authentication.filter.host-factory">
        <xsd:complexContent>
            <xsd:extension base="com.ibm.ws.security.authentication.filter.host">
                <xsd:attribute name="id" type="factoryIdType" use="optional">
                    <xsd:annotation>
                        <xsd:documentation>Уникальный ИД конфигурации.</xsd:documentation>
                        <xsd:appinfo>
                            <ext:label>ИД</ext:label>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.security.authentication.filter">
        <xsd:annotation>
            <xsd:documentation>Задает правило, содержащее критерии сравнения заголовков запроса HTTP. Если все критерии соблюдены, запрос HTTP выбирается для идентификации. Если значение атрибута matchType равно notContain или содержит значение, то фильтр поддерживает конвейерный список значений.</xsd:documentation>
            <xsd:appinfo>
                <ext:label>Фильтр идентификации</ext:label>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:choice maxOccurs="unbounded" minOccurs="0">
            <xsd:element name="webApp" type="com.ibm.ws.security.authentication.filter.webApp-factory"/>
            <xsd:element name="requestUrl" type="com.ibm.ws.security.authentication.filter.requestUrl-factory"/>
            <xsd:element name="remoteAddress" type="com.ibm.ws.security.authentication.filter.remoteAddress-factory"/>
            <xsd:element name="host" type="com.ibm.ws.security.authentication.filter.host-factory"/>
            <xsd:element name="userAgent" type="com.ibm.ws.security.authentication.filter.userAgent-factory"/>
            <xsd:element name="requestHeader" type="com.ibm.ws.security.authentication.filter.requestHeader-factory"/>
            <xsd:element name="cookie" type="com.ibm.ws.security.authentication.filter.cookie-factory"/>
        </xsd:choice>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.security.authentication.filter-factory">
        <xsd:complexContent>
            <xsd:extension base="com.ibm.ws.security.authentication.filter">
                <xsd:attribute name="id" type="factoryIdType" use="optional">
                    <xsd:annotation>
                        <xsd:documentation>Уникальный ИД конфигурации.</xsd:documentation>
                        <xsd:appinfo>
                            <ext:label>ИД</ext:label>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.app.management">
        <xsd:annotation>
            <xsd:documentation>Свойства, контролирующий поведение администратора приложений</xsd:documentation>
            <xsd:appinfo>
                <ext:label>Администратор приложений</ext:label>
            </xsd:appinfo>
        </xsd:annotation>
        <!--Idiom for "empty element context" (whitespace accepted and ignored)-->
        <xsd:sequence>
            <xsd:sequence/>
        </xsd:sequence>
        <xsd:attribute default="false" name="autoExpand" type="booleanType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Обеспечивает автоматическое извлечение WAR-файлов и EAR-файлов</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Автоматически разворачивать приложения</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="useJandex" type="booleanType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Включает использование индексных файлов Jandex при их наличии в приложении</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Использовать Jandex</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="30s" name="startTimeout" type="secondDuration" use="optional">
            <xsd:annotation>
                <xsd:documentation>Задает продолжительность ожидания сервером запуска приложения до пометки этого приложения как медленного. Укажите положительное целое число, за которым следует единица времени: часы (h), минуты (m) или секунды (s). Например, укажите 30 секунд как 30s. Одна запись может включать несколько значений. Так, 1m30s равно 90 сек.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Тайм-аут запуска</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="30s" name="stopTimeout" type="secondDuration" use="optional">
            <xsd:annotation>
                <xsd:documentation>Задает продолжительность ожидания сервером остановки приложения до пометки этого приложения как медленного. Укажите положительное целое число, за которым следует единица времени: часы (h), минуты (m) или секунды (s). Например, укажите 30 секунд как 30s. Одна запись может включать несколько значений. Так, 1m30s равно 90 сек.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Тайм-аут остановки</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="${server.config.dir}/apps/expanded/" name="expandLocation" type="dirLocation" use="optional">
            <xsd:annotation>
                <xsd:documentation>Позволяет задать расположение, куда должно быть автоматически развернуто приложение. По умолчанию это ${server.config.dir}/apps/expanded/. Одно и то же местоположение expandLocation не может быть использовано несколькими серверами.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Расположение для развертывания</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.app.manager.monitor">
        <xsd:annotation>
            <xsd:documentation>Определяет, как сервер отвечает на добавление, обновление и удаление приложений.</xsd:documentation>
            <xsd:appinfo>
                <ext:label>Мониторинг приложения</ext:label>
            </xsd:appinfo>
        </xsd:annotation>
        <!--Idiom for "empty element context" (whitespace accepted and ignored)-->
        <xsd:sequence>
            <xsd:sequence/>
        </xsd:sequence>
        <xsd:attribute default="500ms" name="pollingRate" type="duration" use="optional">
            <xsd:annotation>
                <xsd:documentation>Частота проверки сервером добавления, обновления и удаления приложений. Укажите положительное целое число, за которым следует единица времени: часы (h), минуты (m), секунды (s) или миллисекунды (ms). Например, укажите 500 миллисекунд как 500ms. Одна запись может включать несколько значений. Так, 1s500ms равно 1,5 сек.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Частота опроса обновлений приложения</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="dropins" name="dropins" type="dirLocation" use="optional">
            <xsd:annotation>
                <xsd:documentation>Расположение приложений, полный путь или путь относительно каталога приложений сервера.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Каталог размещения приложений</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="true" name="dropinsEnabled" type="booleanType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Мониторинг каталога размещения на предмет добавления, обновления и удаления приложений.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Мониторинг каталога размещения приложений</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="polled" name="updateTrigger" use="optional">
            <xsd:annotation>
                <xsd:documentation>Метод или триггер обновления приложения.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Триггер обновления приложения</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union memberTypes="variableType">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="polled">
                                <xsd:annotation>
                                    <xsd:documentation>Сервер будет проверять наличие изменений для всех приложений с заданной частотой и обновлять приложения, для которых существуют изменения.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="mbean">
                                <xsd:annotation>
                                    <xsd:documentation>Сервер будет обновлять приложения только по команде MBean, вызванной извне, например, из среды разработки или приложения управления.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="disabled">
                                <xsd:annotation>
                                    <xsd:documentation>Полностью выключает мониторинг обновлений. Изменения приложений не будут применены во время работы сервера.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.app.manager">
        <xsd:annotation>
            <xsd:documentation>Задает свойства приложения.</xsd:documentation>
            <xsd:appinfo>
                <ext:label>Приложение</ext:label>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:choice maxOccurs="unbounded" minOccurs="0">
            <xsd:element name="startAfter" type="com.ibm.ws.app.manager-factory">
                <xsd:annotation>
                    <xsd:documentation>Указывает приложения, которые должны быть запущены перед запуском данного приложения. </xsd:documentation>
                    <xsd:appinfo>
                        <ext:label>Запускать после</ext:label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="classloader" type="com.ibm.ws.classloading.classloader">
                <xsd:annotation>
                    <xsd:documentation>Задает параметры загрузчика классов приложения.</xsd:documentation>
                    <xsd:appinfo>
                        <ext:label>Загрузчик классов</ext:label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="webservices-bnd" type="com.ibm.ws.javaee.ddmodel.wsbnd.WebservicesBnd"/>
            <xsd:element name="application-client-bnd" type="com.ibm.ws.javaee.dd.clientbnd.ApplicationClientBnd"/>
            <xsd:element name="ejb-jar-bnd" type="com.ibm.ws.javaee.dd.ejbbnd.EJBJarBnd"/>
            <xsd:element name="resourceAdapter" type="com.ibm.ws.jca.embeddedResourceAdapter"/>
            <xsd:element name="web-bnd" type="com.ibm.ws.javaee.dd.webbnd.WebBnd"/>
            <xsd:element name="web-ext" type="com.ibm.ws.javaee.dd.webext.WebExt"/>
            <xsd:element name="appProperties" type="com.ibm.ws.appconfig.appProperties"/>
            <xsd:element name="ejb-jar-ext" type="com.ibm.ws.javaee.dd.ejbext.EJBJarExt"/>
            <xsd:element name="managed-bean-bnd" type="com.ibm.ws.javaee.dd.managedbean.ManagedBeanBnd"/>
            <xsd:element name="application-bnd" type="com.ibm.ws.javaee.dd.appbnd.ApplicationBnd"/>
            <xsd:element name="application-ext" type="com.ibm.ws.javaee.dd.appext.ApplicationExt"/>
        </xsd:choice>
        <xsd:attribute name="location" type="location" use="required">
            <xsd:annotation>
                <xsd:documentation>Расположение приложения, полный путь или путь относительно каталога приложений сервера.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Расположение</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="name" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>Имя приложения.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Имя</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="type" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>Тип архива приложения.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Тип приложения</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="context-root" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>Корневой каталог приложения.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Корневой каталог приложения</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="true" name="autoStart" type="booleanType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Указывает, должен сервер запускать приложение автоматически или нет.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Запускать автоматически</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="useJandex" type="booleanType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Включает использование индексных файлов Jandex при их наличии в приложении</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Использовать Jandex</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="startAfterRef" type="pidListType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Указывает приложения, которые должны быть запущены перед запуском данного приложения. </xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Запускать после</ext:label>
                    <ext:reference>application</ext:reference>
                    <ext:reference>webApplication</ext:reference>
                    <ext:reference>enterpriseApplication</ext:reference>
                    <ext:reference>ejbApplication</ext:reference>
                    <ext:reference>resourceAdapter</ext:reference>
                    <ext:reference>springBootApplication</ext:reference>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="classloaderRef" type="pidType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Задает параметры загрузчика классов приложения.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Загрузчик классов</ext:label>
                    <ext:reference>classloader</ext:reference>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
