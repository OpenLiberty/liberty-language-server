                        <ext:label>Свойства конечной точки веб-службы</ext:label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="http-publishing" type="com.ibm.ws.javaee.ddmodel.wsbnd.HttpPublishing-factory">
                <xsd:annotation>
                    <xsd:documentation>Публикация HTTP задает конфигурацию публикации при использовании протокола HTTP для всех конечных точек веб-служб. </xsd:documentation>
                    <xsd:appinfo>
                        <ext:label>Публикация HTTP</ext:label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="service-ref" type="com.ibm.ws.javaee.ddmodel.wsbnd.ServiceRef-factory">
