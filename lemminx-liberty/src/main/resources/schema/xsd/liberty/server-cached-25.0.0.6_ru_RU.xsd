                    <xsd:documentation>Идентификатор фабрики соединений JMS.</xsd:documentation>
                    <xsd:appinfo>
                        <ext:label>Фабрика соединений JMS</ext:label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
        <xsd:attribute default="batchConnectionFactory" name="connectionFactoryRef" type="pidType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Идентификатор фабрики соединений JMS.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Ссылка на фабрику соединений пакетных событий</ext:label>
                    <ext:reference>jmsConnectionFactory</ext:reference>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="batch" name="topicRoot" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>Корневой субъект дерева разделов пакетных событий JMS. Значение по умолчанию для корневого субъекта разделов - 'batch'.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Корневой субъект раздела событий JMS</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.jbatch.jms.events-factory">
        <xsd:complexContent>
            <xsd:extension base="com.ibm.ws.jbatch.jms.events">
                <xsd:attribute name="id" type="factoryIdType" use="optional">
                    <xsd:annotation>
                        <xsd:documentation>Уникальный ИД конфигурации.</xsd:documentation>
