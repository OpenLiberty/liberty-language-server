                <ext:label>Свойства Derby Network Client</ext:label>
                <ext:extraProperties/>
                <ext:groupDecl id="Advanced" label="Дополнительные свойства">Дополнительные свойства для более расширенного использования.</ext:groupDecl>
            </xsd:appinfo>
        </xsd:annotation>
        <!--Idiom for "empty element context" (whitespace accepted and ignored)-->
        <xsd:sequence>
            <xsd:sequence/>
        </xsd:sequence>
        <xsd:attribute name="createDatabase" use="optional">
            <xsd:annotation>
                <xsd:documentation>Свойство драйвера JDBC: createDatabase.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Создать базу данных</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union memberTypes="variableType">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="create">
                                <xsd:annotation>
                                    <xsd:documentation>При установлении соединения автоматически создать базу данных, если она не существует.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="false">
                                <xsd:annotation>
                                    <xsd:documentation>Не создавать базу данных автоматически.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute name="databaseName" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>Свойство драйвера JDBC: databaseName. Это свойство игнорируется драйвером, если настроен URL.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Имя базы данных</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="localhost" name="serverName" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>Свойство драйвераJDBC : serverName. Это свойство игнорируется драйвером, если настроен URL.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Имя сервера</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="1527" name="portNumber" type="intType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Свойство драйвераJDBC : portNumber. Это свойство игнорируется драйвером, если настроен URL.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Номер порта</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="connectionAttributes" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>Свойство драйвера JDBC: connectionAttributes.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Атрибуты соединения</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="loginTimeout" type="secondDuration" use="optional">
            <xsd:annotation>
                <xsd:documentation>Свойство драйвера JDBC: loginTimeout. Укажите положительное целое число, за которым следует единица времени: часы (h), минуты (m) или секунды (s). Например, укажите 30 секунд как 30s. Одна запись может включать несколько значений. Так, 1m30s равно 90 сек.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Тайм-аут сеанса</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="password" type="password" use="optional">
            <xsd:annotation>
                <xsd:documentation>Вместо этого свойства рекомендуется использовать псевдоним идентификации, управляемой контейнером.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Пароль</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="true" name="retrieveMessageText" type="booleanType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Свойство драйвера JDBC: retrieveMessageText.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Получить текст сообщения</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="3" name="securityMechanism" use="optional">
            <xsd:annotation>
                <xsd:documentation>Свойство драйвера JDBC: securityMechanism. Допустимые значения: 3 (CLEAR_TEXT_PASSWORD_SECURITY), 4 (USER_ONLY_SECURITY), 7 (ENCRYPTED_PASSWORD_SECURITY), 8 (STRONG_PASSWORD_SUBSTITUTE_SECURITY), 9 (ENCRYPTED_USER_AND_PASSWORD_SECURITY).</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Механизм защиты</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union memberTypes="variableType">
                    <xsd:simpleType>
                        <xsd:restriction base="shortType">
                            <xsd:enumeration value="3">
                                <xsd:annotation>
                                    <xsd:documentation>CLEAR_TEXT_PASSWORD_SECURITY</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="4">
                                <xsd:annotation>
                                    <xsd:documentation>USER_ONLY_SECURITY</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="7">
                                <xsd:annotation>
                                    <xsd:documentation>ENCRYPTED_PASSWORD_SECURITY</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="8">
                                <xsd:annotation>
                                    <xsd:documentation>STRONG_PASSWORD_SUBSTITUTE_SECURITY</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="9">
                                <xsd:annotation>
                                    <xsd:documentation>ENCRYPTED_USER_AND_PASSWORD_SECURITY</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute name="shutdownDatabase" use="optional">
            <xsd:annotation>
                <xsd:documentation>Свойство драйвера JDBC: shutdownDatabase.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Выключить базу данных</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union memberTypes="variableType">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="shutdown">
                                <xsd:annotation>
                                    <xsd:documentation>Выключать базу данных при попытке соединения.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="false">
                                <xsd:annotation>
                                    <xsd:documentation>Не выключать базу данных.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute name="ssl" use="optional">
            <xsd:annotation>
                <xsd:documentation>Свойство драйвера JDBC: ssl.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>SSL</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union memberTypes="variableType">
