    <xsd:complexType name="com.ibm.ws.security.registry.basic.config.user">
        <xsd:annotation>
            <xsd:documentation>Пользователь в простом реестре пользователей.</xsd:documentation>
            <xsd:appinfo>
                <ext:label>Пользователь</ext:label>
