                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="10">
                                <xsd:annotation>
                                    <xsd:documentation>BIDI_ST10</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="11">
                                <xsd:annotation>
                                    <xsd:documentation>BIDI_ST11</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
