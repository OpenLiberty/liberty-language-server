                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute name="toolboxTrace" use="optional">
            <xsd:annotation>
                <xsd:documentation>Свойство драйвера JDBC: toolboxTrace.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Трассировка инструментария</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union memberTypes="variableType">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="none">
                                <xsd:annotation>
                                    <xsd:documentation>none</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="datastream">
                                <xsd:annotation>
                                    <xsd:documentation>datastream</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="diagnostic">
                                <xsd:annotation>
                                    <xsd:documentation>diagnostic</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="error">
                                <xsd:annotation>
                                    <xsd:documentation>error</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="information">
                                <xsd:annotation>
                                    <xsd:documentation>information</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="warning">
                                <xsd:annotation>
                                    <xsd:documentation>warning</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="conversion">
                                <xsd:annotation>
                                    <xsd:documentation>conversion</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="proxy">
                                <xsd:annotation>
                                    <xsd:documentation>proxy</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="pcml">
                                <xsd:annotation>
                                    <xsd:documentation>pcml</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="jdbc">
                                <xsd:annotation>
                                    <xsd:documentation>jdbc</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="all">
                                <xsd:annotation>
                                    <xsd:documentation>all</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="thread">
                                <xsd:annotation>
                                    <xsd:documentation>thread</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute name="trace" type="booleanType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Свойство драйвера JDBC: trace.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Трассировка</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="false" name="translateBinary" type="booleanType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Свойство драйвера JDBC: translateBinary.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Трансляция binary</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="true" name="translateBoolean" type="booleanType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Свойство драйвера JDBC: translateBoolean.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Трансляция boolean</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="character" name="translateHex" use="optional">
            <xsd:annotation>
                <xsd:documentation>Свойство драйвера JDBC: translateHex.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Трансляция hexadecimal</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union memberTypes="variableType">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="binary">
                                <xsd:annotation>
                                    <xsd:documentation>binary</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="character">
                                <xsd:annotation>
                                    <xsd:documentation>character</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute default="false" name="trueAutoCommit" type="booleanType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Свойство драйвера JDBC: trueAutoCommit.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Безусловная автоматическая фиксация</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="useBlockUpdate" type="booleanType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Свойство драйвера JDBC: useBlockUpdate.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Использовать блочное обновление</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="useDrdaMetadataVersion" type="booleanType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Свойство драйвера JDBC: useDrdaMetadataVersion.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Использовать версию метаданных DRDA</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="user" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>Вместо этого свойства рекомендуется использовать псевдоним идентификации, управляемой контейнером.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Пользователь</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="variableFieldCompression" use="optional">
            <xsd:annotation>
                <xsd:documentation>Свойство драйвера JDBC: variableFieldCompression.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Сжатие полей переменной длины</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union memberTypes="variableType">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="true">
                                <xsd:annotation>
                                    <xsd:documentation>true</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="false">
                                <xsd:annotation>
                                    <xsd:documentation>false</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="insert">
                                <xsd:annotation>
                                    <xsd:documentation>insert</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="all">
                                <xsd:annotation>
                                    <xsd:documentation>all</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute default="0" name="xaLooselyCoupledSupport" use="optional">
            <xsd:annotation>
                <xsd:documentation>Свойство драйвера JDBC: xaLooselyCoupledSupport.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Поддержка слабой связи XA</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union memberTypes="variableType">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:int">
                            <xsd:minInclusive value="0"/>
                            <xsd:maxInclusive value="1"/>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute name="internal.properties" type="internalPropertiesType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Дополнительные свойства поставщика JDBC для источника данных.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Дополнительные свойства</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:anyAttribute processContents="skip"/>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.jdbc.dataSource.properties.db2.i.toolbox-factory">
        <xsd:complexContent>
            <xsd:extension base="com.ibm.ws.jdbc.dataSource.properties.db2.i.toolbox">
                <xsd:attribute name="id" type="factoryIdType" use="optional">
                    <xsd:annotation>
                        <xsd:documentation>Уникальный ИД конфигурации.</xsd:documentation>
                        <xsd:appinfo>
                            <ext:label>ИД</ext:label>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.jdbc.dataSource">
        <xsd:annotation>
            <xsd:documentation>Задает конфигурацию источника данных.</xsd:documentation>
            <xsd:appinfo>
                <ext:label>Источник данных</ext:label>
                <ext:groupDecl id="Advanced" label="Дополнительные свойства">Дополнительные свойства для более расширенного использования.</ext:groupDecl>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:choice maxOccurs="unbounded" minOccurs="0">
            <xsd:element name="jdbcDriver" type="com.ibm.ws.jdbc.jdbcDriver">
                <xsd:annotation>
                    <xsd:documentation>Драйвер JDBC источника данных. Если изменяется при запущенном сервере, существующие соединения уничтожаются.</xsd:documentation>
                    <xsd:appinfo>
                        <ext:label>Драйвер JDBC</ext:label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="connectionManager" type="com.ibm.ws.jca.connectionManager">
                <xsd:annotation>
                    <xsd:documentation>Администратор соединений для источника данных. Если изменяется при запущенном сервере, существующие соединения уничтожаются.</xsd:documentation>
                    <xsd:appinfo>
                        <ext:label>Администратор соединений</ext:label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="properties.informix" type="com.ibm.ws.jdbc.dataSource.properties.informix"/>
            <xsd:element name="properties.db2.i.native" type="com.ibm.ws.jdbc.dataSource.properties.db2.i.native"/>
            <xsd:element name="properties.oracle.ucp" type="com.ibm.ws.jdbc.dataSource.properties.oracle.ucp"/>
            <xsd:element name="properties.derby.embedded" type="com.ibm.ws.jdbc.dataSource.properties.derby.embedded"/>
            <xsd:element name="properties" type="com.ibm.ws.jdbc.dataSource.properties"/>
            <xsd:element name="properties.microsoft.sqlserver" type="com.ibm.ws.jdbc.dataSource.properties.microsoft.sqlserver"/>
            <xsd:element name="properties.oracle" type="com.ibm.ws.jdbc.dataSource.properties.oracle"/>
            <xsd:element name="properties.derby.client" type="com.ibm.ws.jdbc.dataSource.properties.derby.client"/>
            <xsd:element name="properties.datadirect.sqlserver" type="com.ibm.ws.jdbc.dataSource.properties.datadirect.sqlserver"/>
            <xsd:element name="properties.informix.jcc" type="com.ibm.ws.jdbc.dataSource.properties.informix.jcc"/>
            <xsd:element name="properties.db2.jcc" type="com.ibm.ws.jdbc.dataSource.properties.db2.jcc"/>
            <xsd:element name="properties.db2.i.toolbox" type="com.ibm.ws.jdbc.dataSource.properties.db2.i.toolbox"/>
            <xsd:element name="properties.sybase" type="com.ibm.ws.jdbc.dataSource.properties.sybase"/>
            <xsd:element name="properties.postgresql" type="com.ibm.ws.jdbc.dataSource.properties.postgresql"/>
            <xsd:element name="containerAuthData" type="com.ibm.ws.security.jca.internal.authdata.config">
                <xsd:annotation>
                    <xsd:documentation>Идентификационные данные по умолчанию для управляемой контейнером идентификации применяются, когда привязки не указывают псевдоним идентификации для ссылки на ресурс с помощью res-auth=CONTAINER. Если изменяются при запущенном сервере, изменение применяется для новых запросов соединения, на существующие соединения изменение не влияет.</xsd:documentation>
                    <xsd:appinfo>
                        <ext:label>Идентификационные данные по умолчанию, управляемые контейнером</ext:label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="jaasLoginContextEntry" type="com.ibm.ws.security.authentication.internal.jaas.jaasLoginContextEntry">
                <xsd:annotation>
                    <xsd:documentation>Запись контекста сеансов JAAS для идентификации. Если изменяются при запущенном сервере, обновление применяется для новых запросов соединения, на существующие соединения обновление не влияет.</xsd:documentation>
                    <xsd:appinfo>
                        <ext:label>Запись контекста сеансов JAAS</ext:label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="identifyException" type="com.ibm.ws.jdbc.dataSource.identifyException-factory">
                <xsd:annotation>
                    <xsd:documentation>Идентифицировать конкретный код ошибки SQL или состояние SQL в SQLException. В этом случае сервер может правильным образом реагировать на ошибку. Например, устаревшее соединение будет закрыто, а на возвращено в пул соединений.</xsd:documentation>
                    <xsd:appinfo>
                        <ext:label>Идентифицировать исключительную ситуацию</ext:label>
                        <ext:group id="Advanced"/>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="onConnect" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>Команда SQL, которая выполняется каждый раз при подключении к базе данных. Оператор SQL применяется только к новым соединениям; существующие соединения из пула соединений игнорируются. Если изменяется при запущенном сервере, существующие соединения уничтожаются.</xsd:documentation>
                    <xsd:appinfo>
                        <ext:label>При подключении</ext:label>
                        <ext:group id="Advanced"/>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="recoveryAuthData" type="com.ibm.ws.security.jca.internal.authdata.config">
                <xsd:annotation>
                    <xsd:documentation>Идентификационные данные для восстановления транзакций.</xsd:documentation>
                    <xsd:appinfo>
                        <ext:label>Идентификационные данные восстановления</ext:label>
                        <ext:group id="Advanced"/>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
        </xsd:choice>
        <xsd:attribute default="false" name="enableConnectionCasting" type="booleanType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Указывает, что соединения, полученные из источника данных, должны допускать преобразование типов в классы интерфейса, реализованные посредством реализации соединения производителя JDBC. Включение этой опции вызывает дополнительную нагрузку на каждую операцию getConnection. Если интерфейсы JDBC производителя необходимо использовать менее часто, может оказаться более эффективным выключить эту опцию и использовать только Connection.unwrap(interface), где это требуется. Если изменяются при запущенном сервере, обновление применяется для новых запросов соединения, на существующие соединения обновление не влияет.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Включить преобразование типов соединения</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="jndiName" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>Имя JNDI источника данных. Если изменяется при запущенном сервере, существующие соединения уничтожаются.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Имя JNDI</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="jdbcDriverRef" type="pidType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Драйвер JDBC источника данных. Если изменяется при запущенном сервере, существующие соединения уничтожаются.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Ссылка на драйвер JDBC</ext:label>
                    <ext:reference>jdbcDriver</ext:reference>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="connectionManagerRef" type="pidType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Администратор соединений для источника данных. Если изменяется при запущенном сервере, существующие соединения уничтожаются.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Ссылка на администратор соединений</ext:label>
                    <ext:reference>connectionManager</ext:reference>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="type" use="optional">
            <xsd:annotation>
                <xsd:documentation>Тип источника данных. Если изменяется при запущенном сервере, существующие соединения уничтожаются.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Тип</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union memberTypes="variableType">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="javax.sql.XADataSource">
                                <xsd:annotation>
                                    <xsd:documentation>javax.sql.XADataSource</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="javax.sql.ConnectionPoolDataSource">
                                <xsd:annotation>
                                    <xsd:documentation>javax.sql.ConnectionPoolDataSource</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="javax.sql.DataSource">
                                <xsd:annotation>
                                    <xsd:documentation>javax.sql.DataSource</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="java.sql.Driver">
                                <xsd:annotation>
                                    <xsd:documentation>java.sql.Driver</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute default="MatchOriginalRequest" name="connectionSharing" use="optional">
            <xsd:annotation>
                <xsd:documentation>Задает способ сравнения соединений при совместном использовании.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Сравнение соединений</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union memberTypes="variableType">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="MatchOriginalRequest">
                                <xsd:annotation>
                                    <xsd:documentation>При совместном использовании выполнять сравнение исходя из исходного запроса на установление соединения.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="MatchCurrentState">
                                <xsd:annotation>
                                    <xsd:documentation>При совместном использовании выполнять сравнение исходя из текущего состояния соединения. Если изменяется при запущенном сервере, обновление применяется к каждому первому описателю соединения в транзакции.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute name="containerAuthDataRef" type="pidType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Идентификационные данные по умолчанию для управляемой контейнером идентификации применяются, когда привязки не указывают псевдоним идентификации для ссылки на ресурс с помощью res-auth=CONTAINER. Если изменяются при запущенном сервере, изменение применяется для новых запросов соединения, на существующие соединения изменение не влияет.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Идентификационные данные по умолчанию, управляемые контейнером</ext:label>
                    <ext:reference>authData</ext:reference>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="jaasLoginContextEntryRef" type="pidType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Запись контекста сеансов JAAS для идентификации. Если изменяются при запущенном сервере, обновление применяется для новых запросов соединения, на существующие соединения обновление не влияет.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Указатель на запись контекста сеансов JAAS</ext:label>
                    <ext:reference>jaasLoginContextEntry</ext:reference>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="isolationLevel" use="optional">
            <xsd:annotation>
                <xsd:documentation>Уровень изоляции транзакции по умолчанию. Если он не указан, и база данных идентифицируется как DB2, Derby, Informix, Microsoft SQL Server или Sybase, то используется TRANSACTION_REPEATABLE_READ. Если не указан для других баз данных, то используется TRANSACTION_READ_COMMITTED. Если изменяются при запущенном сервере, обновление применяется для новых запросов соединения, на существующие соединения обновление не влияет.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Уровень изоляции транзакции</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union memberTypes="variableType">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="TRANSACTION_READ_UNCOMMITTED">
                                <xsd:annotation>
                                    <xsd:documentation>Возможно грязное чтение, неповторяющееся чтение и фантомное чтение.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="TRANSACTION_READ_COMMITTED">
                                <xsd:annotation>
                                    <xsd:documentation>Грязное чтение не допускается, однако возможно неповторяющееся чтение и фантомное чтение.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="TRANSACTION_REPEATABLE_READ">
                                <xsd:annotation>
                                    <xsd:documentation>Грязное чтение и неповторяющееся чтение не допускаются, фантомное чтение возможно.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="TRANSACTION_SERIALIZABLE">
                                <xsd:annotation>
                                    <xsd:documentation>Грязное чтение, неповторяющееся чтение и фантомное чтение не допускаются.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="TRANSACTION_SNAPSHOT">
                                <xsd:annotation>
                                    <xsd:documentation>Изоляция моментальной копии для Microsoft SQL Server JDBC Driver и DataDirect Connect for JDBC driver.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="TRANSACTION_NONE">
                                <xsd:annotation>
                                    <xsd:documentation>Указывает, что драйвер JDBC не поддерживает транзакции.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute default="10" name="statementCacheSize" use="optional">
            <xsd:annotation>
                <xsd:documentation>Максимальное число помещаемых в кэш операторов для каждого соединения. Если изменяется при запущенном сервере, размер кэша операторов будет изменен при следующем использовании. Для того чтобы указать этот параметр, выполните следующие предварительные требования: проверьте все подготовленные уникальные операторы в коде приложения или данных трассировки SQL, собранных для базы данных или драйвера базы данных. Убедитесь, что размер кэша больше числа операторов.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Операторов в кэше для каждого соединения</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union memberTypes="variableType">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:int">
                            <xsd:minInclusive value="0"/>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute default="true" name="transactional" type="booleanType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Разрешает участие в транзакциях, которыми управляет сервер приложений. Если изменяется при запущенном сервере, существующие соединения уничтожаются.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Участие в транзакциях</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="true" name="beginTranForResultSetScrollingAPIs" type="booleanType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Выполнить операцию enlist транзакции при использовании интерфейсов прокрутки набора результатов.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Выполнить Enlist для API прокрутки</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="true" name="beginTranForVendorAPIs" type="booleanType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Выполнить операцию enlist транзакции при использовании интерфейсов вендора.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Выполнить Enlist для API вендора</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="commitOrRollbackOnCleanup" use="optional">
            <xsd:annotation>
                <xsd:documentation>Задает способ очистки соединений из единичного задания базы данных (AutoCommit=false) при закрытии или возврате соединения в пул.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Фиксация или откат в ходе очистки</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union memberTypes="variableType">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="commit">
                                <xsd:annotation>
                                    <xsd:documentation>Очистка соединения посредством фиксации.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="rollback">
                                <xsd:annotation>
                                    <xsd:documentation>Очистка соединения посредством отката.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute name="queryTimeout" type="secondDuration" use="optional">
            <xsd:annotation>
                <xsd:documentation>Тайм-аут запроса для операторов SQL по умолчанию. В транзакции JTA это значение можно переопределить с помощью syncQueryTimeoutWithTransactionTimeout. Укажите положительное целое число, за которым следует единица времени: часы (h), минуты (m) или секунды (s). Например, укажите 30 секунд как 30s. Одна запись может включать несколько значений. Так, 1m30s равно 90 сек.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Тайм-аут запроса</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="recoveryAuthDataRef" type="pidType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Идентификационные данные для восстановления транзакций.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Ссылка на идентификационные данные восстановления</ext:label>
                    <ext:group id="Advanced"/>
                    <ext:reference>authData</ext:reference>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="false" name="syncQueryTimeoutWithTransactionTimeout" type="booleanType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Использует время, оставшееся (если оно осталось) в транзакции JTA, в качестве тайм-аута запроса по умолчанию для операторов SQL.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Синхронизировать запрос и тайм-ауты транзакций</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="supplementalJDBCTrace" type="booleanType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Дополняет трассировку драйвера JDBC по отношению к параметрам трассировки, заданным в bootstrap.properties. Спецификация трассировки драйвера JDBC включает: com.ibm.ws.database.logwriter, com.ibm.ws.db2.logwriter, com.ibm.ws.derby.logwriter, com.ibm.ws.informix.logwriter, com.ibm.ws.oracle.logwriter, com.ibm.ws.sqlserver.logwriter, com.ibm.ws.sybase.logwriter. Если изменяется при запущенном сервере, существующие соединения уничтожаются.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Дополнительная трассировка JDBC</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="validationTimeout" type="secondDuration" use="optional">
            <xsd:annotation>
                <xsd:documentation>Если значение указано, то соединения из пула соединений проверяются перед повторным использованием. Кроме того, тайм-аут проверки применяется, когда администратор соединений проверяет соединения при обнаружении устаревшего соединения для PurgePolicy=ValidateAllConnections. The full amount of the validation timeout applies to each connection that is validated, which is done via the Connection.isValid(timeout) JDBC API operation. Значение 0 указывает, что соединения проверяются без применения тайм-аута. Для поддержки тайм-аута проверки требуется драйвер JDBC, соответствующий требованиям спецификации JDBC 4.0 и выше. Укажите положительное целое число, за которым следует единица времени: часы (h), минуты (m) или секунды (s). Например, укажите 30 секунд как 30s. Одна запись может включать несколько значений. Так, 1m30s равно 90 сек.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Тайм-аут проверки</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.jdbc.dataSource-factory">
        <xsd:complexContent>
            <xsd:extension base="com.ibm.ws.jdbc.dataSource">
                <xsd:attribute name="id" type="factoryIdType" use="optional">
                    <xsd:annotation>
                        <xsd:documentation>Уникальный ИД конфигурации.</xsd:documentation>
                        <xsd:appinfo>
                            <ext:label>ИД</ext:label>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.jdbc.dataSource.properties.sybase">
        <xsd:annotation>
            <xsd:documentation>Свойства источника данных для драйвера JDBC Sybase.</xsd:documentation>
            <xsd:appinfo>
                <ext:label>Свойства Sybase</ext:label>
                <ext:extraProperties/>
                <ext:groupDecl id="Advanced" label="Дополнительные свойства">Дополнительные свойства для более расширенного использования.</ext:groupDecl>
            </xsd:appinfo>
        </xsd:annotation>
        <!--Idiom for "empty element context" (whitespace accepted and ignored)-->
        <xsd:sequence>
            <xsd:sequence/>
        </xsd:sequence>
        <xsd:attribute name="databaseName" type="xsd:string" use="required">
            <xsd:annotation>
                <xsd:documentation>Свойство драйвера JDBC: databaseName.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Имя базы данных</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="localhost" name="serverName" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>Сервер, на котором работает база данных.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Имя сервера</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="5000" name="portNumber" type="intType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Порт для соединения с базой данных.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Номер порта</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="SELECT_OPENS_CURSOR=true" name="connectionProperties" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>Свойство драйвера JDBC: connectionProperties. Для свойств SSL javax.net.ssl.keyStorePassword и javax.net.ssl.trustStorePassword можно настроить зашифрованные значения. Это делается так же, как для паролей других источников данных.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Свойства соединения</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="loginTimeout" type="secondDuration" use="optional">
            <xsd:annotation>
                <xsd:documentation>Свойство драйвера JDBC: loginTimeout. Укажите положительное целое число, за которым следует единица времени: часы (h), минуты (m) или секунды (s). Например, укажите 30 секунд как 30s. Одна запись может включать несколько значений. Так, 1m30s равно 90 сек.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Тайм-аут сеанса</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="networkProtocol" use="optional">
            <xsd:annotation>
                <xsd:documentation>Свойство драйвера JDBC: networkProtocol.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Сетевой протокол</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union memberTypes="variableType">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="socket">
                                <xsd:annotation>
                                    <xsd:documentation>socket</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="SSL">
                                <xsd:annotation>
                                    <xsd:documentation>SSL</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute name="password" type="password" use="optional">
            <xsd:annotation>
                <xsd:documentation>Вместо этого свойства рекомендуется использовать псевдоним идентификации, управляемой контейнером.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Пароль</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="resourceManagerName" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>Свойство драйвера JDBC: resourceManagerName.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Имя администратора ресурсов</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="false" name="SERVER_INITIATED_TRANSACTIONS" use="optional">
            <xsd:annotation>
                <xsd:documentation>Свойство драйвера JDBC: SERVER_INITIATED_TRANSACTIONS.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Транзакции, начатые сервером</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union memberTypes="variableType">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="true">
                                <xsd:annotation>
                                    <xsd:documentation>true</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="false">
                                <xsd:annotation>
                                    <xsd:documentation>false</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute name="user" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>Вместо этого свойства рекомендуется использовать псевдоним идентификации, управляемой контейнером.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Пользователь</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="version" type="intType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Свойство драйвера JDBC: version.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Версия</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="internal.properties" type="internalPropertiesType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Дополнительные свойства поставщика JDBC для источника данных.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Дополнительные свойства</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:anyAttribute processContents="skip"/>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.jdbc.dataSource.properties.sybase-factory">
        <xsd:complexContent>
            <xsd:extension base="com.ibm.ws.jdbc.dataSource.properties.sybase">
                <xsd:attribute name="id" type="factoryIdType" use="optional">
                    <xsd:annotation>
                        <xsd:documentation>Уникальный ИД конфигурации.</xsd:documentation>
                        <xsd:appinfo>
                            <ext:label>ИД</ext:label>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.jdbc.dataSource.properties.postgresql">
        <xsd:annotation>
            <xsd:documentation>Свойства источника данных для драйвера JDBC PostgreSQL.</xsd:documentation>
            <xsd:appinfo>
                <ext:label>Свойства PostgreSQL</ext:label>
                <ext:extraProperties/>
                <ext:groupDecl id="Advanced" label="Дополнительные свойства">Дополнительные свойства для более расширенного использования.</ext:groupDecl>
            </xsd:appinfo>
        </xsd:annotation>
        <!--Idiom for "empty element context" (whitespace accepted and ignored)-->
        <xsd:sequence>
            <xsd:sequence/>
        </xsd:sequence>
        <xsd:attribute name="databaseName" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>Свойство драйвера JDBC: databaseName. Когда драйвер PostgreSQL использует тип java.sql.Driver и URL, и это свойство настроено, драйвер PostgreSQL игнорирует это свойство. Если драйвер PostgreSQL использует тип javax.sql.DataSource, а URL-адрес и это свойство настроены, драйвер PostgreSQL игнорирует свойство databaseName .</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Имя базы данных</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="serverName" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>Свойство драйвераJDBC : serverName. Когда драйвер PostgreSQL использует тип java.sql.Driver и URL, и это свойство настроено, драйвер PostgreSQL игнорирует это свойство. Если драйвер PostgreSQL использует тип javax.sql.DataSource, а URL-адрес и это свойство настроены, драйвер PostgreSQL игнорирует свойство serverName .</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Имя сервера</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="portNumber" type="intType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Свойство драйвераJDBC : portNumber. Когда драйвер PostgreSQL использует тип java.sql.Driver и URL, и это свойство настроено, драйвер PostgreSQL игнорирует это свойство. Если драйвер PostgreSQL использует тип javax.sql.DataSource, а URL-адрес и это свойство настроены, драйвер PostgreSQL игнорирует свойство portNumber .</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Номер порта</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="applicationName" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>Свойство драйвера JDBC: applicationName.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Имя приложения</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="cancelSignalTimeout" type="secondDuration" use="optional">
            <xsd:annotation>
                <xsd:documentation>Свойство драйвера JDBC: cancelSignalTimeout. Укажите положительное целое число, за которым следует единица времени: часы (h), минуты (m) или секунды (s). Например, укажите 30 секунд как 30s. Одна запись может включать несколько значений. Так, 1m30s равно 90 сек.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Отменить тайм-аут сигнала</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="connectTimeout" type="secondDuration" use="optional">
            <xsd:annotation>
                <xsd:documentation>Свойство драйвера JDBC: connectTimeout. Укажите положительное целое число, за которым следует единица времени: часы (h), минуты (m) или секунды (s). Например, укажите 30 секунд как 30s. Одна запись может включать несколько значений. Так, 1m30s равно 90 сек.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Тайм-аут подключения</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="currentSchema" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>Свойство драйвера JDBC: currentSchema.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Текущая схема</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="defaultRowFetchSize" use="optional">
            <xsd:annotation>
                <xsd:documentation>Свойство драйвера JDBC: defaultRowFetchSize.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Размер выборки строки по умолчанию</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union memberTypes="variableType">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:int">
                            <xsd:minInclusive value="0"/>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute name="loginTimeout" type="secondDuration" use="optional">
            <xsd:annotation>
                <xsd:documentation>Свойство драйвера JDBC: loginTimeout. Укажите положительное целое число, за которым следует единица времени: часы (h), минуты (m) или секунды (s). Например, укажите 30 секунд как 30s. Одна запись может включать несколько значений. Так, 1m30s равно 90 сек.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Тайм-аут сеанса</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="password" type="password" use="optional">
            <xsd:annotation>
                <xsd:documentation>Вместо этого свойства рекомендуется использовать псевдоним идентификации, управляемой контейнером.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Пароль</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="0" name="preparedStatementCacheQueries" use="optional">
            <xsd:annotation>
                <xsd:documentation>Свойство драйвера JDBC: preparedStatementCacheQueries.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Запросы кэша подготовленных операторов</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union memberTypes="variableType">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:int">
                            <xsd:minInclusive value="0"/>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute name="readOnly" type="booleanType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Свойство драйвера JDBC: readOnly.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Только чтение</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ssl" type="booleanType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Свойство драйвера JDBC: ssl.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>SSL</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="sslCert" type="location" use="optional">
            <xsd:annotation>
                <xsd:documentation>Свойство драйвера JDBC: sslCert.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Расположение сертификата SSL</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="sslfactory" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>Свойство драйвера JDBC: sslfactory.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Имя класса фабрики SSL</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="sslKey" type="location" use="optional">
            <xsd:annotation>
                <xsd:documentation>Свойство драйвера JDBC: sslKey.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Расположение ключа SSL</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="sslMode" use="optional">
            <xsd:annotation>
                <xsd:documentation>Свойство драйвера JDBC: sslMode.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Режим SSL</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union memberTypes="variableType">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="disable">
                                <xsd:annotation>
                                    <xsd:documentation>disable</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="allow">
                                <xsd:annotation>
                                    <xsd:documentation>allow</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="prefer">
                                <xsd:annotation>
                                    <xsd:documentation>prefer</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="require">
                                <xsd:annotation>
                                    <xsd:documentation>require</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="verify-ca">
                                <xsd:annotation>
                                    <xsd:documentation>verify-ca</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="verify-full">
                                <xsd:annotation>
                                    <xsd:documentation>verify-full</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute name="sslPassword" type="password" use="optional">
            <xsd:annotation>
                <xsd:documentation>Свойство драйвера JDBC: sslPassword.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Пароль SSL</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="sslRootCert" type="location" use="optional">
            <xsd:annotation>
                <xsd:documentation>Свойство драйвера JDBC: sslRootCert.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Расположение базового сертификата SSL</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="socketTimeout" type="secondDuration" use="optional">
            <xsd:annotation>
                <xsd:documentation>Свойство драйвера JDBC: socketTimeout. Укажите положительное целое число, за которым следует единица времени: часы (h), минуты (m) или секунды (s). Например, укажите 30 секунд как 30s. Одна запись может включать несколько значений. Так, 1m30s равно 90 сек.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Тайм-аут сокета</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="targetServerType" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>Свойство драйвера JDBC: targetServerType.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Тип целевого сервера</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="tcpKeepAlive" type="booleanType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Свойство драйвера JDBC: tcpKeepAlive.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Проверка активности TCP</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="user" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>Вместо этого свойства рекомендуется использовать псевдоним идентификации, управляемой контейнером.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Пользователь</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="URL" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>URL для подключения к базе данных.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>URL</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="internal.properties" type="internalPropertiesType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Дополнительные свойства поставщика JDBC для источника данных.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Дополнительные свойства</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:anyAttribute processContents="skip"/>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.jdbc.dataSource.properties.postgresql-factory">
        <xsd:complexContent>
            <xsd:extension base="com.ibm.ws.jdbc.dataSource.properties.postgresql">
                <xsd:attribute name="id" type="factoryIdType" use="optional">
                    <xsd:annotation>
                        <xsd:documentation>Уникальный ИД конфигурации.</xsd:documentation>
                        <xsd:appinfo>
                            <ext:label>ИД</ext:label>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.jndi.internal.JNDIEntry">
        <xsd:annotation>
            <xsd:documentation>Одиночный элемент в пространстве имен JNDI по умолчанию.</xsd:documentation>
            <xsd:appinfo>
                <ext:label>Элемент JNDI</ext:label>
            </xsd:appinfo>
        </xsd:annotation>
        <!--Idiom for "empty element context" (whitespace accepted and ignored)-->
        <xsd:sequence>
            <xsd:sequence/>
        </xsd:sequence>
        <xsd:attribute name="jndiName" type="xsd:string" use="required">
            <xsd:annotation>
                <xsd:documentation>Имя JNDI для данного элемента.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Имя элемента JNDI</ext:label>
                    <ext:unique>jndiName</ext:unique>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="value" type="xsd:string" use="required">
            <xsd:annotation>
                <xsd:documentation>Значение JNDI, связанное с данным именем.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Значение элемента JNDI</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="false" name="decode" type="booleanType" use="optional">
            <xsd:annotation>
                <xsd:documentation>True, если значение должно быть расшифровано при поиске.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Значение декодирования JNDI</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.jndi.internal.JNDIEntry-factory">
        <xsd:complexContent>
            <xsd:extension base="com.ibm.ws.jndi.internal.JNDIEntry">
                <xsd:attribute name="id" type="factoryIdType" use="optional">
                    <xsd:annotation>
                        <xsd:documentation>Уникальный ИД конфигурации.</xsd:documentation>
                        <xsd:appinfo>
                            <ext:label>ИД</ext:label>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.jndi.internal.JNDIURLEntry">
        <xsd:annotation>
            <xsd:documentation>Одиночный элемент в пространстве имен JNDI по умолчанию, который используется для связывания элементов java.net.URL.</xsd:documentation>
            <xsd:appinfo>
                <ext:label>Элемент URL JNDI</ext:label>
            </xsd:appinfo>
        </xsd:annotation>
        <!--Idiom for "empty element context" (whitespace accepted and ignored)-->
        <xsd:sequence>
            <xsd:sequence/>
        </xsd:sequence>
        <xsd:attribute name="jndiName" type="xsd:string" use="required">
            <xsd:annotation>
                <xsd:documentation>Имя JNDI для данного элемента.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Имя элемента URL JNDI</ext:label>
                    <ext:unique>jndiName</ext:unique>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="value" type="xsd:string" use="required">
            <xsd:annotation>
                <xsd:documentation>Значение URL JNDI, связанное с данным именем.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Значение элемента URL JNDI</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.jndi.internal.JNDIURLEntry-factory">
        <xsd:complexContent>
            <xsd:extension base="com.ibm.ws.jndi.internal.JNDIURLEntry">
                <xsd:attribute name="id" type="factoryIdType" use="optional">
                    <xsd:annotation>
                        <xsd:documentation>Уникальный ИД конфигурации.</xsd:documentation>
                        <xsd:appinfo>
                            <ext:label>ИД</ext:label>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.jndi.referenceEntry.properties">
        <xsd:annotation>
            <xsd:documentation>Свойства для базовой записи.</xsd:documentation>
            <xsd:appinfo>
                <ext:label>Свойства</ext:label>
                <ext:extraProperties/>
            </xsd:appinfo>
        </xsd:annotation>
        <!--Idiom for "empty element context" (whitespace accepted and ignored)-->
        <xsd:sequence>
            <xsd:sequence/>
        </xsd:sequence>
        <xsd:attribute name="internal.properties" type="internalPropertiesType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Дополнительные свойства для базовой записи.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Дополнительные свойства</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:anyAttribute processContents="skip"/>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.jndi.referenceEntry.properties-factory">
        <xsd:complexContent>
            <xsd:extension base="com.ibm.ws.jndi.referenceEntry.properties">
                <xsd:attribute name="id" type="factoryIdType" use="optional">
                    <xsd:annotation>
                        <xsd:documentation>Уникальный ИД конфигурации.</xsd:documentation>
                        <xsd:appinfo>
                            <ext:label>ИД</ext:label>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.jndi.objectFactory">
        <xsd:annotation>
            <xsd:documentation>ObjectFactory для использования с базовой записью JNDI.</xsd:documentation>
            <xsd:appinfo>
                <ext:label>Фабрика объектов JNDI</ext:label>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element maxOccurs="1" minOccurs="0" name="library" type="com.ibm.ws.classloading.sharedlibrary">
                <xsd:annotation>
                    <xsd:documentation>Библиотека, содержащая класс реализации фабрики.</xsd:documentation>
                    <xsd:appinfo>
                        <ext:label>Общая библиотека</ext:label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="className" type="xsd:string" use="required">
            <xsd:annotation>
                <xsd:documentation>Имя класса реализации ObjectFactory.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Имя класса фабрики</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="java.lang.Object" name="objectClassName" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>Тип объекта, возвращаемого фабрикой.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Имя класса объекта</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="libraryRef" type="pidType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Библиотека, содержащая класс реализации фабрики.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Ссылка на общую библиотеку</ext:label>
                    <ext:reference>library</ext:reference>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.jndi.objectFactory-factory">
        <xsd:complexContent>
            <xsd:extension base="com.ibm.ws.jndi.objectFactory">
                <xsd:attribute name="id" type="factoryIdType" use="optional">
                    <xsd:annotation>
                        <xsd:documentation>Уникальный ИД конфигурации.</xsd:documentation>
                        <xsd:appinfo>
                            <ext:label>ИД</ext:label>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.jndi.referenceEntry">
        <xsd:annotation>
            <xsd:documentation>Базовая запись в стандартном пространстве имен JNDI.</xsd:documentation>
            <xsd:appinfo>
                <ext:label>Базовая запись JNDI</ext:label>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:choice maxOccurs="unbounded" minOccurs="0">
            <xsd:element name="factory" type="com.ibm.ws.jndi.objectFactory">
                <xsd:annotation>
                    <xsd:documentation>Фабрика объектов для базовой записи.</xsd:documentation>
                    <xsd:appinfo>
                        <ext:label>Фабрика объектов</ext:label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="properties" type="com.ibm.ws.jndi.referenceEntry.properties">
                <xsd:annotation>
                    <xsd:documentation>Свойства для базовой записи.</xsd:documentation>
                    <xsd:appinfo>
                        <ext:label>Свойства</ext:label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
        </xsd:choice>
        <xsd:attribute name="jndiName" type="xsd:string" use="required">
            <xsd:annotation>
                <xsd:documentation>Имя JNDI базовой записи.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Имя JNDI</ext:label>
                    <ext:unique>jndiName</ext:unique>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="factoryRef" type="pidType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Фабрика объектов для базовой записи.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Указатель на фабрику объектов</ext:label>
                    <ext:reference>jndiObjectFactory</ext:reference>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="false" name="decode" type="booleanType" use="optional">
            <xsd:annotation>
                <xsd:documentation>True, если значение должно быть расшифровано при поиске.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Значение декодирования JNDI</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.jndi.referenceEntry-factory">
        <xsd:complexContent>
            <xsd:extension base="com.ibm.ws.jndi.referenceEntry">
                <xsd:attribute name="id" type="factoryIdType" use="optional">
                    <xsd:annotation>
                        <xsd:documentation>Уникальный ИД конфигурации.</xsd:documentation>
                        <xsd:appinfo>
                            <ext:label>ИД</ext:label>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.jpacomponent">
        <xsd:annotation>
            <xsd:documentation>Свойства конфигурации для контейнера JPA.</xsd:documentation>
            <xsd:appinfo>
                <ext:label>Контейнер JPA</ext:label>
                <ext:groupDecl id="Advanced"/>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:choice maxOccurs="unbounded" minOccurs="0">
            <xsd:element default="" name="excludedApplication" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>Приложение для исключения из обработки JPA.</xsd:documentation>
                    <xsd:appinfo>
                        <ext:label>Исключенное приложение</ext:label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="defaultProperties" type="com.ibm.ws.jpacomponent.defaultProperties">
                <xsd:annotation>
                    <xsd:documentation>Свойства хранилища по умолчанию уровня интеграции, предназначенные для использования поставщиком хранилища, когда создается фабрика администратора сущностей, управляемых контейнером. Указанные свойства хранилища по умолчанию добавляются при вызове метода PersistenceProvider.createContainerEntityManagerFactory. Если в файле persistence.xml указано то же свойство хранилища по умолчанию, эта конфигурация переопределяет значение свойства, заданное в файле persistence.xml.</xsd:documentation>
                    <xsd:appinfo>
                        <ext:label>Свойства хранилища по умолчанию</ext:label>
                        <ext:group id="Advanced"/>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
        </xsd:choice>
        <xsd:attribute default="" name="defaultJtaDataSourceJndiName" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>Имя JNDI источника данных Java™ Transaction API (JTA) для использования приложениями, которые выполняются на этом сервере. По умолчанию все источники данных - JTA. В этом поле допустимы только источники данных для транзакций.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Имя JNDI источника данных JTA по умолчанию</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="" name="defaultNonJtaDataSourceJndiName" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>Имя JNDI источника данных для остальных целей, используемого приложениями на этом сервере. В этом поле допустимы только источники данных, не используемые для транзакций.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Имя JNDI источника данных без поддержки JTA по умолчанию</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="defaultPersistenceProvider" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>Имя класса стандартного поставщика хранилища. Если это свойство не указано, то стандартный поставщик хранилища зависит от того, какой компонент JPA включен.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Стандартный поставщик хранилища</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="-1" name="entityManagerPoolCapacity" type="intType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Вместимость пула EntityManager на каждую ссылку PersistenceContext. Минимальное значение равно 0, а максимальное - 500.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Вместимость пула EntityManager</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ignoreDataSourceErrors" type="booleanType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Если задано значение true, то ошибки, происходящие во время попытки поиска в источнике данных, указанном в элементах jta-data-source или non-jta-data-source в файле persistence.xml, игнорируются и о них создается отчет, что позволяет поставщику хранилища определить источник данных по умолчанию. Если задано значение false, то ошибки передаются в поставщик хранилища, чтобы упростить диагностику ошибок, но при этом приложения с ошибками настройки могут не работать. По умолчанию этому свойству присвоено значение, если включен JPA 2.0, или false в противном случае.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Игнорировать ошибки источника данных</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.jpacomponent.defaultProperties">
        <xsd:annotation>
            <xsd:documentation>Свойства хранилища по умолчанию уровня интеграции, предназначенные для использования поставщиком хранилища, когда создается фабрика администратора сущностей, управляемых контейнером. Указанные свойства хранилища по умолчанию добавляются при вызове метода PersistenceProvider.createContainerEntityManagerFactory. Если в файле persistence.xml указано то же свойство хранилища по умолчанию, эта конфигурация переопределяет значение свойства, заданное в файле persistence.xml.</xsd:documentation>
            <xsd:appinfo>
                <ext:label>Свойства хранилища по умолчанию</ext:label>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="property" type="com.ibm.ws.jpacomponent.defaultProperties.property-factory">
                <xsd:annotation>
                    <xsd:documentation>Задает конкретное свойство хранилища для всех фабрик администратора сущностей, управляемых контейнером.</xsd:documentation>
                    <xsd:appinfo>
                        <ext:label>Пара имя-значение, описывающая свойство хранилища</ext:label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.jpacomponent.defaultProperties-factory">
        <xsd:complexContent>
            <xsd:extension base="com.ibm.ws.jpacomponent.defaultProperties">
                <xsd:attribute name="id" type="factoryIdType" use="optional">
                    <xsd:annotation>
                        <xsd:documentation>Уникальный ИД конфигурации.</xsd:documentation>
                        <xsd:appinfo>
                            <ext:label>ИД</ext:label>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.jpacomponent.defaultProperties.property">
        <xsd:annotation>
            <xsd:documentation>Задает конкретное свойство хранилища для всех фабрик администратора сущностей, управляемых контейнером.</xsd:documentation>
            <xsd:appinfo>
                <ext:label>Пара имя-значение, описывающая свойство хранилища</ext:label>
            </xsd:appinfo>
        </xsd:annotation>
        <!--Idiom for "empty element context" (whitespace accepted and ignored)-->
        <xsd:sequence>
            <xsd:sequence/>
        </xsd:sequence>
        <xsd:attribute default="" name="name" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>Имя свойства хранилища уровня интеграции.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Имя свойства хранилища</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="" name="value" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>Значение свойства хранилища уровня интеграции.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Значение свойства хранилища</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.jpacomponent.defaultProperties.property-factory">
        <xsd:complexContent>
            <xsd:extension base="com.ibm.ws.jpacomponent.defaultProperties.property">
                <xsd:attribute name="id" type="factoryIdType" use="optional">
                    <xsd:annotation>
                        <xsd:documentation>Уникальный ИД конфигурации.</xsd:documentation>
                        <xsd:appinfo>
                            <ext:label>ИД</ext:label>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.jsp.2.2">
        <xsd:annotation>
            <xsd:documentation>Конфигурация JSP</xsd:documentation>
            <xsd:appinfo>
                <ext:label>Служба JSP</ext:label>
            </xsd:appinfo>
        </xsd:annotation>
        <!--Idiom for "empty element context" (whitespace accepted and ignored)-->
        <xsd:sequence>
            <xsd:sequence/>
        </xsd:sequence>
        <xsd:attribute default="false" name="disableJspRuntimeCompilation" type="booleanType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Выключить компиляцию JSP во время выполнения.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Выключить компиляцию JSP во время выполнения</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="" name="extendedDocumentRoot" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>Каталог, в котором модуль JSP выполняет поиск дополнительных файлов JSP для обработки.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Корневой каталог дополнительных документов</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="javaSourceLevel" use="optional">
            <xsd:annotation>
                <xsd:documentation>Задает исходный уровень Java по умолчанию для JSP, скомпилирующих механизм JSP. Принимает одно из следующих выпусков LTS: 8, 11, 17 и 21.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Уровень исходного кода Java</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union memberTypes="variableType">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="8">
                                <xsd:annotation>
                                    <xsd:documentation>8</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="11">
                                <xsd:annotation>
                                    <xsd:documentation>11</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="17">
                                <xsd:annotation>
                                    <xsd:documentation>17</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="21">
                                <xsd:annotation>
                                    <xsd:documentation>21</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute default="false" name="keepGenerated" type="booleanType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Позволяет сохранять исходные файлы Java, созданные для JSP.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Сохранять сгенерированный исходный код Java</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="false" name="recompileJspOnRestart" type="booleanType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Заново компилировать JSP при перезапуске приложения. Повторная компиляция JSP выполняется при первом обращении.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Заново компилировать JSP при перезапуске</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="true" name="useImplicitTagLibs" type="booleanType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Разрешить JSP использовать библиотеки тегов jsx и tsx.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Использовать неявные библиотеки тегов</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="false" name="disableResourceInjection" type="booleanType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Выключить добавление ресурсов в JSP.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Выключить добавление ресурсов</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="prepareJSPs" type="intType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Когда этот атрибут присутствует, все JSP, которые больше данного значения (в килобайтах), компилируются при запуске сервера приложений.  Установите для этого атрибута значение 0, чтобы скомпилировать все JSP.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Переводить и компилировать JSP при запуске</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="scratchdir" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>Если этот атрибут задан, то JSP компилируются не в каталог рабочей области на сервере, а в указанный каталог.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Каталог для хранения файлов созданного класса</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="useJDKCompiler" type="booleanType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Когда задан этот атрибут, JSP компилируются компилятором из Java SDK.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Компилировать JSP компилятором из Java SDK</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="1" name="prepareJSPThreadCount" type="intType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Этот параметр задает число нитей, применяемых в случае, если включена опция prepareJSPs.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Число нитей в случае, когда включена опция prepareJSPs</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.config">
        <xsd:annotation>
            <xsd:documentation>Определяет, как сервер обрабатывает информацию о конфигурации.</xsd:documentation>
            <xsd:appinfo>
                <ext:label>Управление конфигурацией</ext:label>
            </xsd:appinfo>
        </xsd:annotation>
        <!--Idiom for "empty element context" (whitespace accepted and ignored)-->
        <xsd:sequence>
            <xsd:sequence/>
        </xsd:sequence>
        <xsd:attribute default="WARN" name="onError" use="optional">
            <xsd:annotation>
                <xsd:documentation>Действие, выполняемое при возникновении ошибки конфигурации. </xsd:documentation>
                <xsd:appinfo>
                    <ext:label>При ошибке</ext:label>
                    <ext:variable>onError</ext:variable>
                </xsd:appinfo>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union memberTypes="variableType">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="WARN">
                                <xsd:annotation>
                                    <xsd:documentation>Сервер будет выдавать предупреждения и сообщения об ошибках при обнаружении ошибки конфигурации. </xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="FAIL">
                                <xsd:annotation>
                                    <xsd:documentation>Сервер выдаст предупреждение или сообщение об ошибке при первом возникновении ошибки и остановится.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="IGNORE">
                                <xsd:annotation>
                                    <xsd:documentation>Сервер не будет выдавать предупреждения и сообщения об ошибках при обнаружении ошибки конфигурации.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute default="500ms" name="monitorInterval" type="duration" use="optional">
            <xsd:annotation>
                <xsd:documentation>Частота проверки сервером наличия обновлений конфигурации. Укажите положительное целое число, за которым следует единица времени: часы (h), минуты (m), секунды (s) или миллисекунды (ms). Например, укажите 500 миллисекунд как 500ms. Одна запись может включать несколько значений. Так, 1s500ms равно 1,5 сек.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Частота проверки обновлений</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="polled" name="updateTrigger" use="optional">
            <xsd:annotation>
                <xsd:documentation>Метод или триггер обновления конфигурации.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Триггер обновления</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union memberTypes="variableType">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="polled">
                                <xsd:annotation>
                                    <xsd:documentation>Сервер будет проверять наличие изменений во всех файлах конфигурации с заданной частотой и обновлять конфигурацию с учетом обнаруженных изменений.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="mbean">
                                <xsd:annotation>
                                    <xsd:documentation>Сервер обновляет конфигурацию только по запросу FileNotificationMbean. FileNotificationMbean обычно вызывается внешней программой, такой как интегрированная среда разработки или приложение управления.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="disabled">
                                <xsd:annotation>
                                    <xsd:documentation>Полностью выключает мониторинг обновлений. Изменения конфигурации не будут применены во время работы сервера.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.kernel.metatype.helper.fileset">
        <xsd:annotation>
            <xsd:documentation>Указывает набор файлов, начинающийся с базового каталога и соответствующий набору шаблонов.</xsd:documentation>
            <xsd:appinfo>
                <ext:label>Набор файлов</ext:label>
            </xsd:appinfo>
        </xsd:annotation>
        <!--Idiom for "empty element context" (whitespace accepted and ignored)-->
        <xsd:sequence>
            <xsd:sequence/>
        </xsd:sequence>
        <xsd:attribute default="${server.config.dir}" name="dir" type="dirLocation" use="optional">
            <xsd:annotation>
                <xsd:documentation>Базовый каталог для поиска файлов.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Базовый каталог</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="true" name="caseSensitive" type="booleanType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Булевское значение, разрешающее или запрещающее поиск с учетом регистра (значение по умолчанию: true).</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>С учетом регистра</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="*" name="includes" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>Перечисленные через запятую или пробел шаблоны имен файлов, которые следует включить в результаты поиска (значение по умолчанию: *).</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Шаблон включения</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="" name="excludes" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>Перечисленные через запятую или пробел шаблоны имен файлов, которые следует исключить из результатов поиска (по умолчанию не исключаются никакие файлы).</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Шаблон исключения</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="0" name="scanInterval" type="duration" use="optional">
            <xsd:annotation>
                <xsd:documentation>Интервал сканирования для определения того, будут ли файлы добавлены или удалены из набора файлов. Отдельные файлы не сканируются. Суффикс для интервала времени-h-hour, m-minute, s-second и ms-millisecond, например, 2ms или 5s. По умолчанию интервал сканирования отключен, и он отключается вручную, задав интервал сканирования, scanInterval, равным 0. Укажите положительное целое число, за которым следует единица времени: часы (h), минуты (m), секунды (s) или миллисекунды (ms). Например, укажите 500 миллисекунд как 500ms. Одна запись может включать несколько значений. Так, 1s500ms равно 1,5 сек.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Интервал проверки</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.kernel.metatype.helper.fileset-factory">
        <xsd:complexContent>
            <xsd:extension base="com.ibm.ws.kernel.metatype.helper.fileset">
                <xsd:attribute name="id" type="factoryIdType" use="optional">
                    <xsd:annotation>
                        <xsd:documentation>Уникальный ИД конфигурации.</xsd:documentation>
                        <xsd:appinfo>
                            <ext:label>ИД</ext:label>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.threading">
        <xsd:annotation>
            <xsd:documentation>Определяет параметры для стандартного исполнителя ядра Liberty. Обратите внимание, что всегда существует один и только один исполнитель, который используется лишь средой выполнения Liberty и недоступен приложениям непосредственно. Приложения, которым требуется настраивать и использовать исполнители, должны вместо этого использовать управляемые исполнители.</xsd:documentation>
            <xsd:appinfo>
                <ext:label>Управление исполнителя</ext:label>
            </xsd:appinfo>
        </xsd:annotation>
        <!--Idiom for "empty element context" (whitespace accepted and ignored)-->
        <xsd:sequence>
            <xsd:sequence/>
        </xsd:sequence>
        <xsd:attribute default="Default Executor" name="name" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>Имя стандартного исполнителя ядра Liberty.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Имя</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="-1" name="maxThreads" type="intType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Максимальное число нитей, которые можно связать с исполнителем. Если это значение больше 0, то значение maxThreads должно быть не ниже 4 и выше либо равно значению coreThreads; если значение maxThreads меньше значения coreThreads, Liberty уменьшит число coreThreads до величины maxThreads. Если значение maxThreads меньше либо равно 0, то максимальное число нитей не ограничивается. Это позволяет ядру Liberty определить, сколько нитей требуется связать с исполнителем без определения верхнего предела.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Максимальное число нитей</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="-1" name="coreThreads" type="intType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Базовое или минимальное число нитей, связанных с исполнителем. Число нитей исполнителя будет быстро увеличиваться до этого значения. Если указано значение меньше 0, то будет использоваться значение по умолчанию, которое вычисляется на основе числа аппаратных нитей в системе. Если значение является положительным числом меньше 4, будет использоваться значение по умолчанию, равное 4.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Нити ядра</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.kernel.feature">
        <xsd:annotation>
            <xsd:documentation>Определяет, как сервер загружает комплекты.</xsd:documentation>
            <xsd:appinfo>
                <ext:label>Администратор функций</ext:label>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:choice maxOccurs="unbounded" minOccurs="0">
            <xsd:element name="platform" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>Платформа указывает версию модели программирования, например jakartaee-10.0 . Версия платформы определяет версию любых включенных безверсионных функций, которые включены в указанную платформу. </xsd:documentation>
                    <xsd:appinfo>
                        <ext:label>Платформа</ext:label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="feature" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>Указывает компонент для использования при выполнении сервера.</xsd:documentation>
                    <xsd:appinfo>
                        <ext:label>Функция</ext:label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
        </xsd:choice>
        <xsd:attribute default="WARN" name="onError" use="optional">
            <xsd:annotation>
                <xsd:documentation>Действие, выполняемое при сбое загрузки комплекта. </xsd:documentation>
                <xsd:appinfo>
                    <ext:label>При ошибке</ext:label>
                    <ext:variable>onError</ext:variable>
                </xsd:appinfo>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union memberTypes="variableType">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="WARN">
                                <xsd:annotation>
                                    <xsd:documentation>Сервер будет выдавать предупреждения и сообщения об ошибках при обнаружении ошибки конфигурации. </xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="FAIL">
                                <xsd:annotation>
                                    <xsd:documentation>Сервер выдаст предупреждение или сообщение об ошибке при первом возникновении ошибки и остановится.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="IGNORE">
                                <xsd:annotation>
                                    <xsd:documentation>Сервер не будет выдавать предупреждения и сообщения об ошибках при обнаружении ошибки конфигурации.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.security.registry.ldap.internal.filters.secureway">
        <xsd:annotation>
            <xsd:documentation>Список фильтров LDAP IBM SecureWay Directory Server.</xsd:documentation>
            <xsd:appinfo>
                <ext:label>Фильтры LDAP IBM SecureWay Directory Server</ext:label>
            </xsd:appinfo>
        </xsd:annotation>
        <!--Idiom for "empty element context" (whitespace accepted and ignored)-->
        <xsd:sequence>
            <xsd:sequence/>
        </xsd:sequence>
        <xsd:attribute default="(&amp;(uid=%v)(objectclass=ePerson))" name="userFilter" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>Фильтр LDAP для поиска пользователей в реестре пользователей. Если задан, то этот фильтр требует утверждение значения атрибута (AVA), содержащее %v. Пример: uid=%v. В процессе поиска значение %v в AVA заменяется на имя или шаблон пользователя, которого требуется найти. </xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Фильтр пользователей</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="(&amp;(cn=%v)(|(objectclass=groupOfNames)(objectclass=groupOfUniqueNames)))" name="groupFilter" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>Фильтр LDAP для поиска групп в реестре пользователей. Если задан, то этот фильтр требует утверждение значения атрибута (AVA), содержащее %v. Пример: cn=%v. В процессе поиска значение %v в AVA заменяется на имя или шаблон группы, которую требуется найти.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Фильтр групп</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="*:uid" name="userIdMap" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>Фильтр LDAP, применяемый для преобразования имени пользователя в запись LDAP.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Карта ИД пользователей</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="*:cn" name="groupIdMap" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>Фильтр LDAP, применяемый для преобразования имени группы в запись LDAP.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Карта ИД групп</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="groupOfNames:member;groupOfUniqueNames:uniqueMember" name="groupMemberIdMap" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>Фильтр LDAP для связи имен пользователей и групп.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Карта идентификаторов участников групп</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.security.registry.ldap.internal.filters.secureway-factory">
        <xsd:complexContent>
            <xsd:extension base="com.ibm.ws.security.registry.ldap.internal.filters.secureway">
                <xsd:attribute name="id" type="factoryIdType" use="optional">
                    <xsd:annotation>
                        <xsd:documentation>Уникальный ИД конфигурации.</xsd:documentation>
                        <xsd:appinfo>
                            <ext:label>ИД</ext:label>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.security.registry.ldap.config">
        <xsd:annotation>
            <xsd:documentation>Свойства конфигурации реестра пользователей LDAP.</xsd:documentation>
            <xsd:appinfo>
                <ext:label>Реестр пользователей LDAP</ext:label>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:choice maxOccurs="unbounded" minOccurs="0">
            <xsd:element name="failoverServers" type="com.ibm.ws.security.registry.ldap.config.failoverServers-factory">
                <xsd:annotation>
                    <xsd:documentation>Список резервных серверов LDAP.</xsd:documentation>
                    <xsd:appinfo>
                        <ext:label>Резервные серверы LDAP</ext:label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="activedFilters" type="com.ibm.ws.security.registry.ldap.internal.filters.actived">
                <xsd:annotation>
                    <xsd:documentation>Список фильтров LDAP Microsoft Active Directory.</xsd:documentation>
                    <xsd:appinfo>
                        <ext:label>Фильтры LDAP Microsoft Active Directory</ext:label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="customFilters" type="com.ibm.ws.security.registry.ldap.internal.filters.custom">
                <xsd:annotation>
                    <xsd:documentation>Список пользовательских фильтров LDAP.</xsd:documentation>
                    <xsd:appinfo>
                        <ext:label>Пользовательские фильтры LDAP</ext:label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="domino50Filters" type="com.ibm.ws.security.registry.ldap.internal.filters.domino50">
                <xsd:annotation>
                    <xsd:documentation>Список фильтров LDAP IBM Lotus Domino.</xsd:documentation>
                    <xsd:appinfo>
                        <ext:label>Фильтры LDAP IBM Lotus Domino</ext:label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="edirectoryFilters" type="com.ibm.ws.security.registry.ldap.internal.filters.eDirectory">
                <xsd:annotation>
                    <xsd:documentation>Список фильтров LDAP Novell eDirectory.</xsd:documentation>
                    <xsd:appinfo>
                        <ext:label>Фильтры LDAP Novell eDirectory</ext:label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="idsFilters" type="com.ibm.ws.security.registry.ldap.internal.filters.ids">
                <xsd:annotation>
                    <xsd:documentation>Список фильтров LDAP IBM Tivoli Directory Server.</xsd:documentation>
                    <xsd:appinfo>
                        <ext:label>Фильтры LDAP IBM Tivoli Directory Server</ext:label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="iplanetFilters" type="com.ibm.ws.security.registry.ldap.internal.filters.iPlanet">
                <xsd:annotation>
                    <xsd:documentation>Список фильтров LDAP Sun Java System Directory Server.</xsd:documentation>
                    <xsd:appinfo>
                        <ext:label>Фильтры LDAP Sun Java System Directory Server</ext:label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="netscapeFilters" type="com.ibm.ws.security.registry.ldap.internal.filters.netscape">
                <xsd:annotation>
                    <xsd:documentation>Список фильтров LDAP Netscape Directory Server.</xsd:documentation>
                    <xsd:appinfo>
                        <ext:label>Фильтры LDAP Netscape Directory Server</ext:label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="securewayFilters" type="com.ibm.ws.security.registry.ldap.internal.filters.secureway">
                <xsd:annotation>
                    <xsd:documentation>Список фильтров LDAP IBM SecureWay Directory Server.</xsd:documentation>
                    <xsd:appinfo>
                        <ext:label>Фильтры LDAP IBM SecureWay Directory Server</ext:label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="loginProperty" type="com.ibm.ws.security.registry.ldap.config.loginProperty-factory">
                <xsd:annotation>
                    <xsd:documentation>Свойство WIM PersonAccount, применяемое для создания фильтра LDAP для поиска пользователей. Первый экземпляр этого атрибута возвращается в качестве имени субъекта пользователя. Связь между свойствами WIM и атрибутами LDAP можно изменить с помощью атрибута attributeConfiguration. Если этот атрибут задан, то он переопределяет userFilter. В этом атрибуте учитывается регистр символов.</xsd:documentation>
                    <xsd:appinfo>
                        <ext:label>Свойство входа в систему</ext:label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="ldapEntityType" type="com.ibm.ws.security.wim.adapter.ldap.entityType-factory">
                <xsd:annotation>
                    <xsd:documentation>Настройте класс объектов LDAP, фильтры поиска, базы поиска и относительное отличительное имя (RDN) LDAP для пользователя, группы и подразделения. Например тип сущности Group может иметь фильтр поиска (&amp;(ObjectCategory=Groupofnames)(ObjectClass=Groupofnames)) и класс объектов Groupofnames с базой поиска ou=iGroups,o=ibm,c=us.</xsd:documentation>
                    <xsd:appinfo>
                        <ext:label>Типы сущностей LDAP</ext:label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="groupProperties" type="com.ibm.ws.security.wim.adapter.ldap.groupConfiguration">
                <xsd:annotation>
                    <xsd:documentation>Настройка свойств членства в группе (например, memberAttribute или membershipAttribute).</xsd:documentation>
                    <xsd:appinfo>
                        <ext:label>Настроить параметры группы LDAP</ext:label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="attributeConfiguration" type="com.ibm.ws.security.wim.adapter.ldap.attributeConfiguration">
                <xsd:annotation>
                    <xsd:documentation>Конфигурация, преобразующая атрибуты LDAP в имена полей схемы реестра пользователей (пример: Person, PersonAccount или Group).</xsd:documentation>
                    <xsd:appinfo>
                        <ext:label>Преобразование атрибутов LDAP</ext:label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="contextPool" type="com.ibm.ws.security.wim.adapter.ldap.contextPool">
                <xsd:annotation>
                    <xsd:documentation>Свойства пула контекста.</xsd:documentation>
                    <xsd:appinfo>
                        <ext:label>Свойства пула контекста</ext:label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="ldapCache" type="com.ibm.ws.security.wim.adapter.ldap.cacheConfig">
                <xsd:annotation>
                    <xsd:documentation>Настройка атрибутов кэша.</xsd:documentation>
                    <xsd:appinfo>
                        <ext:label>Свойства кэша</ext:label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
        </xsd:choice>
        <xsd:attribute name="host" type="xsd:string" use="required">
            <xsd:annotation>
                <xsd:documentation>Адрес сервера LDAP (IP-адрес или имя DNS).</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Хост</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="port" type="intType" use="required">
            <xsd:annotation>
                <xsd:documentation>Порт сервера LDAP.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Порт</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="baseDN" type="xsd:string" use="required">
            <xsd:annotation>
                <xsd:documentation>Базовое отличительное имя (DN) службы каталогов, определяющее исходную точку поиска службы каталогов на сервере LDAP.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Базовое отличительное имя (DN)</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ldapType" use="required">
            <xsd:annotation>
                <xsd:documentation>Тип сервера LDAP, с которым устанавливается соединение.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Тип сервера LDAP</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union memberTypes="variableType">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="Microsoft Active Directory">
                                <xsd:annotation>
                                    <xsd:documentation>Настроить реестр LDAP для Microsoft Active Directory.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="Custom">
                                <xsd:annotation>
                                    <xsd:documentation>Настроить реестр LDAP для пользовательского сервера LDAP.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="IBM Lotus Domino">
                                <xsd:annotation>
                                    <xsd:documentation>Настроить реестр LDAP для IBM Lotus Domino.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="Novell eDirectory">
                                <xsd:annotation>
                                    <xsd:documentation>Настроить реестр LDAP для Novell eDirectory.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="IBM Tivoli Directory Server">
                                <xsd:annotation>
                                    <xsd:documentation>Настроить реестр LDAP для IBM Tivoli Directory Server.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="Sun Java System Directory Server">
                                <xsd:annotation>
                                    <xsd:documentation>Настроить реестр LDAP для Sun Java System Directory Server.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="Netscape Directory Server">
                                <xsd:annotation>
                                    <xsd:documentation>Настроить реестр LDAP для Netscape Directory Server.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="IBM SecureWay Directory Server">
                                <xsd:annotation>
                                    <xsd:documentation>Настроить реестр LDAP для IBM SecureWay Directory Server.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute default="LdapRegistry" name="realm" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>Имя области, представляющей реестр пользователей.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Имя области</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="bindDN" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>Отличительное имя (DN) сервера приложений, применяемое для связывания со службой каталогов.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Отличительное имя связывания (DN)</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="bindPassword" type="password" use="optional">
            <xsd:annotation>
                <xsd:documentation>Пароль для DN связывания. Это значение может храниться как зашифрованное или незашифрованное. Рекомендуется зашифровать пароль. Для этого используется утилита securityUtility с параметром encode.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Пароль для связывания</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="true" name="ignoreCase" type="booleanType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Выполнить идентификацию без учета регистра.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Игнорировать регистр для авторизации</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="false" name="recursiveSearch" type="booleanType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Позволяет выполнить поиск во вложенных группах. Выберите эту опцию только в том случае, если сервер LDAP не поддерживает рекурсивный поиск на сервере.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Выполнить поиск во вложенных группах</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="true" name="reuseConnection" type="booleanType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Разрешает серверу приложений повторно использовать соединение LDAP.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Повторное использование соединения</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="false" name="sslEnabled" type="booleanType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Указывает, можно ли использовать соединение SSL с сервером LDAP.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Включена поддержка SSL LDAP</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="sslRef" type="pidType" use="optional">
            <xsd:annotation>
                <xsd:documentation>ИД конфигурации SSL для подключения к серверу LDAP с поддержкой SSL.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Ссылка на SSL</ext:label>
                    <ext:reference>ssl</ext:reference>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="1m" name="searchTimeout" type="duration" use="optional">
            <xsd:annotation>
                <xsd:documentation>Максимальное время ожидания ответа сервера LDAP, по истечении которого запрос будет отменен. Укажите положительное целое число, за которым следует единица времени: часы (h), минуты (m), секунды (s) или миллисекунды (ms). Например, укажите 500 миллисекунд как 500ms. Одна запись может включать несколько значений. Так, 1s500ms равно 1,5 сек.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Тайм-аут поиска</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="1m" name="connectTimeout" type="duration" use="optional">
            <xsd:annotation>
                <xsd:documentation>Максимальное время установки соединения с сервером LDAP. Значение 0 означает, что будет использоваться значение тайм-аута протокола TCP. По истечении этого времени программа выводит сообщение об ошибке в протокол. Укажите положительное целое число, за которым следует единица времени: часы (h), минуты (m), секунды (s) или миллисекунды (ms). Например, укажите 500 миллисекунд как 500ms. Одна запись может включать несколько значений. Так, 1s500ms равно 1,5 сек.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Тайм-аут установки соединения с сервером LDAP</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="1m" name="readTimeout" type="duration" use="optional">
            <xsd:annotation>
                <xsd:documentation>Максимальное время ожидания операции чтения из LDAP. Значение 0 означает отсутствие тайм-аута и бесконечное ожидание чтения. По истечении этого времени программа выводит сообщение об ошибке в протокол. Укажите положительное целое число, за которым следует единица времени: часы (h), минуты (m), секунды (s) или миллисекунды (ms). Например, укажите 500 миллисекунд как 500ms. Одна запись может включать несколько значений. Так, 1s500ms равно 1,5 сек.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Тайм-аут чтения данных из соединения с сервером LDAP</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="EXACT_DN" name="certificateMapMode" use="optional">
            <xsd:annotation>
                <xsd:documentation>Задает режим преобразования идентификации сертификатов X.509 для реестра LDAP: CUSTOM, EXACT_DN, CERTIFICATE_FILTER или NOT_SUPPORTED.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Режим отображения сертификатов</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union memberTypes="variableType">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="EXACT_DN">
                                <xsd:annotation>
                                    <xsd:documentation>Реестр LDAP выполняет идентификацию сертификата X.509 путем преобразования значения PrincipalName в сертификате X.509 в точное отличительное имя (DN) в хранилище. Если найдена одна соответствующая сущность, идентификация считается успешной. Если соответствующая сущность не найдена, возникает ошибка идентификации.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="CERTIFICATE_FILTER">
                                <xsd:annotation>
                                    <xsd:documentation>Реестр LDAP выполняет идентификацию сертификата X.509 с помощью свойства преобразования фильтра сертификата для фильтра LDAP. Если найдена одна соответствующая сущность, идентификация считается успешной. Если соответствующая сущность не найдена, возникает ошибка идентификации.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="CUSTOM">
                                <xsd:annotation>
                                    <xsd:documentation>Реестр LDAP выполняет идентификацию сертификата X.509 с помощью пользовательской реализации CertificateMapper, указанной в атрибуте certificateMapperId. Если найдена одна соответствующая сущность, идентификация считается успешной. Если соответствующая сущность не найдена, возникает ошибка идентификации. </xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="NOT_SUPPORTED">
                                <xsd:annotation>
                                    <xsd:documentation>Реестр LDAP не поддерживает идентификацию сертификатов X.509. Выполнить идентификацию с помощью сертификата X.509 не удастся, и возникнет исключительная ситуация CertificateMapNotSupportedException.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute name="certificateFilter" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>Задает свойство преобразования сертификата фильтра для фильтра LDAP, если применяется следующий режим преобразования идентификации сертификата X.509: CERTIFICATE_FILTER. Этот фильтр применяется для связи атрибутов сертификата клиента с записями в реестре LDAP. Например, фильтр может быть задан как uid=${SubjectCN}.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Фильтр отображения сертификатов</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="certificateMapperId" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>Задает X509CertificateMapper, применяемый в том случае, если задан следующий режим преобразования для идентификации сертификатов X.509: CUSTOM. Это значение должно совпадать со значением свойства 'x509.certificate.mapper.id', которое указано для реализации X509CertificateMapper.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>ИД модуль преобразования сертификатов</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="activedFiltersRef" type="pidType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Список фильтров LDAP Microsoft Active Directory.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Справочник по фильтрам LDAP Microsoft Active Directory</ext:label>
                    <ext:reference>activedLdapFilterProperties</ext:reference>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="customFiltersRef" type="pidType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Список пользовательских фильтров LDAP.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Справочник по пользовательским фильтрам LDAP</ext:label>
                    <ext:reference>customLdapFilterProperties</ext:reference>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="domino50FiltersRef" type="pidType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Список фильтров LDAP IBM Lotus Domino.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Справочник по фильтрам LDAP IBM Lotus Domino</ext:label>
                    <ext:reference>domino50LdapFilterProperties</ext:reference>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="edirectoryFiltersRef" type="pidType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Список фильтров LDAP Novell eDirectory.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Справочник по фильтрам LDAP Novell eDirectory</ext:label>
                    <ext:reference>edirectoryLdapFilterProperties</ext:reference>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="idsFiltersRef" type="pidType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Список фильтров LDAP IBM Tivoli Directory Server.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Справочник по фильтрам LDAP IBM Tivoli Directory</ext:label>
                    <ext:reference>idsLdapFilterProperties</ext:reference>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="iplanetFiltersRef" type="pidType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Список фильтров LDAP Sun Java System Directory Server.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Справочник по фильтрам LDAP Sun Java System Directory</ext:label>
                    <ext:reference>iplanetLdapFilterProperties</ext:reference>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="netscapeFiltersRef" type="pidType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Список фильтров LDAP Netscape Directory Server.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Справочник по фильтрам LDAP Netscape Directory Server</ext:label>
                    <ext:reference>netscapeLdapFilterProperties</ext:reference>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="securewayFiltersRef" type="pidType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Список фильтров LDAP IBM SecureWay Directory Server.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Справочник по фильтрам LDAP IBM SecureWay Directory</ext:label>
                    <ext:reference>securewayLdapFilterProperties</ext:reference>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="ignore" name="referral" use="optional">
            <xsd:annotation>
                <xsd:documentation>Настройка поведения переадресации LDAP. По умолчанию переадресация игнорируется.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Обработка переадресации LDAP</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union memberTypes="variableType">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="ignore">
                                <xsd:annotation>
                                    <xsd:documentation>Игнорировать переадресацию LDAP.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="follow">
                                <xsd:annotation>
                                    <xsd:documentation>Обрабатывать переадресацию LDAP.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute default="always" name="derefAliases" use="optional">
            <xsd:annotation>
                <xsd:documentation>При обнаружении записи псевдонима указывает, следует ли раскрывать псевдоним для возврата объекта, на который указывает DN псевдонима.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Обработка раскрытия псевдонимов</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union memberTypes="variableType">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="always">
                                <xsd:annotation>
                                    <xsd:documentation>Всегда раскрывать псевдонимы.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="never">
                                <xsd:annotation>
                                    <xsd:documentation>Никогда не раскрывать псевдонимы.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute default="15" name="primaryServerQueryTimeInterval" type="intType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Интервал (в минутах), с которым администратор виртуального элемента тестирует доступность основного сервера.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Интервал времени между запросами основного сервера</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="true" name="returnToPrimaryServer" type="booleanType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Булевское значение, которое указывает, должен ли поиск выполняться для основного сервера.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Вернуть на основной сервер</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="timestampFormat" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>Строковое представление шаблона SimpleDateFormat, используемое для анализа значений атрибутов системного времени. Например, можно использовать 'ггггММддЧЧммсс.SSSZ' для анализа '20181120214852.869-0000Z'. Если этот атрибут не определен, то по умолчанию будет использоваться значение, основанное на ldapType.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Формат системного времени</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="false" name="allowWriteToSecondaryServers" type="booleanType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Разрешить операции создания, обновления и удаления на резервных серверах.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Разрешить запись на дополнительных серверах</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="simple" name="bindAuthMechanism" use="optional">
            <xsd:annotation>
                <xsd:documentation>Механизм идентификации для связывания с сервером LDAP в ходе поиска или изменения записи LDAP.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Механизм идентификации связывания</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union memberTypes="variableType">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="none">
                                <xsd:annotation>
                                    <xsd:documentation>Анонимное связывание со службой каталогов. Дополнительные атрибуты входа в систему не требуются.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="simple">
                                <xsd:annotation>
                                    <xsd:documentation>Используйте атрибуты bindDN и bindPassword для связывания со службой каталогов. Атрибуты bindDN и bindPassword являются обязательными.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="GSSAPI">
                                <xsd:annotation>
                                    <xsd:documentation>Поддержка GSS-API Kerberos v5 (krb5) для связывания со службой каталогов. Требуется атрибут krb5Principal</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute name="krb5Principal" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>Имя применяемого субъекта или службы Kerberos.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Имя субъекта Kerberos</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="krb5TicketCache" type="fileLocation" use="optional">
            <xsd:annotation>
                <xsd:documentation>Расположение файла, в котором будут храниться идентификационные данные службы или субъекта Kerberos. Другое название - кэш идентификационных данных Kerberos (ccache)</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Кэш паспортов Kerberos</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.security.registry.ldap.config-factory">
        <xsd:complexContent>
            <xsd:extension base="com.ibm.ws.security.registry.ldap.config">
                <xsd:attribute name="id" type="factoryIdType" use="optional">
                    <xsd:annotation>
                        <xsd:documentation>Уникальный ИД конфигурации.</xsd:documentation>
                        <xsd:appinfo>
                            <ext:label>ИД</ext:label>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.security.wim.adapter.config.registryBaseEntry">
        <xsd:annotation>
            <xsd:documentation>Конфигурация базовой записи реестра. Часть дерева под этим узлом будет также частью хранилища.</xsd:documentation>
            <xsd:appinfo>
                <ext:label>Базовая запись реестра</ext:label>
            </xsd:appinfo>
        </xsd:annotation>
        <!--Idiom for "empty element context" (whitespace accepted and ignored)-->
        <xsd:sequence>
            <xsd:sequence/>
        </xsd:sequence>
        <xsd:attribute name="baseDN" type="xsd:string" use="required">
            <xsd:annotation>
                <xsd:documentation>Базовое отличительное имя (DN) службы каталогов, определяющее исходную точку поиска службы каталогов на сервере LDAP.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Базовое отличительное имя (DN)</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="name" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>Базовое имя базовой записи реестра.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Базовое имя</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.security.wim.adapter.config.registryBaseEntry-factory">
        <xsd:complexContent>
            <xsd:extension base="com.ibm.ws.security.wim.adapter.config.registryBaseEntry">
                <xsd:attribute name="id" type="factoryIdType" use="optional">
                    <xsd:annotation>
                        <xsd:documentation>Уникальный ИД конфигурации.</xsd:documentation>
                        <xsd:appinfo>
                            <ext:label>ИД</ext:label>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.security.wim.adapter.ldap.attributeCache">
        <xsd:annotation>
            <xsd:documentation>Свойства кэширования атрибута</xsd:documentation>
            <xsd:appinfo>
                <ext:label>Кэш атрибута LDAP</ext:label>
            </xsd:appinfo>
        </xsd:annotation>
        <!--Idiom for "empty element context" (whitespace accepted and ignored)-->
        <xsd:sequence>
            <xsd:sequence/>
        </xsd:sequence>
        <xsd:attribute default="true" name="enabled" type="booleanType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Булевское значение, которое указывает, включено ли свойство.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Включено</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="2000" name="size" type="intType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Определяет число субъектов, которые можно хранить в кэше. Размер кэша можно увеличить на основе числа субъектов, которое требуется хранить в кэше.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Размер</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="1200s" name="timeout" type="duration" use="optional">
            <xsd:annotation>
                <xsd:documentation>Определяет максимальное время доступности содержимого кэша атрибутов LDAP. После завершения указанного времени кэш атрибутов LDAP будет очищен. Укажите положительное целое число, за которым следует единица времени: часы (h), минуты (m), секунды (s) или миллисекунды (ms). Например, укажите 500 миллисекунд как 500ms. Одна запись может включать несколько значений. Так, 1s500ms равно 1,5 сек.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Тайм-аут</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="2000" name="sizeLimit" type="intType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Максимальное число атрибутов на объект LDAP, которое будет помещено в кэш.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Максимальное число атрибутов объекта в кэше</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.security.wim.adapter.ldap.attributeCache-factory">
        <xsd:complexContent>
            <xsd:extension base="com.ibm.ws.security.wim.adapter.ldap.attributeCache">
                <xsd:attribute name="id" type="factoryIdType" use="optional">
                    <xsd:annotation>
                        <xsd:documentation>Уникальный ИД конфигурации.</xsd:documentation>
                        <xsd:appinfo>
                            <ext:label>ИД</ext:label>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.security.registry.ldap.internal.filters.eDirectory">
        <xsd:annotation>
            <xsd:documentation>Список фильтров LDAP Novell eDirectory.</xsd:documentation>
            <xsd:appinfo>
                <ext:label>Фильтры LDAP Novell eDirectory</ext:label>
            </xsd:appinfo>
        </xsd:annotation>
        <!--Idiom for "empty element context" (whitespace accepted and ignored)-->
        <xsd:sequence>
            <xsd:sequence/>
        </xsd:sequence>
        <xsd:attribute default="(&amp;(cn=%v)(objectclass=Person))" name="userFilter" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>Фильтр LDAP для поиска пользователей в реестре пользователей. Если задан, то этот фильтр требует утверждение значения атрибута (AVA), содержащее %v. Пример: uid=%v. В процессе поиска значение %v в AVA заменяется на имя или шаблон пользователя, которого требуется найти. </xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Фильтр пользователей</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="(&amp;(cn=%v)(objectclass=groupOfNames))" name="groupFilter" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>Фильтр LDAP для поиска групп в реестре пользователей. Если задан, то этот фильтр требует утверждение значения атрибута (AVA), содержащее %v. Пример: cn=%v. В процессе поиска значение %v в AVA заменяется на имя или шаблон группы, которую требуется найти.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Фильтр групп</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="person:cn" name="userIdMap" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>Фильтр LDAP, применяемый для преобразования имени пользователя в запись LDAP.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Карта ИД пользователей</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="*:cn" name="groupIdMap" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>Фильтр LDAP, применяемый для преобразования имени группы в запись LDAP.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Карта ИД групп</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="groupOfNames:member" name="groupMemberIdMap" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>Фильтр LDAP для связи имен пользователей и групп.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Карта идентификаторов участников групп</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.security.registry.ldap.internal.filters.eDirectory-factory">
        <xsd:complexContent>
            <xsd:extension base="com.ibm.ws.security.registry.ldap.internal.filters.eDirectory">
                <xsd:attribute name="id" type="factoryIdType" use="optional">
                    <xsd:annotation>
                        <xsd:documentation>Уникальный ИД конфигурации.</xsd:documentation>
                        <xsd:appinfo>
                            <ext:label>ИД</ext:label>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.security.registry.ldap.internal.filters.actived">
        <xsd:annotation>
            <xsd:documentation>Список фильтров LDAP Microsoft Active Directory.</xsd:documentation>
            <xsd:appinfo>
                <ext:label>Фильтры LDAP Microsoft Active Directory</ext:label>
            </xsd:appinfo>
        </xsd:annotation>
        <!--Idiom for "empty element context" (whitespace accepted and ignored)-->
        <xsd:sequence>
            <xsd:sequence/>
        </xsd:sequence>
        <xsd:attribute default="(&amp;(sAMAccountName=%v)(objectcategory=user))" name="userFilter" type="xsd:string" use="optional">
            <xsd:annotation>
