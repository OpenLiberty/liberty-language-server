                                    <xsd:documentation>off</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute name="traceDirectory" type="xsd:string" use="optional">
            <xsd:annotation>
