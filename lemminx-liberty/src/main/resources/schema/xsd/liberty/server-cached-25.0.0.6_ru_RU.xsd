            <xsd:appinfo>
                <ext:label>Cookie</ext:label>
            </xsd:appinfo>
        </xsd:annotation>
        <!--Idiom for "empty element context" (whitespace accepted and ignored)-->
        <xsd:sequence>
            <xsd:sequence/>
        </xsd:sequence>
        <xsd:attribute name="name" type="xsd:string" use="required">
            <xsd:annotation>
                <xsd:documentation>Задает имя.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Имя</ext:label>
                </xsd:appinfo>
