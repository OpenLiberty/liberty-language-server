                <xsd:documentation>Свойство драйвера JDBC: queryCloseImplicit. Допустимые значения: 1 (QUERY_CLOSE_IMPLICIT_YES) или 2 (QUERY_CLOSE_IMPLICIT_NO).</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Неявное закрытие запроса</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union memberTypes="variableType">
                    <xsd:simpleType>
                        <xsd:restriction base="intType">
                            <xsd:enumeration value="1">
                                <xsd:annotation>
                                    <xsd:documentation>QUERY_CLOSE_IMPLICIT_YES</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="2">
                                <xsd:annotation>
                                    <xsd:documentation>QUERY_CLOSE_IMPLICIT_NO</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
