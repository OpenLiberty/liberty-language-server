        <xsd:attribute name="hostNameInCertificate" type="xsd:string" use="optional">
            <xsd:annotation>
                <xsd:documentation>Свойство драйвера JDBC: hostNameInCertificate.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Имя хоста в сертификате</ext:label>
