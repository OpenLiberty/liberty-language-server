                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="COMMIT">
                                <xsd:annotation>
                                    <xsd:documentation>Véglegesítés</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="MANUAL">
                                <xsd:annotation>
                                    <xsd:documentation>Manuális</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
