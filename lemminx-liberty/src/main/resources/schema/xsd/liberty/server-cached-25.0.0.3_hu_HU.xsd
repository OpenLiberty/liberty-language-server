                    <xsd:documentation>A hitelesítési szűrő referenciát adja meg.</xsd:documentation>
                    <xsd:appinfo>
                        <ext:label>Hitelesítési szűrő referencia</ext:label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
        <xsd:attribute default="JWT" name="cookieName" type="xsd:string" use="optional">
