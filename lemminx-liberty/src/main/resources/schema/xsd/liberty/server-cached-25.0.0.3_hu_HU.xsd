    <xsd:complexType name="com.ibm.ws.security.kerberos.auth.KerberosService">
        <xsd:annotation>
            <xsd:documentation>A Kerberos hitelesítés rendszerszintű beállításai</xsd:documentation>
            <xsd:appinfo>
                <ext:label>Kerberos</ext:label>
            </xsd:appinfo>
