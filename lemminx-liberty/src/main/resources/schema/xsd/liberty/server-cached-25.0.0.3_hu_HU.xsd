        <xsd:attribute name="libraryRef" type="pidType" use="optional">
            <xsd:annotation>
                <xsd:documentation>Hivatkozás a megosztott könyvtár konfiguráció azonosítójára.</xsd:documentation>
                <xsd:appinfo>
