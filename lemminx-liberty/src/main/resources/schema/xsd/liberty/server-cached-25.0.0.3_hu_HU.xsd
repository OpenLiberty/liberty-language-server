            <xsd:annotation>
                <xsd:documentation>Megadja a tulajdonság nevét</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Tulajdonságnév</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
