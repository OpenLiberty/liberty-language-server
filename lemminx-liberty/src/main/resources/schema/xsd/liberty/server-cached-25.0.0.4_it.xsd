                <xsd:documentation>Proprietà driver JDBC: optimizationProfile.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Profilo di ottimizzazione</ext:label>
                    <ext:group id="Advanced"/>
                </xsd:appinfo>
