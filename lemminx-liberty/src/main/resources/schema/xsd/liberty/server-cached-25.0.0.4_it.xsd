                    <xsd:appinfo>
                        <ext:label>Factory di connessioni JMS</ext:label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
        <xsd:attribute default="batchConnectionFactory" name="connectionFactoryRef" type="pidType" use="optional">
            <xsd:annotation>
