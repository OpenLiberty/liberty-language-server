                <xsd:documentation>Il defaultValue da assegnare alla variabile se non è disponibile alcun valore.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Valore predefinito</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
