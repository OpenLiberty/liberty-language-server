                                    <xsd:documentation>IMPLICIT_ROLLBACK_OPTION_CLOSE_CONNECTION</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                        </xsd:restriction>
                    </xsd:simpleType>
