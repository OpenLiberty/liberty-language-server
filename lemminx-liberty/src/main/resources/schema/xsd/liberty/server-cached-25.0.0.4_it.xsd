    <xsd:complexType name="com.ibm.ws.security.authentication.filter.webApp-factory">
        <xsd:complexContent>
            <xsd:extension base="com.ibm.ws.security.authentication.filter.webApp">
                <xsd:attribute name="id" type="factoryIdType" use="optional">
