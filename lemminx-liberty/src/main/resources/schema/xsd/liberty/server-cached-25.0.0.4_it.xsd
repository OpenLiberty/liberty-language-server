                <xsd:appinfo>
                    <ext:label>Password</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:complexType name="com.ibm.ws.javaee.dd.appbnd.RunAs-factory">
