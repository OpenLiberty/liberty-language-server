                                    <xsd:documentation>QUERY_CLOSE_IMPLICIT_YES</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="2">
                                <xsd:annotation>
                                    <xsd:documentation>QUERY_CLOSE_IMPLICIT_NO</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
