            <xsd:appinfo>
                <ext:label>Rol</ext:label>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:choice maxOccurs="unbounded" minOccurs="0">
            <xsd:element name="queuePermission" type="com.ibm.ws.messaging.security.queuePermission-factory">
