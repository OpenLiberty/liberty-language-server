    <xsd:complexType name="com.ibm.ws.security.s4u2proxy.KerberosExtService">
        <xsd:annotation>
            <xsd:documentation>Controla el funcionamiento de la delegación restringida. </xsd:documentation>
            <xsd:appinfo>
