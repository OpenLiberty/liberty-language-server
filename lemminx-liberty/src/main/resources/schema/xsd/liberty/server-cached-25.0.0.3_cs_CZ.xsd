                <xsd:documentation>Heslo uživatele použité se jménem uživatele.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Heslo uživatele</ext:label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
