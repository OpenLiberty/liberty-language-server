                <xsd:documentation>Zadejte konfiguraci SSL nezbytnou k navázání zabezpečeného připojení.</xsd:documentation>
                <xsd:appinfo>
                    <ext:label>Odkaz na SSL</ext:label>
                    <ext:reference>ssl</ext:reference>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
