    <xsd:complexType name="com.ibm.ws.persistence.databaseStore"> 
        <xsd:annotation> 
            <xsd:documentation>指定关系数据库作为服务器功能部件的持久存储器。</xsd:documentation> 
            <xsd:appinfo> 
                <ext:label>数据库存储</ext:label> 
            </xsd:appinfo> 
        </xsd:annotation> 
        <xsd:choice minOccurs="0" maxOccurs="unbounded"> 
            <xsd:element name="dataSource" type="com.ibm.ws.jdbc.dataSource"> 
                <xsd:annotation> 
                    <xsd:documentation>连接至持久存储器的数据源。</xsd:documentation> 
                    <xsd:appinfo> 
                        <ext:label>数据源</ext:label> 
                    </xsd:appinfo> 
                </xsd:annotation> 
            </xsd:element> 
            <xsd:element name="authData" type="com.ibm.ws.security.jca.internal.authdata.config"> 
                <xsd:annotation> 
                    <xsd:documentation>任务安排、查询和执行的认证数据。</xsd:documentation> 
                    <xsd:appinfo> 
                        <ext:label>认证数据</ext:label> 
                    </xsd:appinfo> 
                </xsd:annotation> 
            </xsd:element> 
        </xsd:choice> 
        <xsd:attribute name="createTables" use="optional" default="true" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>设置为 true 时创建数据库表。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>创建数据库表</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="dataSourceRef" use="optional" default="DefaultDataSource" type="pidType"> 
            <xsd:annotation> 
                <xsd:documentation>连接至持久存储器的数据源。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>数据源引用</ext:label> 
                    <ext:reference>dataSource</ext:reference> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="keyGenerationStrategy" use="optional" default="AUTO"> 
            <xsd:annotation> 
                <xsd:documentation>用于生成唯一主键的首选策略。如果数据库不支持所选策略，那么可使用另一策略。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>键生成策略</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
            <xsd:simpleType> 
                <xsd:restriction base="xsd:string"> 
                    <xsd:enumeration value="AUTO"> 
                        <xsd:annotation> 
                            <xsd:documentation>自动选择用于生成唯一主键的策略。</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                    <xsd:enumeration value="IDENTITY"> 
                        <xsd:annotation> 
                            <xsd:documentation>使用数据库标识列来生成唯一主键。</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                    <xsd:enumeration value="SEQUENCE"> 
                        <xsd:annotation> 
                            <xsd:documentation>使用数据库序列来生成唯一主键。</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                    <xsd:enumeration value="TABLE"> 
                        <xsd:annotation> 
                            <xsd:documentation>使用数据库表来生成唯一主键。</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                </xsd:restriction> 
            </xsd:simpleType> 
        </xsd:attribute> 
        <xsd:attribute name="tablePrefix" use="optional" default="WLP" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>表、序列和其他数据库工件的名称前缀。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>表名前缀</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="authDataRef" use="optional" type="pidType"> 
            <xsd:annotation> 
                <xsd:documentation>任务安排、查询和执行的认证数据。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>认证数据引用</ext:label> 
                    <ext:reference>authData</ext:reference> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="schema" use="optional" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>带有对数据库表的读写访问权的模式名称。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>模式名称</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:anyAttribute processContents="skip"> 
        </xsd:anyAttribute> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.persistence.databaseStore-factory"> 
        <xsd:complexContent> 
            <xsd:extension base="com.ibm.ws.persistence.databaseStore"> 
                <xsd:attribute name="id" type="factoryIdType" use="optional"> 
                    <xsd:annotation> 
                        <xsd:documentation>唯一配置标识。</xsd:documentation> 
                        <xsd:appinfo> 
                            <ext:label>标识</ext:label> 
                        </xsd:appinfo> 
                    </xsd:annotation> 
                </xsd:attribute> 
            </xsd:extension> 
        </xsd:complexContent> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.security.acme.config"> 
        <xsd:annotation> 
            <xsd:documentation>ACME 认证中心的配置。</xsd:documentation> 
            <xsd:appinfo> 
                <ext:label>ACME 认证中心</ext:label> 
            </xsd:appinfo> 
        </xsd:annotation> 
        <xsd:choice minOccurs="0" maxOccurs="unbounded"> 
            <xsd:element name="domain" type="xsd:string"> 
                <xsd:annotation> 
                    <xsd:documentation>要针对其请求证书的域名。</xsd:documentation> 
                    <xsd:appinfo> 
                        <ext:label>域名</ext:label> 
                    </xsd:appinfo> 
                </xsd:annotation> 
            </xsd:element> 
            <xsd:element name="accountContact" type="xsd:string"> 
                <xsd:annotation> 
                    <xsd:documentation>ACME 服务器在就与此 ACME 帐户相关的问题联系客户时可使用的联系人 URL。</xsd:documentation> 
                    <xsd:appinfo> 
                        <ext:label>帐户联系人</ext:label> 
                    </xsd:appinfo> 
                </xsd:annotation> 
            </xsd:element> 
            <xsd:element name="acmeTransportConfig" type="com.ibm.ws.security.acme.transport"> 
                <xsd:annotation> 
                    <xsd:documentation>ACME 传输层。</xsd:documentation> 
                    <xsd:appinfo> 
                        <ext:label>ACME 传输</ext:label> 
                    </xsd:appinfo> 
                </xsd:annotation> 
            </xsd:element> 
            <xsd:element name="acmeRevocationChecker" type="com.ibm.ws.security.acme.revocation"> 
                <xsd:annotation> 
                    <xsd:documentation>有关使用在线证书状态协议 (OCSP) 或证书撤销列表 (CRL) 检查证书撤销状态的配置。</xsd:documentation> 
                    <xsd:appinfo> 
                        <ext:label>ACME 证书撤销校验器</ext:label> 
                    </xsd:appinfo> 
                </xsd:annotation> 
            </xsd:element> 
        </xsd:choice> 
        <xsd:attribute name="directoryURI" use="required" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>ACME CA 服务器目录对象的 URI。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>ACME 服务器目录 URI</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="validFor" use="optional" type="duration"> 
            <xsd:annotation> 
                <xsd:documentation>证书签名请求指定的证书有效期。缺省值由 ACME CA 服务器定义。. 指定后跟时间单位的正整数，时间单位可以是小时 (h)、分钟 (m)、秒 (s) 或毫秒 (ms)。例如，以 500ms 的形式指定 500 毫秒。可将多个值包括在单个条目中。例如，1s500ms 相当于 1.5 秒。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>有效期：</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="subjectDN" use="optional" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>要用于证书的主题专有名称 (DN)。DN 可以包含下列相对专有名称 (RDN) 类型：cn、c、st、l、o 和 ou。如果定义了 cn RDN 类型，那么它必须是由域配置元素定义的其中一个域，并且它必须是 DN 中的第一个 RDN。如果未定义 cn RDN 类型，那么由域配置元素定义的第一个域将用作 cn RDN 值。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>主题专有名称</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="challengePollTimeout" use="optional" default="120s" type="duration"> 
            <xsd:annotation> 
                <xsd:documentation>轮询中断并将 ACME 挑战视为失败之前，在轮询 ACME 挑战状态上所消耗的时间。值为 0 指示无限轮询。. 指定后跟时间单位的正整数，时间单位可以是小时 (h)、分钟 (m)、秒 (s) 或毫秒 (ms)。例如，以 500ms 的形式指定 500 毫秒。可将多个值包括在单个条目中。例如，1s500ms 相当于 1.5 秒。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>挑战轮询超时</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="orderPollTimeout" use="optional" default="120s" type="duration"> 
            <xsd:annotation> 
                <xsd:documentation>轮询中断并将 ACME 指令视为失败之前，在轮询 ACME 指令状态上所消耗的时间。值为 0 指示无限轮询。. 指定后跟时间单位的正整数，时间单位可以是小时 (h)、分钟 (m)、秒 (s) 或毫秒 (ms)。例如，以 500ms 的形式指定 500 毫秒。可将多个值包括在单个条目中。例如，1s500ms 相当于 1.5 秒。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>指令轮询超时</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="accountKeyFile" use="optional" default="${server.output.dir}/resources/security/acmeAccountKey.pem" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>此路径指向包含 ACME CA 服务器上的已注册帐户的密钥标识的文件。如果该文件不存在，那么系统会向 ACME CA 服务器注册新帐户，并且关联密钥会写至此文件。备份此文件以保持在 ACME CA 服务器上对该帐户的控制。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>帐户密钥文件</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="domainKeyFile" use="optional" default="${server.output.dir}/resources/security/acmeDomainKey.pem" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>此路径指向包含域的密钥标识的文件。如果该文件不存在，那系统会生成新密钥并写至此文件。备份此文件以保持对域的控制。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>域密钥文件</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="acmeTransportConfigRef" use="optional" type="pidType"> 
            <xsd:annotation> 
                <xsd:documentation>ACME 传输层。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>ACME 传输参考</ext:label> 
                    <ext:reference>acmeTransportConfig</ext:reference> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="renewBeforeExpiration" use="optional" default="7d" type="duration"> 
            <xsd:annotation> 
                <xsd:documentation>到期之前的更新时间. 指定后跟时间单位的正整数，时间单位可以是小时 (h)、分钟 (m)、秒 (s) 或毫秒 (ms)。例如，以 500ms 的形式指定 500 毫秒。可将多个值包括在单个条目中。例如，1s500ms 相当于 1.5 秒。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>到期之前的更新时间</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="acmeRevocationCheckerRef" use="optional" type="pidType"> 
            <xsd:annotation> 
                <xsd:documentation>有关使用在线证书状态协议 (OCSP) 或证书撤销列表 (CRL) 检查证书撤销状态的配置。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>ACME 证书撤销校验器参考</ext:label> 
                    <ext:reference>acmeRevocationChecker</ext:reference> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:anyAttribute processContents="skip"> 
        </xsd:anyAttribute> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.security.acme.transport"> 
        <xsd:annotation> 
            <xsd:documentation>ACME 传输层。</xsd:documentation> 
            <xsd:appinfo> 
                <ext:label>ACME 传输</ext:label> 
            </xsd:appinfo> 
        </xsd:annotation><!--Idiom for "empty element context" (whitespace accepted and ignored)--> 
        <xsd:sequence> 
            <xsd:sequence/> 
        </xsd:sequence> 
        <xsd:attribute name="protocol" use="optional" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>SSL 握手协议。可以在底层 JRE 的 Java 安全套接字扩展 (JSSE) 提供程序的文档中找到协议值。使用 IBM JRE 时，缺省值为 SSL_TLSv2；使用 Oracle JRE 时，缺省值为 SSL。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>SSL 协议</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="trustStore" use="optional" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>其中包含 SSL 用于为证书签名的可信证书条目的密钥库。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>信任库</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="trustStorePassword" use="optional" type="password"> 
            <xsd:annotation> 
                <xsd:documentation>用来装入信任库文件的密码。可采用明文或编码格式存储该值。请使用 securityUtility 工具来对该密码进行编码。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>信任库密码</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="trustStoreType" use="optional" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>信任库的密钥库类型。受支持的类型为 JKS、PKCS12 和 JCEKS。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>信任库类型</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:anyAttribute processContents="skip"> 
        </xsd:anyAttribute> 
    </xsd:complexType> 
