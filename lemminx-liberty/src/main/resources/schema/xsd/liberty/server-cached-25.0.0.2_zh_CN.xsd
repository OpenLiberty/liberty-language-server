    <xsd:complexType name="com.ibm.ws.security.mp.jwt"> 
        <xsd:annotation> 
            <xsd:documentation>用于处理 MicroProfile JWT 令牌的配置。</xsd:documentation> 
            <xsd:appinfo> 
                <ext:label>MicroProfile JWT</ext:label> 
            </xsd:appinfo> 
        </xsd:annotation> 
        <xsd:choice minOccurs="0" maxOccurs="unbounded"> 
            <xsd:element name="audiences" type="xsd:string"> 
                <xsd:annotation> 
                    <xsd:documentation>要包含在 JSON Web 令牌的受众声明中的可信受众列表。</xsd:documentation> 
                    <xsd:appinfo> 
                        <ext:label>可信受众</ext:label> 
                    </xsd:appinfo> 
                </xsd:annotation> 
            </xsd:element> 
            <xsd:element name="authFilter" type="com.ibm.ws.security.authentication.filter"> 
