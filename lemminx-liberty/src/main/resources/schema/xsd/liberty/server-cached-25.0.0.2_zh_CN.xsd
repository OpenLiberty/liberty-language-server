    <xsd:complexType name="com.ibm.ws.sip.introspector"> 
        <xsd:annotation> 
            <xsd:documentation>SIP 自省的配置</xsd:documentation> 
            <xsd:appinfo> 
                <ext:label>SIP 自省</ext:label> 
            </xsd:appinfo> 
        </xsd:annotation><!--Idiom for "empty element context" (whitespace accepted and ignored)--> 
        <xsd:sequence> 
            <xsd:sequence/> 
        </xsd:sequence> 
        <xsd:attribute name="method" use="optional" default="SUCCINCT"> 
            <xsd:annotation> 
                <xsd:documentation>生成服务器转储时，要包括的 SIP 状态详细信息的级别。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>转储详细程度</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
            <xsd:simpleType> 
                <xsd:restriction base="xsd:string"> 
                    <xsd:enumeration value="SUCCINCT"> 
                        <xsd:annotation> 
                            <xsd:documentation>服务器转储仅包括 SIP 应用程序会话和 SIP 会话标识</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                    <xsd:enumeration value="VERBOSE"> 
                        <xsd:annotation> 
                            <xsd:documentation>包含服务器转储中 SIP 应用程序会话和 SIP 会话的详细状态。</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                </xsd:restriction> 
            </xsd:simpleType> 
        </xsd:attribute> 
        <xsd:anyAttribute processContents="skip"> 
        </xsd:anyAttribute> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.sip.introspector-factory"> 
        <xsd:complexContent> 
            <xsd:extension base="com.ibm.ws.sip.introspector"> 
                <xsd:attribute name="id" type="factoryIdType" use="optional"> 
                    <xsd:annotation> 
                        <xsd:documentation>唯一配置标识。</xsd:documentation> 
                        <xsd:appinfo> 
                            <ext:label>标识</ext:label> 
                        </xsd:appinfo> 
                    </xsd:annotation> 
