        </xsd:sequence> 
        <xsd:attribute name="alias" use="optional" default="email" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>指定别名。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>别名</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="uriType" use="optional" default="http://axschema.org/contact/email" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>指定 URI 类型。</xsd:documentation> 
                <xsd:appinfo> 
