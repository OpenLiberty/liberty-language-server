                <xsd:appinfo> 
                    <ext:label>颁发者</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="groupNameAttribute" use="optional" default="groups" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>此声明的值将用作用户组成员资格。</xsd:documentation> 
