    <xsd:complexType name="com.ibm.ws.ssl.default"> 
        <xsd:annotation> 
            <xsd:documentation>SSL 服务的缺省指令表。</xsd:documentation> 
            <xsd:appinfo> 
                <ext:label>SSL 缺省指令表</ext:label> 
            </xsd:appinfo> 
        </xsd:annotation><!--Idiom for "empty element context" (whitespace accepted and ignored)--> 
        <xsd:sequence> 
            <xsd:sequence/> 
