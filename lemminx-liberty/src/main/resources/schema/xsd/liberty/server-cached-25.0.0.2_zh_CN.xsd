                <xsd:appinfo> 
                    <ext:label>SSL 引用</ext:label> 
                    <ext:reference>ssl</ext:reference> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="keyName" use="optional" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>指定用于使用公共密钥来验证令牌签名的可信密钥别名。</xsd:documentation> 
