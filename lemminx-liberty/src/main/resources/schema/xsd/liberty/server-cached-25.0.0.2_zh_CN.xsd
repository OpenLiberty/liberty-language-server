                <xsd:appinfo> 
                    <ext:label>启用用户信息端点</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="jwksUri" use="optional" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>指定 JSON Web Key 服务 URL。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>JWK 端点 URL</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="scope" use="optional" default="openid profile email" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>指定所需作用域。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>OAuth 作用域</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="userNameAttribute" use="optional" default="sub" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>声明的值为已认证的用户主体。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>作为主体声明</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="mapToUserRegistry" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>指定是否将用户标识映射至注册表用户。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>映射用户标识</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="sslRef" use="optional" type="pidType"> 
            <xsd:annotation> 
                <xsd:documentation>指定用于连接社交媒体的 SSL 配置的标识。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>SSL 引用</ext:label> 
                    <ext:reference>ssl</ext:reference> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="authFilterRef" use="optional" type="pidType"> 
            <xsd:annotation> 
                <xsd:documentation>指定认证过滤器参考。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>认证过滤器参考</ext:label> 
                    <ext:reference>authFilter</ext:reference> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="trustAliasName" use="optional" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>指定用于使用公共密钥来验证令牌签名的可信密钥别名。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>可信别名</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="isClientSideRedirectSupported" use="optional" default="true" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>指定是否支持客户端重定向。客户机示例包括浏览器或独立 JavaScript 应用程序。如果设置为 true，那么客户机必须支持 JavaScript。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>是否支持客户端重定向</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="displayName" use="optional" default="oidcLogin" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>屏幕的社交登录配置的名称。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>社会化登录名</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="website" use="optional" type="tokenType"> 
            <xsd:annotation> 
                <xsd:documentation>Web 站点地址。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Web 站点</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="issuer" use="optional" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>颁发者的 URL。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>颁发者</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="realmNameAttribute" use="optional" default="iss" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>声明的值将用作主体集域。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>作为域声明</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="groupNameAttribute" use="optional" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>声明的值将用作用户组成员资格。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>作为组声明</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="userUniqueIdAttribute" use="optional" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>声明的值将用作主体集唯一标识。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>作为主体集唯一标识声明</ext:label> 
                </xsd:appinfo> 
