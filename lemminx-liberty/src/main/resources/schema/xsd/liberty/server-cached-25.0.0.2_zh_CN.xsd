                <xsd:appinfo> 
                    <ext:label>CouchDB 用户标识</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="password" use="required" type="password"> 
            <xsd:annotation> 
