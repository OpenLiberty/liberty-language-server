    <xsd:complexType name="includeType"> 
        <xsd:annotation> 
            <xsd:documentation>指定要包括在服务器配置中的配置资源。</xsd:documentation> 
            <xsd:appinfo> 
                <ext:label>包含</ext:label> 
            </xsd:appinfo> 
        </xsd:annotation> 
        <xsd:attribute name="optional" type="xsd:boolean" use="optional" default="false"> 
            <xsd:annotation> 
                <xsd:documentation>允许跳过找不到的所包括资源。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>资源是可选的</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="location" type="location" use="required"> 
            <xsd:annotation> 
                <xsd:documentation>指定资源位置。 该值可以是远程资源的文件路径，目录或 URI。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>位置</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="onConflict" use="optional" default="MERGE"> 
            <xsd:annotation> 
                <xsd:documentation>指定发现冲突时用于合并元素的行为。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>发生冲突</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
            <xsd:simpleType> 
                <xsd:restriction base="xsd:string"> 
                    <xsd:enumeration value="MERGE"> 
                        <xsd:annotation> 
                            <xsd:documentation>冲突元素将合并到一起。</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                    <xsd:enumeration value="REPLACE"> 
                        <xsd:annotation> 
                            <xsd:documentation>如果元素有冲突，那么被包含文件中的元素将替换冲突元素。</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                    <xsd:enumeration value="IGNORE"> 
                        <xsd:annotation> 
                            <xsd:documentation>被包含文件中的冲突元素将被忽略。</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                </xsd:restriction> 
            </xsd:simpleType> 
        </xsd:attribute> 
    </xsd:complexType> 
