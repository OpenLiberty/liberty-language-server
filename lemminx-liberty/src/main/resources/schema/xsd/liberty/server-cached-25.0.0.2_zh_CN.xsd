                    <ext:label>SSL 引用</ext:label> 
                    <ext:reference>ssl</ext:reference> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:anyAttribute processContents="skip"> 
        </xsd:anyAttribute> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.security.csiv2.transportLayer-factory"> 
