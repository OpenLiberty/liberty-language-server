                    </xsd:appinfo> 
                </xsd:annotation> 
                <xsd:simpleType> 
                    <xsd:restriction base="xsd:string"> 
                        <xsd:enumeration value="authorization_code"> 
                            <xsd:annotation> 
                                <xsd:documentation>authorization_code</xsd:documentation> 
                            </xsd:annotation> 
                        </xsd:enumeration> 
                        <xsd:enumeration value="implicit"> 
                            <xsd:annotation> 
                                <xsd:documentation>隐式</xsd:documentation> 
                            </xsd:annotation> 
                        </xsd:enumeration> 
                        <xsd:enumeration value="refresh_token"> 
                            <xsd:annotation> 
                                <xsd:documentation>refresh_token</xsd:documentation> 
                            </xsd:annotation> 
                        </xsd:enumeration> 
                        <xsd:enumeration value="client_credentials"> 
                            <xsd:annotation> 
                                <xsd:documentation>client_credentials</xsd:documentation> 
                            </xsd:annotation> 
                        </xsd:enumeration> 
                        <xsd:enumeration value="password"> 
                            <xsd:annotation> 
                                <xsd:documentation>密码</xsd:documentation> 
                            </xsd:annotation> 
                        </xsd:enumeration> 
                        <xsd:enumeration value="urn:ietf:params:oauth:grant-type:jwt-bearer"> 
                            <xsd:annotation> 
                                <xsd:documentation>urn:ietf:params:oauth:grant-type:jwt-bearer</xsd:documentation> 
                            </xsd:annotation> 
                        </xsd:enumeration> 
                    </xsd:restriction> 
                </xsd:simpleType> 
            </xsd:element> 
            <xsd:element name="responseTypes" default="code"> 
                <xsd:annotation> 
                    <xsd:documentation>客户机可使用的响应类型。</xsd:documentation> 
                    <xsd:appinfo> 
                        <ext:label>响应类型</ext:label> 
                    </xsd:appinfo> 
                </xsd:annotation> 
                <xsd:simpleType> 
                    <xsd:restriction base="xsd:string"> 
                        <xsd:enumeration value="code"> 
                            <xsd:annotation> 
                                <xsd:documentation>代码</xsd:documentation> 
                            </xsd:annotation> 
                        </xsd:enumeration> 
                        <xsd:enumeration value="token"> 
                            <xsd:annotation> 
                                <xsd:documentation>令牌</xsd:documentation> 
                            </xsd:annotation> 
                        </xsd:enumeration> 
                        <xsd:enumeration value="id_token token"> 
                            <xsd:annotation> 
                                <xsd:documentation>id_token 令牌</xsd:documentation> 
                            </xsd:annotation> 
                        </xsd:enumeration> 
                    </xsd:restriction> 
                </xsd:simpleType> 
            </xsd:element> 
            <xsd:element name="postLogoutRedirectUris" type="xsd:string"> 
                <xsd:annotation> 
                    <xsd:documentation>RP 提供的 URL 数组，RP 可能在执行注销后使用 post_logout_redirect_uri 参数请求最终用户的用户代理重定向至这些 URL。</xsd:documentation> 
                    <xsd:appinfo> 
                        <ext:label>注销后重定向 URI</ext:label> 
                    </xsd:appinfo> 
                </xsd:annotation> 
            </xsd:element> 
            <xsd:element name="trustedUriPrefixes" type="xsd:string"> 
                <xsd:annotation> 
                    <xsd:documentation>覆盖范围映射使用的 URI。</xsd:documentation> 
                    <xsd:appinfo> 
                        <ext:label>可信 URI 前缀</ext:label> 
                    </xsd:appinfo> 
                </xsd:annotation> 
            </xsd:element> 
            <xsd:element name="resourceIds" type="xsd:string"> 
                <xsd:annotation> 
                    <xsd:documentation>作为 JSON Web 令牌的受众的资源标识。</xsd:documentation> 
                    <xsd:appinfo> 
                        <ext:label>资源标识</ext:label> 
                    </xsd:appinfo> 
                </xsd:annotation> 
            </xsd:element> 
            <xsd:element name="functionalUserGroupIds" type="xsd:string"> 
                <xsd:annotation> 
                    <xsd:documentation>要与此客户机使用客户机凭证授予类型获取的访问令牌关联的组标识列表。如果指定了此客户机参数，那么值在内省端点的 functional_user_groupIds 响应参数中返回。</xsd:documentation> 
                    <xsd:appinfo> 
                        <ext:label>生效用户组标识</ext:label> 
                    </xsd:appinfo> 
                </xsd:annotation> 
            </xsd:element> 
        </xsd:choice> 
        <xsd:attribute name="name" use="optional" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>客户机的名称（有时通过标识来引用）。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>名称</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="secret" use="optional" type="password"> 
            <xsd:annotation> 
                <xsd:documentation>客户机的密钥。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>密钥</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="displayname" use="optional" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>客户机的显示名称。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>显示名称</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="tokenEndpointAuthMethod" use="optional" default="client_secret_basic"> 
            <xsd:annotation> 
                <xsd:documentation>针对客户机的令牌端点请求的认证方法。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>令牌端点认证方法</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
            <xsd:simpleType> 
                <xsd:restriction base="xsd:string"> 
                    <xsd:enumeration value="none"> 
                        <xsd:annotation> 
                            <xsd:documentation>无</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                    <xsd:enumeration value="client_secret_post"> 
                        <xsd:annotation> 
                            <xsd:documentation>client_secret_post</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                    <xsd:enumeration value="client_secret_basic"> 
                        <xsd:annotation> 
                            <xsd:documentation>client_secret_basic</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                </xsd:restriction> 
            </xsd:simpleType> 
        </xsd:attribute> 
        <xsd:attribute name="scope" use="optional" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>指定客户机作用域的空格分隔列表。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>作用域</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="applicationType" use="optional" default="web"> 
            <xsd:annotation> 
                <xsd:documentation>最能描述客户机的应用程序类型。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>应用程序类型</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
            <xsd:simpleType> 
                <xsd:restriction base="xsd:string"> 
                    <xsd:enumeration value="native"> 
                        <xsd:annotation> 
                            <xsd:documentation>本机</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                    <xsd:enumeration value="web"> 
                        <xsd:annotation> 
                            <xsd:documentation>web</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                </xsd:restriction> 
            </xsd:simpleType> 
        </xsd:attribute> 
        <xsd:attribute name="subjectType" use="optional"> 
            <xsd:annotation> 
                <xsd:documentation>请求的主题类型，用于响应此客户机。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>主题类型</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
            <xsd:simpleType> 
                <xsd:restriction base="xsd:string"> 
                    <xsd:enumeration value="public"> 
                        <xsd:annotation> 
                            <xsd:documentation>公用</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                </xsd:restriction> 
            </xsd:simpleType> 
        </xsd:attribute> 
        <xsd:attribute name="sessionManaged" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>布尔值，指示客户机是否参与 OpenID 会话管理。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>受管会话</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="preAuthorizedScope" use="optional" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>请求资源所有者似乎已提前批准并因此不需要资源所有者同意的访问令牌时，客户机可使用的作用域值的空格分隔列表。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>PreAuthorized 作用域</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="introspectTokens" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>布尔值，指定是否允许客户机访问内省端点以内省授权服务器颁发的令牌。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>内省令牌</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="allowRegexpRedirects" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>允许重定向 URL 以包含正则表达式。缺省值为 false。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>允许正则表达式重定向</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="functionalUserId" use="optional" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>要与此客户机使用客户机凭证授予类型获取的访问令牌关联的用户标识。如果指定了此客户机参数，那么值在内省端点的 functional_user_id 响应参数中返回。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>生效用户标识</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="enabled" use="optional" default="true" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>如果为 true，那么客户机处于启用状态；如果为 false，那么客户机处于禁用状态。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>客户机已启用</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="appPasswordAllowed" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>设置为 true 时，允许 OAuth 客户机请求应用程序密码。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>允许使用应用程序密码</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="appTokenAllowed" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>设置为 true 时，允许 OAuth 客户机请求应用程序令牌。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>允许使用应用程序令牌</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="proofKeyForCodeExchange" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>OAuth 客户机的代码交换证明密钥支持。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>代码交换证明密钥</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="publicClient" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>指定 OAuth 客户机是否为公共客户机。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>公共 OAuth 客户机</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="backchannelLogoutUri" use="optional" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>指定 OpenID Connect 客户机的后台通道注销 URI。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>回通道注销 URI</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:anyAttribute processContents="skip"> 
        </xsd:anyAttribute> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.security.oauth20.client-factory"> 
        <xsd:complexContent> 
            <xsd:extension base="com.ibm.ws.security.oauth20.client"> 
                <xsd:attribute name="id" type="factoryIdType" use="optional"> 
                    <xsd:annotation> 
                        <xsd:documentation>唯一配置标识。</xsd:documentation> 
                        <xsd:appinfo> 
                            <ext:label>标识</ext:label> 
                        </xsd:appinfo> 
                    </xsd:annotation> 
                </xsd:attribute> 
            </xsd:extension> 
        </xsd:complexContent> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.security.oauth20.database.store"> 
        <xsd:annotation> 
            <xsd:documentation>已定义客户机，并且标记高速缓存在数据库中。</xsd:documentation> 
            <xsd:appinfo> 
                <ext:label>数据库存储</ext:label> 
            </xsd:appinfo> 
        </xsd:annotation> 
        <xsd:sequence> 
            <xsd:element name="dataSource" minOccurs="0" maxOccurs="1" type="com.ibm.ws.jdbc.dataSource"> 
                <xsd:annotation> 
                    <xsd:documentation>对存储的数据源的引用。</xsd:documentation> 
                    <xsd:appinfo> 
                        <ext:label>数据源</ext:label> 
                    </xsd:appinfo> 
                </xsd:annotation> 
            </xsd:element> 
        </xsd:sequence> 
        <xsd:attribute name="dataSourceRef" use="optional" type="pidType"> 
            <xsd:annotation> 
                <xsd:documentation>对存储的数据源的引用。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>数据源引用</ext:label> 
                    <ext:reference>dataSource</ext:reference> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="cleanupExpiredTokenInterval" use="optional" default="3600" type="secondDuration"> 
            <xsd:annotation> 
                <xsd:documentation>用于从数据库存储中清除到期令牌的时间间隔。有效范围是 0 到 Integer.MAX_VALUE（以秒计）。值 0 指示不执行清除。. 指定后跟时间单位的正整数，时间单位可以是小时 (h)、分钟 (m) 或秒 (s)。例如，以 30s 的形式指定 30 秒。可将多个值包括在单个条目中。例如，1m30s 相当于 90 秒。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>数据库清除时间间隔</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="user" use="optional" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>用户</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>用户</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="password" use="optional" type="password"> 
            <xsd:annotation> 
                <xsd:documentation>用于访问数据库的密码。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>密码</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="schema" use="optional" default="OAuthDBSchema" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>模式</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>模式</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:anyAttribute processContents="skip"> 
        </xsd:anyAttribute> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.security.oauth20.database.store-factory"> 
        <xsd:complexContent> 
            <xsd:extension base="com.ibm.ws.security.oauth20.database.store"> 
                <xsd:attribute name="id" type="factoryIdType" use="optional"> 
                    <xsd:annotation> 
                        <xsd:documentation>唯一配置标识。</xsd:documentation> 
                        <xsd:appinfo> 
                            <ext:label>标识</ext:label> 
                        </xsd:appinfo> 
                    </xsd:annotation> 
                </xsd:attribute> 
            </xsd:extension> 
        </xsd:complexContent> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.security.oauth20.user"> 
        <xsd:annotation> 
            <xsd:documentation>具有安全角色的用户。</xsd:documentation> 
            <xsd:appinfo> 
                <ext:label>用户</ext:label> 
            </xsd:appinfo> 
        </xsd:annotation><!--Idiom for "empty element context" (whitespace accepted and ignored)--> 
        <xsd:sequence> 
            <xsd:sequence/> 
        </xsd:sequence> 
        <xsd:attribute name="name" use="required" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>具有安全角色的用户的名称。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>用户名</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="access-id" use="optional" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>以常规格式 user:realmName/userUniqueId 表示的用户访问标识。如果未指定一个值，那么将生成一个值。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>用户访问标识</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:anyAttribute processContents="skip"> 
        </xsd:anyAttribute> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.security.oauth20.user-factory"> 
        <xsd:complexContent> 
            <xsd:extension base="com.ibm.ws.security.oauth20.user"> 
                <xsd:attribute name="id" type="factoryIdType" use="optional"> 
                    <xsd:annotation> 
                        <xsd:documentation>唯一配置标识。</xsd:documentation> 
                        <xsd:appinfo> 
                            <ext:label>标识</ext:label> 
                        </xsd:appinfo> 
                    </xsd:annotation> 
                </xsd:attribute> 
            </xsd:extension> 
        </xsd:complexContent> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.security.oauth20.provider"> 
        <xsd:annotation> 
            <xsd:documentation>OAuth 提供程序定义。</xsd:documentation> 
            <xsd:appinfo> 
                <ext:label>OAuth 提供程序定义</ext:label> 
            </xsd:appinfo> 
        </xsd:annotation> 
        <xsd:choice minOccurs="0" maxOccurs="unbounded"> 
            <xsd:element name="localStore" type="com.ibm.ws.security.oauth20.server.store"> 
            </xsd:element> 
            <xsd:element name="databaseStore" type="com.ibm.ws.security.oauth20.database.store"> 
            </xsd:element> 
            <xsd:element name="customStore" type="com.ibm.ws.security.oauth20.provider.custom.store"> 
            </xsd:element> 
            <xsd:element name="library" type="com.ibm.ws.classloading.sharedlibrary"> 
                <xsd:annotation> 
                    <xsd:documentation>对包含介体插件类的共享库的引用。</xsd:documentation> 
                    <xsd:appinfo> 
                        <ext:label>共享库</ext:label> 
                    </xsd:appinfo> 
                </xsd:annotation> 
            </xsd:element> 
            <xsd:element name="mediatorClassname" type="xsd:string"> 
                <xsd:annotation> 
                    <xsd:documentation>介体插件类名称。完整应用程序服务器概要文件中的等效提供程序参数为 oauth20.mediator.classnames。</xsd:documentation> 
                    <xsd:appinfo> 
                        <ext:label>介体类名称</ext:label> 
                    </xsd:appinfo> 
                </xsd:annotation> 
            </xsd:element> 
            <xsd:element name="grantType" default="authorization_code" type="xsd:string"> 
                <xsd:annotation> 
                    <xsd:documentation>提供程序允许的 OAuth 2.0 授权类型，详细描述可参见 OAuth 2.0 规范。缺省情况下，允许使用所有授权类型。受支持的值为 authorization_code、client_credentials、refresh_token、password、implicit 和 urn:ietf:params:oauth:grant-type:jwt-bearer。</xsd:documentation> 
                    <xsd:appinfo> 
                        <ext:label>OAuth 授予类型</ext:label> 
                    </xsd:appinfo> 
                </xsd:annotation> 
            </xsd:element> 
            <xsd:element name="autoAuthorizeClient" type="xsd:string"> 
                <xsd:annotation> 
                    <xsd:documentation>允许使用自动授权的客户机的名称。完整应用程序服务器概要文件中的等效提供程序参数为 oauth20.autoauthorize.clients。</xsd:documentation> 
                    <xsd:appinfo> 
                        <ext:label>自动授权客户机</ext:label> 
                    </xsd:appinfo> 
                </xsd:annotation> 
            </xsd:element> 
            <xsd:element name="tokenFormat"> 
                <xsd:annotation> 
                    <xsd:documentation>要生成的令牌类型。应该为下列其中一项：opaque、jwt 或 mpjwt。mpjwt 是微概要文件标准化格式。缺省类型为 opaque。</xsd:documentation> 
                    <xsd:appinfo> 
                        <ext:label>所发出的 JWT 令牌类型</ext:label> 
                    </xsd:appinfo> 
                </xsd:annotation> 
                <xsd:simpleType> 
                    <xsd:restriction base="xsd:string"> 
                        <xsd:enumeration value="opaque"> 
                            <xsd:annotation> 
                                <xsd:documentation>opaque</xsd:documentation> 
                            </xsd:annotation> 
                        </xsd:enumeration> 
                        <xsd:enumeration value="jwt"> 
                            <xsd:annotation> 
                                <xsd:documentation>jwt</xsd:documentation> 
                            </xsd:annotation> 
                        </xsd:enumeration> 
                        <xsd:enumeration value="mpjwt"> 
                            <xsd:annotation> 
                                <xsd:documentation>mpjwt</xsd:documentation> 
                            </xsd:annotation> 
                        </xsd:enumeration> 
                    </xsd:restriction> 
                </xsd:simpleType> 
            </xsd:element> 
            <xsd:element name="jwtGrantType" type="com.ibm.ws.security.oauth20.provider.granttype.jwt"> 
            </xsd:element> 
        </xsd:choice> 
        <xsd:attribute name="httpsRequired" use="optional" default="true" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>需要 OAuth 客户机与提供程序之间的 SSL 通信。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>需要 SSL 通信</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="authorizationGrantLifetime" use="optional" default="604800" type="secondDuration"> 
            <xsd:annotation> 
                <xsd:documentation>授权生存期（秒）。完整应用程序服务器概要文件中的等效提供程序参数为 oauth20.max.authorization.grant.lifetime.seconds。. 指定后跟时间单位的正整数，时间单位可以是小时 (h)、分钟 (m) 或秒 (s)。例如，以 30s 的形式指定 30 秒。可将多个值包括在单个条目中。例如，1m30s 相当于 90 秒。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>授权生存期</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="authorizationCodeLifetime" use="optional" default="60" type="secondDuration"> 
            <xsd:annotation> 
                <xsd:documentation>授权码生存期（秒）。完整应用程序服务器概要文件中的等效提供程序参数为 oauth20.code.lifetime.seconds。. 指定后跟时间单位的正整数，时间单位可以是小时 (h)、分钟 (m) 或秒 (s)。例如，以 30s 的形式指定 30 秒。可将多个值包括在单个条目中。例如，1m30s 相当于 90 秒。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>授权码生存期</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="authorizationCodeLength" use="optional" default="30" type="longType"> 
            <xsd:annotation> 
                <xsd:documentation>生成的授权码的长度。完整应用程序服务器概要文件中的等效提供程序参数为 oauth20.code.length。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>授权码长度</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="accessTokenLifetime" use="optional" default="7200" type="secondDuration"> 
            <xsd:annotation> 
                <xsd:documentation>访问令牌有效的时间（秒）。完整应用程序服务器概要文件中的等效提供程序参数为 oauth20.token.lifetime.seconds。. 指定后跟时间单位的正整数，时间单位可以是小时 (h)、分钟 (m) 或秒 (s)。例如，以 30s 的形式指定 30 秒。可将多个值包括在单个条目中。例如，1m30s 相当于 90 秒。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>访问令牌生存期</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="accessTokenLength" use="optional" default="40" type="longType"> 
            <xsd:annotation> 
                <xsd:documentation>生成的 OAuth 访问令牌的长度。完整应用程序服务器概要文件中的等效提供程序参数为 oauth20.access.token.length。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>访问令牌长度</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="issueRefreshToken" use="optional" default="true" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>值为 false 时禁止生成和使用刷新标记。完整应用程序服务器概要文件中的等效提供程序参数为 oauth20.issue.refresh.token。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>刷新标记使用标志</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="refreshTokenLength" use="optional" default="50" type="longType"> 
            <xsd:annotation> 
                <xsd:documentation>生成的刷新令牌的长度。完整应用程序服务器概要文件中的等效提供程序参数为 oauth20.refresh.token.length。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>刷新令牌长度</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="refreshedAccessTokenLimit" use="optional" default="100" type="longType"> 
            <xsd:annotation> 
                <xsd:documentation>可以通过对单个 OAuth 客户机和用户组合使用刷新令牌来创建的最大访问令牌数。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>已刷新访问令牌限制</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="revokeAccessTokensWithRefreshTokens" use="optional" default="true" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>如果值为 false，那么当撤销了刷新令牌时，将禁用撤销相关联的访问令牌。缺省值为 true。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>当撤销了刷新令牌时，撤销访问令牌</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="libraryRef" use="optional" type="pidType"> 
            <xsd:annotation> 
                <xsd:documentation>对包含介体插件类的共享库的引用。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>共享库引用</ext:label> 
                    <ext:reference>library</ext:reference> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="allowPublicClients" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>值为 false 时禁止公共客户机访问，详细描述可参见 OAuth 规范。完整应用程序服务器概要文件中的等效提供程序参数为 oauth20.allow.public.clients。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>允许公共客户机访问 OAuth 提供程序</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="authorizationFormTemplate" use="optional" default="template.html" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>定制授权页面模板的 URL。完整应用程序服务器概要文件中的等效提供程序参数为 oauth20.authorization.form.template。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>定制授权表单</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="authorizationErrorTemplate" use="optional" default="" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>定制授权错误页面模板的 URL。完整应用程序服务器概要文件中的等效提供程序参数为 oauth20.authorization.error.template。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>定制授权错误表单</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="customLoginURL" use="optional" default="login.jsp" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>定制登录页面的 URL。完整应用程序服务器概要文件中的等效提供程序参数为 oauth20.authorization.loginURL。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>定制登录表单</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="autoAuthorizeParam" use="optional" default="autoauthz" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>要使用自动授权，请将 autoAuthorize 参数（值为 true）追加到请求。完整应用程序服务器概要文件中的等效提供程序参数为 oauth20.autoauthorize.param。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>自动授权参数</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="autoAuthorize" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>要使用自动授权，请将 autoAuthorize 参数（值为 true）追加到请求。完整应用程序服务器概要文件中的等效提供程序参数为 oauth20.autoauthorize.param。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>自动授权参数</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="clientURISubstitutions" use="optional" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>用于替换用于动态主机名的客户机 URI 字符串的可选值。完整应用程序服务器概要文件中的等效提供程序参数为 oauth20.client.uri.substitutions。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>客户机 URI 替换</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="clientTokenCacheSize" use="optional" type="longType"> 
            <xsd:annotation> 
                <xsd:documentation>客户机标记高速缓存中的最大条目数。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>客户机标记高速缓存大小</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="userClientTokenLimit" use="optional" type="longType"> 
            <xsd:annotation> 
                <xsd:documentation>每个用户和客户机组合的令牌限制。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>用户客户机令牌限制</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="filter" use="optional" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>URI 过滤器选择要由此提供程序授权的请求。完整应用程序服务器概要文件中的等效提供程序参数为 Filter。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>请求过滤器</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="characterEncoding" use="optional" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>将请求字符编码设置为此值。完整应用程序服务器概要文件中的等效提供程序参数为 characterEncoding。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>字符编码</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="oauthOnly" use="optional" default="true" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>如果值为 true，那么与过滤器相匹配的请求必须具有访问令牌，否则它们将失败。如果为 false，那么没有访问令牌时系统将检查匹配请求中是否存在其他认证数据。完整应用程序服务器概要文件中的等效提供程序参数为 oauthOnly。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>如果缺少 OAuth 访问令牌，那么认证失败</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="includeTokenInSubject" use="optional" default="true" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>如果值为 true，那么会将 com.ibm.wsspi.security.oauth20.token.WSOAuth20Token 作为专用凭证来添加。完整应用程序服务器概要文件中的等效提供程序参数为 includeToken。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>在主题集中包括 OAuth 标记</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="consentCacheEntryLifetime" use="optional" default="1800" type="secondDuration"> 
            <xsd:annotation> 
                <xsd:documentation>准许的高速缓存中条目有效的时间（秒）。. 指定后跟时间单位的正整数，时间单位可以是小时 (h)、分钟 (m) 或秒 (s)。例如，以 30s 的形式指定 30 秒。可将多个值包括在单个条目中。例如，1m30s 相当于 90 秒。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>准许的高速缓存条目生存期</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="consentCacheSize" use="optional" default="1000" type="longType"> 
            <xsd:annotation> 
                <xsd:documentation>准许的高速缓存中允许的最大条目数。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>准许的高速缓存大小</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="accessTokenCacheEnabled" use="optional" default="true" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>启用缓存以将访问令牌保存在数据库和内存中缓存中。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>缓存访问令牌</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="certAuthentication" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>在 HTTPS 请求中启用客户机证书的认证。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>启用客户机证书认证</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="allowCertAuthentication" use="optional" default="true" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>允许在 HTTPS 请求中包含客户机证书时进行客户机证书认证。此属性允许使用客户机证书进行认证，即使在 certAuthentication 属性设置为 false 的情况下。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>允许客户机证书认证</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="allowSpnegoAuthentication" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>允许认证 HTTPS 请求中的 SPNEGO 令牌。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>允许 SPNEGO 认证</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="jwtAccessToken" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>生成 JSON Web 令牌，作为字符串进行序列化并作为访问令牌。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>访问令牌为 JWT</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="coverageMapSessionMaxAge" use="optional" default="600" type="secondDuration"> 
            <xsd:annotation> 
                <xsd:documentation>指示覆盖范围映射服务的 cache-control 头的 max-age 值（秒数）。. 指定后跟时间单位的正整数，时间单位可以是小时 (h)、分钟 (m) 或秒 (s)。例如，以 30s 的形式指定 30 秒。可将多个值包括在单个条目中。例如，1m30s 相当于 90 秒。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>覆盖范围映射会话最长寿命</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="skipResourceOwnerValidation" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>如果值为 true，那么会跳过资源所有者验证。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>跳过资源所有者验证</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="accessTokenEncoding" use="optional" default="plain"> 
            <xsd:annotation> 
                <xsd:documentation>所存储访问令牌的编码类型。缺省类型设置为 plain，以与之前版本兼容。建议使用 PBKDF2WithHmacSHA512。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>访问令牌编码</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
            <xsd:simpleType> 
                <xsd:restriction base="xsd:string"> 
                    <xsd:enumeration value="plain"> 
                        <xsd:annotation> 
                            <xsd:documentation>plain</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                    <xsd:enumeration value="PBKDF2WithHmacSHA512"> 
                        <xsd:annotation> 
                            <xsd:documentation>PBKDF2WithHmacSHA512</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                </xsd:restriction> 
            </xsd:simpleType> 
        </xsd:attribute> 
        <xsd:attribute name="logoutRedirectURL" use="optional" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>成功调用注销端点后客户机重定向至的可选 URL。如果未提供 URL，那么会使用基本缺省注销页面。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>注销之后使用的 URL</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="passwordGrantRequiresAppPassword" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>设置为 true 时，使用资源所有者密码凭证授权类型的 OAuth 授权流程必须使用应用程序密码，而不是使用用户注册表中为用户配置的密码。启用了此属性时，OAuth 客户机必须从此 OAuth 提供程序获取应用程序密码，才能使用密码授权类型。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>密码授予需要应用程序密码</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="appPasswordLifetime" use="optional" default="90d" type="secondDuration"> 
            <xsd:annotation> 
                <xsd:documentation>指定此 OAuth 提供程序生成的应用程序密码的生命周期。. 指定后跟时间单位的正整数，时间单位可以是小时 (h)、分钟 (m) 或秒 (s)。例如，以 30s 的形式指定 30 秒。可将多个值包括在单个条目中。例如，1m30s 相当于 90 秒。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>应用程序密码生命周期</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="appTokenLifetime" use="optional" default="90d" type="secondDuration"> 
            <xsd:annotation> 
                <xsd:documentation>指定此 OAuth 提供程序生成的应用程序令牌的生命周期。. 指定后跟时间单位的正整数，时间单位可以是小时 (h)、分钟 (m) 或秒 (s)。例如，以 30s 的形式指定 30 秒。可将多个值包括在单个条目中。例如，1m30s 相当于 90 秒。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>应用程序令牌生命周期</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="appTokenOrPasswordLimit" use="optional" default="100" type="longType"> 
            <xsd:annotation> 
                <xsd:documentation>每个用户和客户机组合的令牌限制。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>应用程序令牌或密码限制</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="internalClientId" use="optional" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>将用来生成 PersonalTokenManagement 和 UsersTokenManagement UI 页面令牌的现有客户机的标识。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>用来生成令牌的客户机标识</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="internalClientSecret" use="optional" type="password"> 
            <xsd:annotation> 
                <xsd:documentation>将用来生成 PersonalTokenManagement 和 UsersTokenManagement UI 页面令牌的现有客户机的密钥。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>用来生成令牌的客户机密钥</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="clientSecretEncoding" use="optional" default="xor"> 
            <xsd:annotation> 
                <xsd:documentation>所存储客户机密钥的编码类型。缺省类型设置为 XOR，以与之前版本兼容。建议使用 PBKDF2WithHmacSHA512。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>客户机密钥编码</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
            <xsd:simpleType> 
                <xsd:restriction base="xsd:string"> 
                    <xsd:enumeration value="xor"> 
                        <xsd:annotation> 
                            <xsd:documentation>xor</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                    <xsd:enumeration value="PBKDF2WithHmacSHA512"> 
                        <xsd:annotation> 
                            <xsd:documentation>PBKDF2WithHmacSHA512</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                </xsd:restriction> 
            </xsd:simpleType> 
        </xsd:attribute> 
        <xsd:attribute name="ropcPreferUserSecurityName" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>如果用户安全性名称不同于 ROPC（资源所有者密码凭证）请求中所提供的用户名，那么将用户名设置为 ROPC 授权类型所创建的所有令牌的用户安全性名称。缺省值为 false。如果 ropcPreferUserSecurityName 和 ropcPreferUserPrincipalName 都设置为 true，那么 ropcPreferUserPrincipalName 优先。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>ROPC 授权更愿意使用用户安全性名称</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="ropcPreferUserPrincipalName" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>如果用户主体名称不同于 ROPC（资源所有者密码凭证）请求中所提供的用户名，那么将用户名设置为 ROPC 授权类型所创建的所有令牌的用户主体名称。缺省值为 false。如果 ropcPreferUserSecurityName 和 ropcPreferUserPrincipalName 都设置为 true，那么 ropcPreferUserPrincipalName 优先。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>ROPC 授权更愿意使用用户主体名称</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="trackOAuthClients" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>跟踪所有与 OAuth 提供程序交互的 OAuth 客户机。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>跟踪 OAuth 客户机</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:anyAttribute processContents="skip"> 
        </xsd:anyAttribute> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.security.oauth20.provider-factory"> 
        <xsd:complexContent> 
            <xsd:extension base="com.ibm.ws.security.oauth20.provider"> 
                <xsd:attribute name="id" type="factoryIdType" use="optional"> 
                    <xsd:annotation> 
                        <xsd:documentation>唯一配置标识。</xsd:documentation> 
                        <xsd:appinfo> 
                            <ext:label>标识</ext:label> 
                        </xsd:appinfo> 
                    </xsd:annotation> 
                </xsd:attribute> 
            </xsd:extension> 
        </xsd:complexContent> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.security.openidconnect.client.webapp"> 
        <xsd:annotation> 
            <xsd:documentation>服务的描述</xsd:documentation> 
            <xsd:appinfo> 
                <ext:label>OpenID Connect 客户机 Web 应用程序</ext:label> 
            </xsd:appinfo> 
        </xsd:annotation><!--Idiom for "empty element context" (whitespace accepted and ignored)--> 
        <xsd:sequence> 
            <xsd:sequence/> 
        </xsd:sequence> 
        <xsd:attribute name="contextPath" use="optional" default="/oidcclient" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>服务的描述</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>上下文路径</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:anyAttribute processContents="skip"> 
        </xsd:anyAttribute> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.security.openidconnect.client.oidcClientConfig"> 
        <xsd:annotation> 
            <xsd:documentation>OpenID Connect 客户机。</xsd:documentation> 
            <xsd:appinfo> 
                <ext:label>OpenID Connect 客户机</ext:label> 
            </xsd:appinfo> 
        </xsd:annotation> 
        <xsd:choice minOccurs="0" maxOccurs="unbounded"> 
            <xsd:element name="authzParameter" type="com.ibm.ws.security.openidconnect.client.param-factory"> 
                <xsd:annotation> 
                    <xsd:documentation>指定要发送至 OpenID Connect 提供程序的授权端点的定制参数。</xsd:documentation> 
                    <xsd:appinfo> 
                        <ext:label>授权端点的定制参数</ext:label> 
                    </xsd:appinfo> 
                </xsd:annotation> 
            </xsd:element> 
            <xsd:element name="tokenParameter" type="com.ibm.ws.security.openidconnect.client.param-factory"> 
                <xsd:annotation> 
                    <xsd:documentation>指定要发送至 OpenID Connect 提供程序的令牌端点的定制参数。</xsd:documentation> 
                    <xsd:appinfo> 
                        <ext:label>令牌端点的定制参数</ext:label> 
                    </xsd:appinfo> 
                </xsd:annotation> 
            </xsd:element> 
            <xsd:element name="audiences" type="xsd:string"> 
                <xsd:annotation> 
                    <xsd:documentation>指定以逗号分隔的可信受众列表，以根据 JSON Web 令牌中的受众声明进行验证。</xsd:documentation> 
                    <xsd:appinfo> 
                        <ext:label>可信受众</ext:label> 
                    </xsd:appinfo> 
                </xsd:annotation> 
            </xsd:element> 
            <xsd:element name="authFilter" type="com.ibm.ws.security.authentication.filter"> 
                <xsd:annotation> 
                    <xsd:documentation>指定认证过滤器参考。</xsd:documentation> 
                    <xsd:appinfo> 
                        <ext:label>认证过滤器参考</ext:label> 
                    </xsd:appinfo> 
                </xsd:annotation> 
            </xsd:element> 
            <xsd:element name="resource" type="xsd:string"> 
                <xsd:annotation> 
                    <xsd:documentation>请求中包含了资源参数。</xsd:documentation> 
                    <xsd:appinfo> 
                        <ext:label>资源请求参数</ext:label> 
                    </xsd:appinfo> 
                </xsd:annotation> 
            </xsd:element> 
            <xsd:element name="forwardLoginParameter" type="xsd:string"> 
                <xsd:annotation> 
                    <xsd:documentation>指定要转发给 OpenID Connect 提供程序的参数名称的逗号分隔列表。如果受保护资源请求包含一个或多个指定的参数，那么 OpenID Connect 客户机会将那些参数及其值包含在对 OpenID Connect 提供程序的授权端点请求中。</xsd:documentation> 
                    <xsd:appinfo> 
                        <ext:label>转发登录参数</ext:label> 
                    </xsd:appinfo> 
                </xsd:annotation> 
            </xsd:element> 
        </xsd:choice> 
        <xsd:attribute name="scope" use="optional" default="openid profile" type="tokenType"> 
            <xsd:annotation> 
                <xsd:documentation>提供程序允许的 OpenID Connect 作用域（如 OpenID Connect 规范中详述）。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>作用域</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="userIdentityToCreateSubject" use="optional" default="sub" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>在标识令牌中指定用作主体集中用户主体名称的 JSON 属性。如果未指定值，那么会使用 JSON 属性“sub”。此属性的值将被 userIdentifier 的值（如果指定的话）覆盖。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>用于创建主题集的用户身份</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="httpsRequired" use="optional" default="true" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>在 OpenID 依赖方与提供程序服务之间需要 SSL 通信。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>需要 SSL 通信</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="grantType" use="optional" default="authorization_code"> 
            <xsd:annotation> 
                <xsd:documentation>指定要用于此客户机的授权类型。将改为优先使用 responseType 属性。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>授予类型</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
            <xsd:simpleType> 
                <xsd:restriction base="xsd:string"> 
                    <xsd:enumeration value="authorization_code"> 
                        <xsd:annotation> 
                            <xsd:documentation>授权代码授予类型</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                    <xsd:enumeration value="implicit"> 
                        <xsd:annotation> 
                            <xsd:documentation>隐式授予类型</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                </xsd:restriction> 
            </xsd:simpleType> 
        </xsd:attribute> 
        <xsd:attribute name="clientId" use="optional" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>客户的身份。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>客户身份</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="clientSecret" use="optional" type="password"> 
            <xsd:annotation> 
                <xsd:documentation>客户机的密钥。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>客户机密钥</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="redirectToRPHostAndPort" use="optional" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>授权后，依赖方将重定向至此目标而不是缺省位置。缺省位置是依赖方请求的源。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>重定向发送至依赖方的目标</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="redirectJunctionPath" use="optional" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>指定要插入到重定向 URL 中（在主机名和端口后）的路径片段。缺省值为空字符串。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>插入至重定向 URL 的路径片段</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="isClientSideRedirectSupported" use="optional" default="true" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>如果您不希望使用 JavaScript 将初始认证请求重定向到 OpenID Connect 提供程序，请将此属性设置为 false。如果未使用 JavaScript，那么原始入站请求中存在的任何 URI 片段都将丢失。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>客户机端重定向</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="issuerIdentifier" use="optional" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>一个使用 HTTPS 方案的 URL（区分大小写），其中包含方案、主机以及（可选）端口号和路径部分。请以逗号分隔列表的形式指定多个值。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>颁发者标识</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="mapIdentityToRegistryUser" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>指定是否将身份映射至注册表用户。如果此项设置为 false，那么不会使用用户注册表来创建用户主题。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>将身份映射至注册表用户</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="trustStoreRef" use="optional" type="pidType"> 
            <xsd:annotation> 
                <xsd:documentation>密钥库，包含在验证标识令牌的签名时必需的公用密钥。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>信任密钥库</ext:label> 
                    <ext:reference>keyStore</ext:reference> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="trustAliasName" use="optional" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>用于找到公用密钥（与非对称算法一起用于签名验证）的密钥别名名称。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>信任别名</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="nonceEnabled" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>在授权代码流程中启用现时标志参数。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>启用现时标志参数</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="realmName" use="optional" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>指定 mapIdentityToRegistryUser 设置为 false 时用于创建用户主体集的域名。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>域名</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="sslRef" use="optional" type="pidType"> 
            <xsd:annotation> 
                <xsd:documentation>指定用于连接至 OpenID Connect 提供程序的 SSL 配置的标识。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>SSL 引用</ext:label> 
                    <ext:reference>ssl</ext:reference> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="signatureAlgorithm" use="optional" default="HS256"> 
            <xsd:annotation> 
                <xsd:documentation>指定将用于验证标识令牌的签名的签名算法。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>标识令牌签名算法</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
            <xsd:simpleType> 
                <xsd:restriction base="xsd:string"> 
                    <xsd:enumeration value="none"> 
                        <xsd:annotation> 
                            <xsd:documentation>不需要为令牌签名</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                    <xsd:enumeration value="RS256"> 
                        <xsd:annotation> 
                            <xsd:documentation>使用 RS256 签名算法为令牌签名和验证令牌</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                    <xsd:enumeration value="RS384"> 
                        <xsd:annotation> 
                            <xsd:documentation>使用 RS384 签名算法为令牌签名和验证令牌</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                    <xsd:enumeration value="RS512"> 
                        <xsd:annotation> 
                            <xsd:documentation>使用 RS512 签名算法为令牌签名和验证令牌</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                    <xsd:enumeration value="HS256"> 
                        <xsd:annotation> 
                            <xsd:documentation>使用 HS256 签名算法为令牌签名和验证令牌</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                    <xsd:enumeration value="HS384"> 
                        <xsd:annotation> 
                            <xsd:documentation>使用 HS384 签名算法为令牌签名和验证令牌</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                    <xsd:enumeration value="HS512"> 
                        <xsd:annotation> 
                            <xsd:documentation>使用 HS512 签名算法为令牌签名和验证令牌</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                    <xsd:enumeration value="ES256"> 
                        <xsd:annotation> 
                            <xsd:documentation>使用 ES256 签名算法为令牌签名和验证令牌</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                    <xsd:enumeration value="ES384"> 
                        <xsd:annotation> 
                            <xsd:documentation>使用 ES384 签名算法为令牌签名和验证令牌</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                    <xsd:enumeration value="ES512"> 
                        <xsd:annotation> 
                            <xsd:documentation>使用 ES512 签名算法为令牌签名和验证令牌</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                </xsd:restriction> 
            </xsd:simpleType> 
        </xsd:attribute> 
        <xsd:attribute name="includeIdTokenInSubject" use="optional" default="true" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>指定是否在客户机主题中包括标识令牌。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>在客户机主题中包括标识令牌</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="accessTokenInLtpaCookie" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>指定 LTPA 令牌是否包含访问令牌。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>LTPA 令牌包含访问令牌</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="initialStateCacheCapacity" use="optional" default="3000" type="intType"> 
            <xsd:annotation> 
                <xsd:documentation>指定状态高速缓存的起始容量。此容量在本身需要时会增长。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>状态高速缓存的起始容量</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="hostNameVerificationEnabled" use="optional" default="true" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>指定是否启用主机名验证。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>启用主机名验证</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="discoveryEndpointUrl" use="optional" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>指定 OpenID Connect 提供程序的发现端点 URL。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>发现端点 URL</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="authorizationEndpointUrl" use="optional" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>指定授权端点 URL。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>授权端点 URL</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="tokenEndpointUrl" use="optional" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>指定令牌端点 URL。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>令牌端点 URL</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="userInfoEndpointUrl" use="optional" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>指定用户信息端点 URL</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>用户信息端点 URL</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="userInfoEndpointEnabled" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>指定是否与用户信息端点联系。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>启用用户信息端点</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="jwkEndpointUrl" use="optional" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>指定 JWK 端点 URL。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>JSON Web 密钥 (JWK) 端点 URL</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="jwkClientId" use="optional" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>指定要包括在 JWK 请求的基本认证方案中的客户机标识。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>JWK 客户机标识</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="jwkClientSecret" use="optional" type="password"> 
            <xsd:annotation> 
                <xsd:documentation>指定要包括在 JWK 请求的基本认证方案中的客户机密码。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>JWK 客户机密码</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="responseType" use="optional"> 
            <xsd:annotation> 
                <xsd:documentation>指定从提供程序请求的响应（授权代码或隐式流程令牌）。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>响应类型</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
            <xsd:simpleType> 
                <xsd:restriction base="xsd:string"> 
                    <xsd:enumeration value="code"> 
                        <xsd:annotation> 
                            <xsd:documentation>授权代码</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                    <xsd:enumeration value="id_token"> 
                        <xsd:annotation> 
                            <xsd:documentation>标识令牌</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                    <xsd:enumeration value="id_token token"> 
                        <xsd:annotation> 
                            <xsd:documentation>标识令牌和访问令牌</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                    <xsd:enumeration value="token"> 
                        <xsd:annotation> 
                            <xsd:documentation>访问令牌</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                </xsd:restriction> 
            </xsd:simpleType> 
        </xsd:attribute> 
        <xsd:attribute name="userIdentifier" use="optional" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>在标识令牌中指定用作主题中用户主体名称的 JSON 属性。如果未指定值，那么会使用 JSON 属性“sub”。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>主体标识</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="groupIdentifier" use="optional" default="groupIds" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>在标识令牌中指定用作已认证主体所属组的名称的 JSON 属性。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>组标识</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="realmIdentifier" use="optional" default="realmName" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>在标识令牌中指定用作域名的 JSON 属性。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>域标识</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="uniqueUserIdentifier" use="optional" default="uniqueSecurityName" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>在标识令牌中指定应用到主题中 WSCredential 时用作唯一用户名的 JSON 属性。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>唯一标识</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="tokenEndpointAuthMethod" use="optional" default="post"> 
            <xsd:annotation> 
                <xsd:documentation>用于将凭证发送至 OpenID Connect 提供程序的令牌端点以便验证客户机的方法。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>令牌端点的认证方法</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
            <xsd:simpleType> 
                <xsd:restriction base="xsd:string"> 
                    <xsd:enumeration value="basic"> 
                        <xsd:annotation> 
                            <xsd:documentation>使用 HTTP Basic 身份验证方案，通过 OpenID 连接提供程序的令牌端点对客户端进行身份验证。</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                    <xsd:enumeration value="post"> 
                        <xsd:annotation> 
                            <xsd:documentation>在请求正文中包含客户端凭据，以便使用 OpenID 连接提供程序的令牌端点对客户端进行身份验证。</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                    <xsd:enumeration value="private_key_jwt"> 
                        <xsd:annotation> 
                            <xsd:documentation>专用密钥 JWT 客户机认证。</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                </xsd:restriction> 
            </xsd:simpleType> 
        </xsd:attribute> 
        <xsd:attribute name="tokenEndpointAuthSigningAlgorithm" use="optional" default="RS256"> 
            <xsd:annotation> 
                <xsd:documentation>用于对令牌（用于客户机认证）进行签名的签名算法。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>令牌端点认证签名算法</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
            <xsd:simpleType> 
                <xsd:restriction base="xsd:string"> 
                    <xsd:enumeration value="RS256"> 
                        <xsd:annotation> 
                            <xsd:documentation>使用 RS256 签名算法对用于客户机认证的令牌进行签名</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                    <xsd:enumeration value="RS384"> 
                        <xsd:annotation> 
                            <xsd:documentation>使用 RS384 签名算法对用于客户机认证的令牌进行签名</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                    <xsd:enumeration value="RS512"> 
                        <xsd:annotation> 
                            <xsd:documentation>使用 RS512 签名算法对用于客户机认证的令牌进行签名</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                    <xsd:enumeration value="ES256"> 
                        <xsd:annotation> 
                            <xsd:documentation>使用 ES256 签名算法对用于客户机认证的令牌进行签名</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                    <xsd:enumeration value="ES384"> 
                        <xsd:annotation> 
                            <xsd:documentation>使用 ES384 签名算法对用于客户机认证的令牌进行签名</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                    <xsd:enumeration value="ES512"> 
                        <xsd:annotation> 
                            <xsd:documentation>使用 ES512 签名算法对用于客户机认证的令牌进行签名</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                </xsd:restriction> 
            </xsd:simpleType> 
        </xsd:attribute> 
        <xsd:attribute name="keyAliasName" use="optional" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>用于对客户机认证令牌进行签名的专用密钥的别名。 对应于专用密钥的公用密钥也必须使用此别名。 专用密钥必须位于由 sslRef 属性引用的 SSL 配置指定的密钥库中。 公用密钥必须位于以下一个位置：由 trustStoreRef 属性指定的信任库、由 sslRef 属性引用的 SSL 配置指定的信任库，或由 sslRef 属性引用的 SSL 配置指定的密钥库。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>密钥别名名称</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="inboundPropagation" use="optional" default="none"> 
            <xsd:annotation> 
                <xsd:documentation>控制 OpenID 依赖方的令牌入站传播的操作。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>启用令牌入站传播</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
            <xsd:simpleType> 
                <xsd:restriction base="xsd:string"> 
                    <xsd:enumeration value="supported"> 
                        <xsd:annotation> 
                            <xsd:documentation>支持入站令牌传播</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                    <xsd:enumeration value="required"> 
                        <xsd:annotation> 
                            <xsd:documentation>需要入站令牌传播</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                    <xsd:enumeration value="none"> 
                        <xsd:annotation> 
                            <xsd:documentation>不支持入站令牌传播</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                </xsd:restriction> 
            </xsd:simpleType> 
        </xsd:attribute> 
        <xsd:attribute name="validationMethod" use="optional" default="introspect"> 
            <xsd:annotation> 
                <xsd:documentation>用于对令牌入站传播进行验证的方法。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>用于验证入站令牌的方法</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
            <xsd:simpleType> 
                <xsd:restriction base="xsd:string"> 
                    <xsd:enumeration value="introspect"> 
                        <xsd:annotation> 
                            <xsd:documentation>使用令牌自省来验证入站令牌</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                    <xsd:enumeration value="userinfo"> 
                        <xsd:annotation> 
                            <xsd:documentation>使用 userinfo 端点来验证入站令牌</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                </xsd:restriction> 
            </xsd:simpleType> 
        </xsd:attribute> 
        <xsd:attribute name="jwtAccessTokenRemoteValidation" use="optional" default="none"> 
            <xsd:annotation> 
                <xsd:documentation>指定所接收的用于入站传播的 JWT 访问令牌是由 OpenID Connect 客户机在本地验证，还是使用 OpenID Connect 客户机所配置的验证方法进行验证。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>JWT 访问令牌远程验证</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
            <xsd:simpleType> 
                <xsd:restriction base="xsd:string"> 
                    <xsd:enumeration value="none"> 
                        <xsd:annotation> 
                            <xsd:documentation>所接收的用于入站传播的 JWT 访问令牌是由 OpenID Connect 客户机在本地进行解析和验证。如果验证失败，那么不会使用 OpenID Connect 客户机所配置的验证方法来验证该访问令牌。</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                    <xsd:enumeration value="allow"> 
                        <xsd:annotation> 
                            <xsd:documentation>所接收的用于入站传播的 JWT 访问令牌是由 OpenID Connect 客户机在本地进行解析和验证。如果验证失败，那么会使用 OpenID Connect 客户机所配置的验证方法来验证该访问令牌。</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                    <xsd:enumeration value="require"> 
                        <xsd:annotation> 
                            <xsd:documentation>使用 OpenID Connect 客户机所配置的验证方法来验证所接收的用于入站传播的 JWT 访问令牌。OpenID Connect 客户机不会在本地解析或验证该访问令牌。</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                </xsd:restriction> 
            </xsd:simpleType> 
        </xsd:attribute> 
        <xsd:attribute name="headerName" use="optional" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>在请求中携带了入站令牌的头的名称。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>包含入站令牌的头的名称</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="validationEndpointUrl" use="optional" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>用于验证令牌入站传播的端点 URL。端点的类型由 validationMethod 决定。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>用于验证的端点 URL</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="disableIssChecking" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>当 OpenID Connect 客户机验证用于入站传播的 JWT 访问令牌或执行入站传播的令牌内部检查时，要求签发者声明不存在。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>禁用发布者检查</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="authnSessionDisabled" use="optional" default="true" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>将不会为入站传播创建认证会话 Cookie。客户机预期会针对每个请求发送一个有效 OAuth 令牌。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>认证会话 Cookie 已禁用</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="disableLtpaCookie" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>请勿在处理 OAuth 令牌期间创建 LTPA 令牌。改为创建特定服务提供程序的 cookie。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>禁用 LTPA 令牌</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="useSystemPropertiesForHttpClientConnections" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>指定是否在 OpenID Connect 客户机创建 HTTP 客户机连接时使用 Java 系统属性。如果希望这些连接使用 http* 或 javax* 系统属性，请将此属性设置为 true。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>将系统属性用于 HTTP 客户机连接</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="reAuthnOnAccessTokenExpire" use="optional" default="true" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>当用户的认证访问令牌到期，而 disableLtpaCookie 设置为 true 时，请再次认证该用户。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>访问令牌到期时重新认证</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="reAuthnCushion" use="optional" default="0s" type="duration"> 
            <xsd:annotation> 
                <xsd:documentation>当用户的令牌将要到期时，要再次认证该用户的时间段。标识令牌的到期时间由其 exp 声明指定。. 指定后跟时间单位的正整数，时间单位可以是小时 (h)、分钟 (m)、秒 (s) 或毫秒 (ms)。例如，以 500ms 的形式指定 500 毫秒。可将多个值包括在单个条目中。例如，1s500ms 相当于 1.5 秒。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>重新认证的缓冲时间</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="allowCustomCacheKey" use="optional" default="true" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>指定是否使用定制高速缓存键将用户存储在认证高速缓存中。如果此属性设置为 true，并且在认证高速缓存中找不到用户的高速缓存密钥，那么将要求用户再次登录。当您使用 jwtSso 功能时，请将此属性设置为 false，以允许直接从 jwtSso cookie 构造安全主体集。当您不使用 jwtSso 功能时，请将此属性设置为 false，以允许直接从用户注册表构造安全主体集。如果从用户注册表重新构造安全主体集，那么该主体集中将没有 SSO 组件。如果有多个服务器使用您的 LTPA cookie，请考虑将此属性设置为 false。如果应用程序始终要求主体集中存在 SSO 组件，那么必须将此属性设置为 true，或者使用 jwtSso 功能。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>允许定制缓存键</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="clockSkew" use="optional" default="300s" type="duration"> 
            <xsd:annotation> 
                <xsd:documentation>指定验证 JSON Web 令牌时允许的时钟偏差（秒）。. 指定后跟时间单位的正整数，时间单位可以是小时 (h)、分钟 (m)、秒 (s) 或毫秒 (ms)。例如，以 500ms 的形式指定 500 毫秒。可将多个值包括在单个条目中。例如，1s500ms 相当于 1.5 秒。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>时钟偏差</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="authFilterRef" use="optional" type="pidType"> 
            <xsd:annotation> 
                <xsd:documentation>指定认证过滤器参考。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>认证过滤器参考</ext:label> 
                    <ext:reference>authFilter</ext:reference> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="createSession" use="optional" default="true" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>指定在当前 HttpSession 不存在的情况下是否创建 HttpSession。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>创建会话</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="authenticationTimeLimit" use="optional" default="420s" type="duration"> 
            <xsd:annotation> 
                <xsd:documentation>重定向至认证服务器与从认证服务器返回之间的最长持续时间（以毫秒计）。在此持续时间后，cookie 将到期。. 指定后跟时间单位的正整数，时间单位可以是小时 (h)、分钟 (m)、秒 (s) 或毫秒 (ms)。例如，以 500ms 的形式指定 500 毫秒。可将多个值包括在单个条目中。例如，1s500ms 相当于 1.5 秒。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>认证时间限制</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="discoveryPollingRate" use="optional" default="300s" type="duration"> 
            <xsd:annotation> 
                <xsd:documentation>OpenID Connect 客户机检查对发现文件的更新的时间间隔（以毫秒计）。仅当认证失败时，才会执行检查。. 指定后跟时间单位的正整数，时间单位可以是小时 (h)、分钟 (m)、秒 (s) 或毫秒 (ms)。例如，以 500ms 的形式指定 500 毫秒。可将多个值包括在单个条目中。例如，1s500ms 相当于 1.5 秒。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>下一次发现时间间隔</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="tokenReuse" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>指定能否复用 JSON Web 令牌。令牌必须包含此属性的 jti 声明才有效。jti 声明是与 iss 声明一起使用的令牌标识，以唯一标识令牌并使其与特定签发者相关联。此属性设置为 false 时，如果某个请求包含带有 jti 和 iss 值组合的 JWT，并且在令牌的生命周期内已使用了该值组合，那么此请求会受到拒绝。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>允许复用 JSON Web 令牌 (JWT)</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="keyManagementKeyAlias" use="optional" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>这是密钥管理密钥的专用密钥别名，用于将 JSON Web 加密 (JWE) 令牌的内容加密密钥解密。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>密钥管理密钥别名</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="accessTokenCacheEnabled" use="optional" default="true" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>指定是否高速缓存了使用所传播的访问令牌创建的已认证主题。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>启用访问令牌高速缓存</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="accessTokenCacheTimeout" use="optional" default="5m" type="duration"> 
            <xsd:annotation> 
                <xsd:documentation>指定对通过使用所传播的访问令牌创建的已认证主题进行高速缓存的时间长度。. 指定后跟时间单位的正整数，时间单位可以是小时 (h)、分钟 (m)、秒 (s) 或毫秒 (ms)。例如，以 500ms 的形式指定 500 毫秒。可将多个值包括在单个条目中。例如，1s500ms 相当于 1.5 秒。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>访问令牌高速缓存超时</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="pkceCodeChallengeMethod" use="optional" default="disabled"> 
            <xsd:annotation> 
                <xsd:documentation>指定要用于 Proof Key for Code Exchange (PKCE) 的质询方法。 启用 PKCE 时，由于使用的是本地缓存，因此需要会话亲和。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>用于代码交换质询方法的验证密钥</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
            <xsd:simpleType> 
                <xsd:restriction base="xsd:string"> 
                    <xsd:enumeration value="disabled"> 
                        <xsd:annotation> 
                            <xsd:documentation>已禁用</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                    <xsd:enumeration value="plain"> 
                        <xsd:annotation> 
                            <xsd:documentation>普通</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                    <xsd:enumeration value="S256"> 
                        <xsd:annotation> 
                            <xsd:documentation>S256</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                </xsd:restriction> 
            </xsd:simpleType> 
        </xsd:attribute> 
        <xsd:attribute name="tokenRequestOriginHeader" use="optional" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>指定要在源 HTTP 头中使用的值，该头包含在针对 OpenID Connect 提供程序的令牌端点发出的 HTTP POST 请求中。 如果未指定，那么源 HTTP 头不会包含在请求中。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>令牌请求源头</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="tokenOrderToFetchCallerClaims" use="optional" default="IDToken"> 
            <xsd:annotation> 
                <xsd:documentation>指定用于访存调用者名称和组声明值的令牌顺序。 如果第一个令牌中不存在该声明，那么 OpenID Connect 客户机将继续搜索列表中的其他令牌。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>指定一个或多个令牌以访存调用者声明</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
            <xsd:simpleType> 
                <xsd:restriction base="xsd:string"> 
                    <xsd:enumeration value="IDToken"> 
                        <xsd:annotation> 
                            <xsd:documentation>仅使用 IDToken 来确定调用者声明</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                    <xsd:enumeration value="AccessToken IDToken UserInfo"> 
                        <xsd:annotation> 
                            <xsd:documentation>按此顺序使用 AccessToken， IDToken 和 Userinfo 令牌来确定调用者声明</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                </xsd:restriction> 
            </xsd:simpleType> 
        </xsd:attribute> 
        <xsd:anyAttribute processContents="skip"> 
        </xsd:anyAttribute> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.security.openidconnect.client.oidcClientConfig-factory"> 
        <xsd:complexContent> 
            <xsd:extension base="com.ibm.ws.security.openidconnect.client.oidcClientConfig"> 
                <xsd:attribute name="id" type="factoryIdType" use="optional"> 
                    <xsd:annotation> 
                        <xsd:documentation>唯一配置标识。</xsd:documentation> 
                        <xsd:appinfo> 
                            <ext:label>标识</ext:label> 
                        </xsd:appinfo> 
                    </xsd:annotation> 
                </xsd:attribute> 
            </xsd:extension> 
        </xsd:complexContent> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.security.openidconnect.client.param"> 
        <xsd:annotation> 
            <xsd:documentation>指定要包括在将发送至提供程序的请求中的其他参数。</xsd:documentation> 
            <xsd:appinfo> 
                <ext:label>要包括在请求中的定制参数</ext:label> 
            </xsd:appinfo> 
        </xsd:annotation><!--Idiom for "empty element context" (whitespace accepted and ignored)--> 
        <xsd:sequence> 
            <xsd:sequence/> 
        </xsd:sequence> 
        <xsd:attribute name="name" use="optional" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>指定其他参数的名称。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>定制请求参数名称</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="value" use="optional" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>指定其他参数的值。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>定制请求参数值</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:anyAttribute processContents="skip"> 
        </xsd:anyAttribute> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.security.openidconnect.client.param-factory"> 
        <xsd:complexContent> 
            <xsd:extension base="com.ibm.ws.security.openidconnect.client.param"> 
                <xsd:attribute name="id" type="factoryIdType" use="optional"> 
                    <xsd:annotation> 
                        <xsd:documentation>唯一配置标识。</xsd:documentation> 
                        <xsd:appinfo> 
                            <ext:label>标识</ext:label> 
                        </xsd:appinfo> 
                    </xsd:annotation> 
                </xsd:attribute> 
            </xsd:extension> 
        </xsd:complexContent> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.security.oidcs.scopeToClaimMap"> 
        <xsd:annotation> 
            <xsd:documentation>对作用域指定声明。</xsd:documentation> 
            <xsd:appinfo> 
                <ext:label>作用域至声明映射</ext:label> 
                <ext:extraProperties/> 
            </xsd:appinfo> 
        </xsd:annotation> 
        <xsd:sequence> 
            <xsd:element name="property" minOccurs="0" maxOccurs="unbounded" type="com.ibm.ws.security.oidcs.property-factory"> 
            </xsd:element> 
        </xsd:sequence> 
        <xsd:attribute name="profile" use="optional" default="name, family_name, given_name, middle_name, nickname, preferred_username, profile, picture, website, gender, birthdate, zoneinfo, locale, updated_at" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>指定与概要文件作用域相关联的声明的逗号分隔列表。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>概要文件作用域</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="email" use="optional" default="email, email_verified" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>指定与电子邮件作用域相关联的声明的逗号分隔列表。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>电子邮件作用域</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="address" use="optional" default="address" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>指定与地址作用域相关联的声明的逗号分隔列表。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>地址作用域</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="phone" use="optional" default="phone_number, phone_number_verified" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>指定与电话作用域相关联的声明的逗号分隔列表。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>电话作用域</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="internal.properties" type="internalPropertiesType" use="optional"> 
            <xsd:annotation> 
                <xsd:documentation>对作用域指定声明。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>作用域至声明映射</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:anyAttribute processContents="skip"> 
        </xsd:anyAttribute> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.security.oidcs.scopeToClaimMap-factory"> 
        <xsd:complexContent> 
            <xsd:extension base="com.ibm.ws.security.oidcs.scopeToClaimMap"> 
                <xsd:attribute name="id" type="factoryIdType" use="optional"> 
                    <xsd:annotation> 
                        <xsd:documentation>唯一配置标识。</xsd:documentation> 
                        <xsd:appinfo> 
                            <ext:label>标识</ext:label> 
                        </xsd:appinfo> 
                    </xsd:annotation> 
                </xsd:attribute> 
            </xsd:extension> 
        </xsd:complexContent> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.security.oidcs.claimToUserRegistryMap"> 
        <xsd:annotation> 
            <xsd:documentation>对声明指定用户注册表键。</xsd:documentation> 
            <xsd:appinfo> 
                <ext:label>声明至用户注册表映射</ext:label> 
                <ext:extraProperties/> 
            </xsd:appinfo> 
        </xsd:annotation> 
        <xsd:sequence> 
            <xsd:element name="property" minOccurs="0" maxOccurs="unbounded" type="com.ibm.ws.security.oidcs.property-factory"> 
            </xsd:element> 
        </xsd:sequence> 
        <xsd:attribute name="name" use="optional" default="displayName" type="xsd:string"> 
            <xsd:annotation> 
