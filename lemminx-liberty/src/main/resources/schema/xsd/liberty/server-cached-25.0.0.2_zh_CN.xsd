    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.security.audit.event"> 
        <xsd:annotation> 
            <xsd:documentation>审计事件</xsd:documentation> 
            <xsd:appinfo> 
                <ext:label>审计事件</ext:label> 
            </xsd:appinfo> 
        </xsd:annotation><!--Idiom for "empty element context" (whitespace accepted and ignored)--> 
        <xsd:sequence> 
            <xsd:sequence/> 
        </xsd:sequence> 
        <xsd:attribute name="eventName" use="optional"> 
            <xsd:annotation> 
                <xsd:documentation>审计事件的唯一名称。例如：SECURITY_AUTHN 或 SECURITY_AUTHZ。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>审计事件名称</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
            <xsd:simpleType> 
                <xsd:restriction base="xsd:string"> 
                    <xsd:enumeration value="SECURITY_AUDIT_MGMT"> 
                        <xsd:annotation> 
                            <xsd:documentation>启动和停止审计服务的审计记录。</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                    <xsd:enumeration value="SECURITY_MEMBER_MGMT"> 
                        <xsd:annotation> 
                            <xsd:documentation>所有用户和组管理事件（包括创建、读取、更新以及删除用户和组记录）的审计记录。</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                    <xsd:enumeration value="SECURITY_API_AUTHN"> 
                        <xsd:annotation> 
                            <xsd:documentation>任何完成了认证 API 的认证（不包括注销）的审计记录。</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                    <xsd:enumeration value="SECURITY_API_AUTHN_TERMINATE"> 
                        <xsd:annotation> 
                            <xsd:documentation>任何完成了认证 API 的认证注销的审计记录。</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                    <xsd:enumeration value="SECURITY_AUTHN"> 
                        <xsd:annotation> 
                            <xsd:documentation>任何安全认证事件（不包括注销）的审计记录。</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                    <xsd:enumeration value="SECURITY_AUTHN_TERMINATE"> 
                        <xsd:annotation> 
                            <xsd:documentation>任何认证注销事件的审计记录。</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                    <xsd:enumeration value="SECURITY_AUTHN_FAILOVER"> 
                        <xsd:annotation> 
                            <xsd:documentation>任何认证故障转移事件的审计记录。</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                    <xsd:enumeration value="SECURITY_AUTHN_DELEGATION"> 
                        <xsd:annotation> 
                            <xsd:documentation>任何授权、身份声明和 runAS 的审计记录。在给定的会话中切换用户身份时使用。</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                    <xsd:enumeration value="SECURITY_AUTHZ"> 
                        <xsd:annotation> 
                            <xsd:documentation>任何安全授权事件的审计记录。</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                    <xsd:enumeration value="JMX_MBEAN"> 
                        <xsd:annotation> 
                            <xsd:documentation>任何创建、查询或调用 MBean 操作的审计记录。</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                    <xsd:enumeration value="JMX_MBEAN_ATTRIBUTES"> 
                        <xsd:annotation> 
                            <xsd:documentation>对 MBean 属性进行的任何查询、创建或更新的审计记录。</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                    <xsd:enumeration value="JMX_MBEAN_REGISTER"> 
                        <xsd:annotation> 
                            <xsd:documentation>任何注册或注销 MBean 操作的审计记录。</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                    <xsd:enumeration value="JMX_NOTIFICATION"> 
                        <xsd:annotation> 
                            <xsd:documentation>对通知侦听器进行的任何添加或移除的审计记录。</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                    <xsd:enumeration value="SECURITY_JMS_AUTHN"> 
                        <xsd:annotation> 
                            <xsd:documentation>任何 JMS 认证事件的审计记录。</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                    <xsd:enumeration value="SECURITY_JMS_AUTHZ"> 
                        <xsd:annotation> 
                            <xsd:documentation>任何 JMS 授权事件的审计记录。</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                    <xsd:enumeration value="SECURITY_SAF_AUTHZ_DETAILS"> 
                        <xsd:annotation> 
                            <xsd:documentation>SAF 授权服务配置为报告有关授权失败的其他信息时及抛出 SAFAuthorizationException 时针对 SAF 授权事件的审计记录。</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                    <xsd:enumeration value="APPLICATION_TOKEN_MANAGEMENT"> 
                        <xsd:annotation> 
                            <xsd:documentation>针对 OAuth 应用程序令牌及密码管理的审计记录</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                    <xsd:enumeration value="SECURITY_SAF_AUTHZ"> 
                        <xsd:annotation> 
                            <xsd:documentation>针对 SAF 授权事件的审计记录。</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                </xsd:restriction> 
            </xsd:simpleType> 
        </xsd:attribute> 
        <xsd:attribute name="outcome" use="optional"> 
            <xsd:annotation> 
                <xsd:documentation>定义要收集和报告的审计事件的结果。例如：SUCCESS、FAILURE 或 DENIED。如果未指定结果，那么会将特定审计事件的所有结果发出到 audit.log 中。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>审计结果</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
            <xsd:simpleType> 
                <xsd:restriction base="xsd:string"> 
                    <xsd:enumeration value="SUCCESS"> 
                        <xsd:annotation> 
                            <xsd:documentation>success</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                    <xsd:enumeration value="FAILURE"> 
                        <xsd:annotation> 
                            <xsd:documentation>failure</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                    <xsd:enumeration value="ERROR"> 
                        <xsd:annotation> 
                            <xsd:documentation>error</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                    <xsd:enumeration value="REDIRECT"> 
                        <xsd:annotation> 
                            <xsd:documentation>redirect</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                    <xsd:enumeration value="INFO"> 
                        <xsd:annotation> 
                            <xsd:documentation>info</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                    <xsd:enumeration value="WARNING"> 
                        <xsd:annotation> 
                            <xsd:documentation>warning</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                    <xsd:enumeration value="DENIED"> 
                        <xsd:annotation> 
                            <xsd:documentation>denied</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                    <xsd:enumeration value="CHALLENGE"> 
                        <xsd:annotation> 
                            <xsd:documentation>challenge</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                </xsd:restriction> 
            </xsd:simpleType> 
        </xsd:attribute> 
        <xsd:anyAttribute processContents="skip"> 
        </xsd:anyAttribute> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.security.audit.event-factory"> 
        <xsd:complexContent> 
            <xsd:extension base="com.ibm.ws.security.audit.event"> 
                <xsd:attribute name="id" type="factoryIdType" use="optional"> 
                    <xsd:annotation> 
                        <xsd:documentation>唯一配置标识。</xsd:documentation> 
                        <xsd:appinfo> 
                            <ext:label>标识</ext:label> 
                        </xsd:appinfo> 
                    </xsd:annotation> 
                </xsd:attribute> 
            </xsd:extension> 
        </xsd:complexContent> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.jbatch.joblog"> 
        <xsd:annotation> 
            <xsd:documentation>配置批处理作业日志记录。</xsd:documentation> 
            <xsd:appinfo> 
                <ext:label>批处理作业日志记录</ext:label> 
            </xsd:appinfo> 
        </xsd:annotation><!--Idiom for "empty element context" (whitespace accepted and ignored)--> 
        <xsd:sequence> 
            <xsd:sequence/> 
        </xsd:sequence> 
        <xsd:attribute name="enabled" use="optional" default="true" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>启用或禁用批处理作业日志记录。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>启用批处理作业日志记录</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="maxRecords" use="optional" default="1000" type="intType"> 
            <xsd:annotation> 
                <xsd:documentation>每个作业日志文件在滚动至下一个文件之前的最大日志记录数。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>每个文件的最大日志记录数</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="maxTime" use="optional" default="0" type="intType"> 
            <xsd:annotation> 
                <xsd:documentation>发布作业日志事件之间允许耗用的最大时间（秒）。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>发布日志的最大时间间隔（秒）</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:anyAttribute processContents="skip"> 
        </xsd:anyAttribute> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.jca.connectionManager"> 
        <xsd:annotation> 
            <xsd:documentation>连接管理器配置</xsd:documentation> 
            <xsd:appinfo> 
                <ext:label>连接管理器</ext:label> 
                <ext:groupDecl id="Advanced" label="高级属性">用于更高级用法的附加属性。</ext:groupDecl> 
            </xsd:appinfo> 
        </xsd:annotation><!--Idiom for "empty element context" (whitespace accepted and ignored)--> 
        <xsd:sequence> 
            <xsd:sequence/> 
        </xsd:sequence> 
        <xsd:attribute name="agedTimeout" use="optional" default="-1" type="secondDuration"> 
            <xsd:annotation> 
                <xsd:documentation>池维护可以废弃某个连接之前的时间量。值为 -1 时会禁用此超时。值 0 指示废弃每个连接，这会禁用连接池。. 指定后跟时间单位的正整数，时间单位可以是小时 (h)、分钟 (m) 或秒 (s)。例如，以 30s 的形式指定 30 秒。可将多个值包括在单个条目中。例如，1m30s 相当于 90 秒。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>时效超时</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="connectionTimeout" use="optional" default="30s" type="secondDuration"> 
            <xsd:annotation> 
                <xsd:documentation>连接请求超时之前的时间量。如果值为 -1，那么将禁用此超时，这意味着将无限期等待。如果值为 0，那么表示立即连接，这意味着不等待。. 指定后跟时间单位的正整数，时间单位可以是小时 (h)、分钟 (m) 或秒 (s)。例如，以 30s 的形式指定 30 秒。可将多个值包括在单个条目中。例如，1m30s 相当于 90 秒。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>连接超时</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="maxIdleTime" use="optional" default="30m" type="secondDuration"> 
            <xsd:annotation> 
                <xsd:documentation>池维护期间可废弃某个未使用或空闲的连接之前的时间量（如果这样做不会使池大小减小到小于最小大小）。值为 -1 时会禁用此超时。. 指定后跟时间单位的正整数，时间单位可以是小时 (h)、分钟 (m) 或秒 (s)。例如，以 30s 的形式指定 30 秒。可将多个值包括在单个条目中。例如，1m30s 相当于 90 秒。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>最长空闲时间</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="maxPoolSize" use="optional" default="50" type="intType"> 
            <xsd:annotation> 
                <xsd:documentation>池的物理连接的最大数目。值为 0 意味着不受限制。优化设置取决于应用程序特征。对于其中每个线程包含与数据库的连接的应用程序，您可从“coreThreads”属性的 1:1 映射开始。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>最大连接数</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="purgePolicy" use="optional" default="EntirePool"> 
            <xsd:annotation> 
                <xsd:documentation>指定在池中检测到旧连接时要销毁哪些连接。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>清除策略</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
            <xsd:simpleType> 
                <xsd:restriction base="xsd:string"> 
                    <xsd:enumeration value="EntirePool"> 
                        <xsd:annotation> 
                            <xsd:documentation>当检测到失效连接时，会将池中的所有连接都标记为失效，当不再使用这些连接时，会将其关闭。</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                    <xsd:enumeration value="FailingConnectionOnly"> 
                        <xsd:annotation> 
                            <xsd:documentation>当检测到失效连接时，会仅关闭发现存在错误的连接。</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                    <xsd:enumeration value="ValidateAllConnections"> 
                        <xsd:annotation> 
                            <xsd:documentation>当检测到失效连接时，会测试连接并关闭发现存在错误的连接。</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                </xsd:restriction> 
            </xsd:simpleType> 
        </xsd:attribute> 
        <xsd:attribute name="reapTime" use="optional" default="3m" type="secondDuration"> 
            <xsd:annotation> 
                <xsd:documentation>池维护线程各次运行之间的时间量。值为 -1 会禁用池维护。. 指定后跟时间单位的正整数，时间单位可以是小时 (h)、分钟 (m) 或秒 (s)。例如，以 30s 的形式指定 30 秒。可将多个值包括在单个条目中。例如，1m30s 相当于 90 秒。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>收集时间</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="autoCloseConnections" use="optional" default="true" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>在 Servlet 请求、企业 Bean 实例、受管执行程序任务、上下文任务或者受管完成阶段结束之后，在使连接保持打开状态的应用程序之后尝试清除。当在这些应用程序工件之一中获得不可共享的连接，并在连接结束时保持打开状态时，容器将尝试关闭连接句柄。容器还可能关闭不支持 DissociatableManagedConnection 的可共享连接。应用程序始终应遵循按规范定义的编程模型，并在适当的时间关闭连接，而不是依赖于容器，即使启用了此选项也是如此。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>自动关闭连接</ext:label> 
                    <ext:group id="Advanced"/> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="enableContainerAuthForDirectLookups" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>如果设置为 true，那么连接将使用容器认证。如果设置为 false，那么连接将使用应用程序认证。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>在直接查询时启用容器认证</ext:label> 
                    <ext:group id="Advanced"/> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="enableSharingForDirectLookups" use="optional" default="true" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>如果设置为 true，那么将共享连接。如果设置为 false，那么不会共享连接。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>为直接查询启用连接共享</ext:label> 
                    <ext:group id="Advanced"/> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="minPoolSize" use="optional" type="intType"> 
            <xsd:annotation> 
                <xsd:documentation>池中要维持的最小物理连接数。池未进行预填充。时效超时可以覆盖此最小值。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>最小连接数</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="maxConnectionsPerThread" use="optional" type="intType"> 
            <xsd:annotation> 
                <xsd:documentation>限制每个线程上打开的连接数。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>每个线程的最大打开连接数</ext:label> 
                    <ext:group id="Advanced"/> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="numConnectionsPerThreadLocal" use="optional" type="intType"> 
            <xsd:annotation> 
                <xsd:documentation>为每个线程高速缓存指定数目的连接。 通过为每个线程保留指定数量的数据库连接，此设置可以提高大型多核 (8 +) 机器的性能。 为了获得最佳性能，如果有 n 个应用程序线程，请将最大池连接数设置为 numConnectionsPerThreadLocal 属性值的至少 n 倍。 对所有连接请求使用相同的凭证。 例如，如果使用 20 个应用程序线程，请将最大池连接数设置为 20 个或更多。 如果将 numConnectionPerThreadLocal 属性的值设置为 2，并且有 20 个应用程序线程，那么将最大池连接数设置为 40 或更多。 如果设置 numConnectionsPerThreadLocal 不会提高性能，因为使用 numConnectionsPerThreadLocal 不会使应用程序连接使用模式受益，请从配置中除去该属性或将值设置为 0。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>每个线程的高速缓存连接数</ext:label> 
                    <ext:group id="Advanced"/> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:anyAttribute processContents="skip"> 
        </xsd:anyAttribute> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.jca.connectionManager-factory"> 
        <xsd:complexContent> 
            <xsd:extension base="com.ibm.ws.jca.connectionManager"> 
                <xsd:attribute name="id" type="factoryIdType" use="optional"> 
                    <xsd:annotation> 
                        <xsd:documentation>唯一配置标识。</xsd:documentation> 
                        <xsd:appinfo> 
                            <ext:label>标识</ext:label> 
                        </xsd:appinfo> 
                    </xsd:annotation> 
                </xsd:attribute> 
            </xsd:extension> 
        </xsd:complexContent> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.cache.cacheGroup.member"> 
        <xsd:annotation> 
            <xsd:documentation>WebSphere Application Server 控制的外部高速缓存组的成员。</xsd:documentation> 
            <xsd:appinfo> 
                <ext:label>外部高速缓存组成员</ext:label> 
            </xsd:appinfo> 
        </xsd:annotation> 
        <xsd:sequence> 
            <xsd:element name="adapterBeanName" minOccurs="1" maxOccurs="1" type="xsd:string"> 
                <xsd:annotation> 
                    <xsd:documentation>指定 WebSphere Application Server 与此外部高速缓存之间的适配器的类名（位于 WebSphere Application Server 类路径上）。</xsd:documentation> 
                    <xsd:appinfo> 
                        <ext:label>适配器 bean 名称</ext:label> 
                    </xsd:appinfo> 
                </xsd:annotation> 
            </xsd:element> 
        </xsd:sequence> 
        <xsd:attribute name="host" use="optional" type="xsd:string"> 
