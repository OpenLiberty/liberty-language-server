    <xsd:complexType name="com.ibm.ws.app.manager.springappcfg"> 
        <xsd:annotation> 
            <xsd:documentation>定义 Spring Boot 应用程序的属性。</xsd:documentation> 
            <xsd:appinfo> 
                <ext:label>Spring Boot 应用程序</ext:label> 
            </xsd:appinfo> 
        </xsd:annotation> 
        <xsd:choice minOccurs="0" maxOccurs="unbounded"> 
            <xsd:element name="startAfter" type="com.ibm.ws.app.manager-factory"> 
                <xsd:annotation> 
                    <xsd:documentation>指定在此应用程序可以开始启动之前，需要先启动的应用程序。</xsd:documentation> 
                    <xsd:appinfo> 
                        <ext:label>在此之后启动</ext:label> 
                    </xsd:appinfo> 
                </xsd:annotation> 
            </xsd:element> 
            <xsd:element name="classloader" type="com.ibm.ws.classloading.classloader"> 
                <xsd:annotation> 
                    <xsd:documentation>定义应用程序类装入器的设置。</xsd:documentation> 
                    <xsd:appinfo> 
                        <ext:label>类装入器</ext:label> 
                    </xsd:appinfo> 
                </xsd:annotation> 
            </xsd:element> 
            <xsd:element name="applicationArgument" type="xsd:string"> 
                <xsd:annotation> 
                    <xsd:documentation>定义传递至 Spring Boot 应用程序的 start 类的 main 方法的应用程序自变量。</xsd:documentation> 
                    <xsd:appinfo> 
                        <ext:label>应用程序自变量</ext:label> 
                    </xsd:appinfo> 
                </xsd:annotation> 
            </xsd:element> 
        </xsd:choice> 
        <xsd:attribute name="location" use="required" type="location"> 
