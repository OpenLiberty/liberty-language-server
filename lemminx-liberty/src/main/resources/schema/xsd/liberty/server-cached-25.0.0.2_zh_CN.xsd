                    <xsd:appinfo> 
                        <ext:label>认证过滤器参考</ext:label> 
                    </xsd:appinfo> 
                </xsd:annotation> 
            </xsd:element> 
            <xsd:element name="jwt" type="com.ibm.ws.security.social.login.jwt"> 
            </xsd:element> 
        </xsd:choice> 
        <xsd:attribute name="clientId" use="required" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>应用程序或客户机标识。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>应用程序或客户机的标识</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="clientSecret" use="required" type="password"> 
            <xsd:annotation> 
                <xsd:documentation>应用程序或客户机的私钥。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>应用程序或客户机的密钥</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="loginDialogEndpoint" use="optional" default="https://www.facebook.com/v2.8/dialog/oauth" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>Facebook 的登录授权端点 URL。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>登录对话框端点 URL</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="tokenEndpoint" use="optional" default="https://graph.facebook.com/v2.8/oauth/access_token" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>指定令牌端点 URL。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>令牌端点 URL</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="userApi" use="optional" default="https://graph.facebook.com/v2.8/me?fields=id,name,email" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>用于检索用户信息的 URL。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>用户信息 URL</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="permissions" use="optional" default="public_profile email" type="xsd:string"> 
