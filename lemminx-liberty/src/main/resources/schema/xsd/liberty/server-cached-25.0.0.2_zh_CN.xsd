                    <ext:label>OAuth 许可权</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="userNameAttribute" use="optional" default="email" type="xsd:string"> 
