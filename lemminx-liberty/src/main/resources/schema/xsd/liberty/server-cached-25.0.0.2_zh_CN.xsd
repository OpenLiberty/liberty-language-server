                <xsd:appinfo> 
                    <ext:label>授权端点 URL</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="tokenEndpoint" use="optional" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>指定令牌端点 URL。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>令牌端点 URL</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="userInfoEndpoint" use="optional" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>指定用户信息端点 URL。</xsd:documentation> 
