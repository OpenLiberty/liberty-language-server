                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="count" use="optional" default="1" type="intType"> 
            <xsd:annotation> 
                <xsd:documentation>指定包括在 openID 提供程序的响应中的用户信息量。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>计数</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="required" use="optional" default="true" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>指定是否需要用户信息。</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>必需</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:anyAttribute processContents="skip"> 
        </xsd:anyAttribute> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.security.openid20.openidClientConfig.userInfo-factory"> 
        <xsd:complexContent> 
            <xsd:extension base="com.ibm.ws.security.openid20.openidClientConfig.userInfo"> 
                <xsd:attribute name="id" type="factoryIdType" use="optional"> 
                    <xsd:annotation> 
                        <xsd:documentation>唯一配置标识。</xsd:documentation> 
                        <xsd:appinfo> 
